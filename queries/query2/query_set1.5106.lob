SELECT COB_DATE , a.EM_GROUP , a.G10_GROUP ,CASE WHEN a.CCC_BUSINESS_AREA IN ('FXEM MACRO TRADING') and a.CCC_PL_REPORTING_REGION in ('AMERICAS') THEN ('US_FXEM_MACRO') ELSE 'OTHER' END AS FLAG ,SUM(COALESCE((USD_FX) :: numeric(15,5), 0))/1000000 AS FX FROM CDWUSER.U_DM_FIRMWIDE a WHERE COB_DATE in ( '2018-02-28', '2018-01-31', '2017-12-29', '2017-11-30', '2017-10-31', '2017-09-29' ) AND usd_fx <> 0 AND BU_RISK_SYSTEM not like 'PIPELINE%' AND VAR_EXCL_FL <> 'Y' AND BU_RISK_RUN_CUSTOM1 <> 'STORAGE' AND CCC_DIVISION NOT IN ('FIC DVA') AND ccc_business_area NOT LIKE 'CPM TRADING%' AND ccc_business_area <> 'CREDIT' AND a.CCC_TAPS_COMPANY = '1633' and a.CCC_BANKING_TRADING = 'TRADING' AND CURRENCY_OF_MEASURE NOT IN ('UBD', 'USD') GROUP BY COB_DATE , a.EM_GROUP , a.G10_GROUP ,CASE WHEN a.CCC_BUSINESS_AREA IN ('FXEM MACRO TRADING') and a.CCC_PL_REPORTING_REGION in ('AMERICAS') THEN ('US_FXEM_MACRO') ELSE 'OTHER' END