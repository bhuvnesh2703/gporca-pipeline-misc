select * from (SELECT     A.CCC_DIVISION, a.PRODUCT_DESCRIPTION,     term_bucket,     CASE WHEN A.FID1_INDUSTRY_NAME_LEVEL1 = 'SOVEREIGN' THEN 'SOV'     ELSE 'NON SOV' END AS SOV_FLAG,     SUM (COALESCE (a.USD_EXPOSURE,0)) AS NET_EXP FROM CDWUSER.U_exp_msr A WHERE COB_DATE='2018-02-28' AND    A.CCC_DIVISION IN ('INSTITUTIONAL EQUITY DIVISION') AND     A.ISSUER_COUNTRY_CODE = 'BRA' AND     CCC_BUSINESS_AREA NOT LIKE '%CPM%' AND     A.CCC_BANKING_TRADING = 'TRADING' AND     A.FID1_INDUSTRY_NAME_LEVEL1 <> 'UNDEFINED'     and coalesce(a.usd_exposure,0)<>0 GROUP BY     CASE WHEN A.FID1_INDUSTRY_NAME_LEVEL1 = 'SOVEREIGN' THEN 'SOV'     ELSE 'NON SOV' END,     A.CCC_DIVISION,     term_bucket, a.PRODUCT_DESCRIPTION ORDER BY A.CCC_DIVISION) X where net_exp<>0