SELECT a.COB_DATE , CASE WHEN a.CCC_TAPS_COMPANY IN ( '0201','0103','0205','0206','0530','5924') THEN 'MSCO' WHEN a.CCC_TAPS_COMPANY IN ('0111') THEN 'MSCS' ELSE 'OTHER LE' END as LE , CASE WHEN a.SPG_DESC IN ('GOVERNMENT', 'RATE FUTURES', 'SWAP', 'SWAPS') THEN 'Govt & Swap' WHEN a.SPG_DESC IN ('CORPORATE INDEX') THEN 'CDX' WHEN a.SPG_DESC = 'CORPORATE DEFAULT SWAP' THEN 'CDS' WHEN a.SPG_DESC = 'OTHER' THEN 'OTHER' WHEN ( a.SPG_DESC LIKE '%AGENCY%' OR a.SPG_DESC LIKE '%POOL%' OR a.SPG_DESC LIKE '%TBA%' OR a.SPG_DESC LIKE '%RMBS IOS%' OR a.SPG_DESC LIKE '%RMBS MBX%' OR a.SPG_DESC LIKE '%RMBS MBS%' OR a.SPG_DESC = 'AGENCY CMBS SECURITY' OR a.SPG_DESC = 'OTHER RMBS' OR ( a.SPG_DESC = 'CMBS SECURITY' AND A.SPG_PRODUCT_TYPE = 'AGENCY CMBS' ) ) THEN 'Agency SPG' ELSE 'Non-Agency SPG' END AS ASSET , SUM(a.USD_EXPOSURE) AS NE FROM CDWUSER.U_EXP_MSR a Where a.cob_date IN ('03/31/2017','04/28/2017','05/31/2017','06/30/2017','07/31/2017','08/31/2017','09/29/2017','10/31/2017','11/30/2017','12/29/2017','01/31/2018','02/28/2018') AND a.CCC_BUSINESS_AREA = 'SECURITIZED PRODUCTS GRP' AND A.CCC_TAPS_COMPANY IN ( '0201','0103','0205','0206','0530','5924') GROUP BY a.COB_DATE , CASE WHEN a.CCC_TAPS_COMPANY IN ( '0201','0103','0205','0206','0530','5924') THEN 'MSCO' WHEN a.CCC_TAPS_COMPANY IN ('0111') THEN 'MSCS' ELSE 'OTHER LE' END , CASE WHEN a.SPG_DESC IN ('GOVERNMENT', 'RATE FUTURES', 'SWAP', 'SWAPS') THEN 'Govt & Swap' WHEN a.SPG_DESC IN ('CORPORATE INDEX') THEN 'CDX' WHEN a.SPG_DESC = 'CORPORATE DEFAULT SWAP' THEN 'CDS' WHEN a.SPG_DESC = 'OTHER' THEN 'OTHER' WHEN ( a.SPG_DESC LIKE '%AGENCY%' OR a.SPG_DESC LIKE '%POOL%' OR a.SPG_DESC LIKE '%TBA%' OR a.SPG_DESC LIKE '%RMBS IOS%' OR a.SPG_DESC LIKE '%RMBS MBX%' OR a.SPG_DESC LIKE '%RMBS MBS%' OR a.SPG_DESC = 'AGENCY CMBS SECURITY' OR a.SPG_DESC = 'OTHER RMBS' OR ( a.SPG_DESC = 'CMBS SECURITY' AND A.SPG_PRODUCT_TYPE = 'AGENCY CMBS' ) ) THEN 'Agency SPG' ELSE 'Non-Agency SPG' END