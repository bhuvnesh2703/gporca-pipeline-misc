SELECT cr.cob_date, cr.ccc_business_area, cr.CCC_PRODUCT_LINE, CR.ISSUER_COUNTRY_CODE, CR.FID1_INDUSTRY_NAME_LEVEL1, CR.PRODUCT_TYPE_CODE, CR.LE_GROUP, CR.CCC_GL_COMPANY_CODE, CR.FID1_SENIORITY, CR.CCC_PL_REPORTING_REGION, CR.POSITION_ULT_ISSUER_PARTY_DARWIN_NAME, cASE WHEN (CR.FID1_SENIORITY IN ('SUBT1', 'SUBUT2', 'AT1') AND cr.PRODUCT_TYPE_CODE IN ('BOND', 'PREF') AND CR.CCC_BUSINESS_AREA = 'CREDIT-CORPORATES') then 'junior sub' when cr.CCC_PRODUCT_LINE = 'DISTRESSED TRADING' THEN 'distressed' WHEN (CR.ISSUER_COUNTRY_CODE = 'GRC' AND CR.FID1_INDUSTRY_NAME_LEVEL1 IN ('SOVEREIGN', 'CORPORATES: FINANCIALS')) THEN 'defaulted' when (cr.FID1_INDUSTRY_NAME_LEVEL1 = 'SOVEREIGN' and cr.PRODUCT_TYPE_CODE not in ('CRDINDEX', 'DEFSWAP', 'CRDBSKT', 'ZCS')) then 'rates shock' ELSE 'pv10%' END AS RISK_MARK, cr.VERTICAL_SYSTEM, SUM (cr.USD_PV10_BENCH) AS BPV10, SUM (cr.usd_exposure) AS NetExposure, SUM (cr.USD_PV01SPRD) AS SPV01, SUM (CR.USD_DEFAULT_PNL) AS DEF_PNL FROM CDWUSER.u_cr_msr cr WHERE cr.cob_date in ('2018-02-28','2018-02-21') AND cr.ccc_division = 'FIXED INCOME DIVISION' AND CR.ISSUER_COUNTRY_CODE IN ('PRT', 'GRC', 'IRL', 'ITA', 'ESP', 'AUT', 'BEL', 'CYP', 'EST', 'FIN', 'FRA', 'DEU', 'LAT', 'LUX', 'MLT', 'NLD', 'SVK', 'SVN', 'GBR') AND CR.CCC_BUSINESS_AREA NOT IN ('LENDING', 'CPM TRADING (MPE)', 'MS CVA MNE - FID') AND cr.CCC_PRODUCT_LINE <> 'PRIMARY - LOANS' GROUP BY cr.cob_date, cr.ccc_business_area, cr.CCC_PRODUCT_LINE, CR.ISSUER_COUNTRY_CODE, CR.FID1_INDUSTRY_NAME_LEVEL1, CR.PRODUCT_TYPE_CODE, CR.LE_GROUP, CR.CCC_GL_COMPANY_CODE, CR.FID1_SENIORITY, CR.CCC_PL_REPORTING_REGION, CR.POSITION_ULT_ISSUER_PARTY_DARWIN_NAME, cr.VERTICAL_SYSTEM