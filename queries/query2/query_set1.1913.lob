SELECT COB_DATE,CCC_PL_REPORTING_REGION,PROD_POS_NAME_DESCRIPTION, SUM(cast(USD_FX_DELTA as numeric (15,5))) as USD_DELTA, SUM(cast(USD_FX_GAMMA as numeric(15,5))) as USD_GAMMA FROM cdwuser.U_FX_MSR WHERE COB_DATE IN ('2018-02-28','2018-01-31') AND ((CCC_DIVISION='COMMODITIES' and CCC_BUSINESS_AREA not in ('INVESTMENTS AND STR TRANS','INVESTOR BUSINESS', 'CREDIT','MS CVA MNE - COMMOD', 'COMMODS FINANCING') and CCC_PRODUCT_LINE in ('PRECIOUS METALS', 'INV STR TRANS - METALS')) /*OLD LOGIC*/ OR (CCC_DIVISION = 'FIXED INCOME DIVISION' AND CCC_BUSINESS_AREA = 'COMMODITIES' and CCC_PRODUCT_LINE IN ('PRECIOUS METALS'))) /* NEW LOGIC*/ and PRODUCT_TYPE_CODE in ('CURRENCY','INV STR TRANS - METALS') group by COB_DATE,CCC_PL_REPORTING_REGION,PROD_POS_NAME_DESCRIPTION