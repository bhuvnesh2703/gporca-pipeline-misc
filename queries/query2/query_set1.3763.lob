SELECT a.COB_DATE, a.CCC_PL_REPORTING_REGION, a.GROUPED_RATING, GROUPED_VINTAGE, GROUPED_TRADING_LENDING, SUM (a.USD_EXPOSURE) AS NET_EXPOSURE FROM CDWUSER.U_DM_SPG a WHERE a.COB_DATE >= '02/01/2017' AND a.COB_DATE <= '02/28/2018' AND a.CCC_BUSINESS_AREA = 'SECURITIZED PRODUCTS GRP' AND A.CCC_PRODUCT_LINE NOT IN ('CRE LENDING', 'CREL BANK HFI', 'CRE LENDING SEC/HFS') AND A.CCC_TAPS_COMPANY IN ( '0201','0103','0205','0206','0530','5924') GROUP BY a.COB_DATE, a.CCC_PL_REPORTING_REGION, a.GROUPED_RATING, GROUPED_VINTAGE, GROUPED_TRADING_LENDING