SELECT SUM(USD_BREAKEVEN_KAPPA/10) AS usd_breakeven_kappa, cob_date, CASE WHEN CURRENCY_CODE IN ('EUR', 'GBP','USD') THEN CURRENCY_CODE ELSE 'Others' END AS CURRENCY_OF_MEASURE1, a.CCC_STRATEGY FROM CDWUSER.U_EXP_TRENDS a WHERE a.COB_DATE >= '2017-11-30' AND CCC_BUSINESS_AREA IN ('STRUCTURED RATES', 'LIQUID FLOW RATES') AND CCC_PL_REPORTING_REGION = 'EMEA' AND VAR_EXCL_FL = 'N' AND VERTICAL_SYSTEM NOT IN ('PIPELINE_NY') AND ccc_product_line NOT LIKE 'MS CVA MPE%' AND ccc_business_area NOT LIKE 'CPM TRADING%' AND (ccc_product_line NOT LIKE 'NON IG PRIMARY%' OR ccc_strategy NOT LIKE 'NON IG PRIMARY%') AND (ccc_product_line NOT IN ('MS CVA MPE DERIVS CPM', 'CORE MPE CVA', 'MONOLINE MPE CVA', 'MS CVA MNE DERIVS IRCC') OR ccc_strategy NOT IN ('MS CVA MPE DERIVS CPM', 'CORE MPE CVA', 'MONOLINE MPE CVA', 'MS CVA MNE DERIVS IRCC')) AND CCC_BUSINESS_AREA NOT IN ('LENDING') AND CCC_BUSINESS_AREA NOT LIKE '%CVA%' GROUP BY cob_date, CASE WHEN CURRENCY_CODE IN ('EUR', 'GBP','USD') THEN CURRENCY_CODE ELSE 'Others' END, a.CCC_STRATEGY