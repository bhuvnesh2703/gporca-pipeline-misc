SELECT cob_date, CURRENCY_OF_MEASURE, case when TERM_NEW <=10 then '0-10y' else '10y+' end as term, sum(coalesce(a.USD_IR_UNIFIED_PV01,0)) as PV01, a.CCC_STRATEGY FROM CDWUSER.U_DM_IR a WHERE a.COB_DATE in ('2018-02-28','2018-02-21') AND CURRENCY_OF_MEASURE IN ('EUR', 'GBP','USD') AND CCC_BUSINESS_AREA IN ('STRUCTURED RATES','LIQUID FLOW RATES') AND CCC_PL_REPORTING_REGION = 'EMEA' AND BOOK NOT IN ('TOSPL') AND USD_IR_UNIFIED_PV01 is not null AND USD_IR_UNIFIED_PV01 <> 0 GROUP BY cob_date , CURRENCY_OF_MEASURE, case when TERM_NEW <=10 then '0-10y' else '10y+' end, a.CCC_STRATEGY