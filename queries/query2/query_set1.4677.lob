SELECT A.COB_DATE, CASE WHEN A.CCC_PRODUCT_LINE IN ('DSP INDEX PRODUCTS TRADING','DSP INDEX PRODUCTS') THEN 'CREDIT-CORPORATES' ELSE A.CCC_BUSINESS_AREA END AS CCC_BUSINESS_AREA, SUM(A.USD_BPV10_FID) /1000 AS FID_BPV10 FROM CDWUSER.U_DM_FIRMWIDE A WHERE COB_DATE >= '07/01/2016' AND COB_DATE <= '02/28/2018' AND CCC_BUSINESS_AREA <> 'LENDING' AND CCC_BUSINESS_AREA NOT LIKE 'CPM%' AND CCC_BUSINESS_AREA <> 'DERIVS FUNDING' AND A.USD_BPV10_FID IS NOT NULL AND NOT ( A.CCC_PRODUCT_LINE IN ('DISTRESSED TRADING') AND A.CASH_TYPE <> 'Index' ) AND CCC_BUSINESS_AREA NOT IN ('COMMODITIES') /*AND NOT ( A.CCC_BUSINESS_AREA = 'SECURITIZED PRODUCTS GRP' AND SPG_DESC IN ('CMBS IO REREMIC','CMBS IO','CMBS SECURITY','RMBS PRIME RESIDUAL','CMBS INDEX','CORPORATE INDEX') AND CCC_PRODUCT_LINE IN ('CRE LENDING', 'CREL BANK HFI', 'CRE LENDING SEC/HFS') )*/ GROUP BY A.COB_DATE, CASE WHEN A.CCC_PRODUCT_LINE IN ('DSP INDEX PRODUCTS TRADING','DSP INDEX PRODUCTS') THEN 'CREDIT-CORPORATES' ELSE A.CCC_BUSINESS_AREA END