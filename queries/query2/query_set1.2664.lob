SELECT a.COB_DATE, SUM (a.USD_EXPOSURE) AS NET_EXPOSURE FROM cdwuser.U_DM_SPG a WHERE A.cob_date in ( '02/28/2018', '02/27/2018') AND a.CCC_BUSINESS_AREA = 'SECURITIZED PRODUCTS GRP' AND A.CCC_PRODUCT_LINE NOT IN ('CRE LENDING', 'CREL BANK HFI', 'CRE LENDING SEC/HFS') AND A.CCC_TAPS_COMPANY IN ( '0201','0103','0205','0206','0530','5924') AND GROUPED_TRADING_LENDING = 'CMBS' GROUP BY a.COB_DATE