SELECT DISTINCT CUR.COB_DATE, CUR.RANK_TYPE, CUR.RANK_NAME, CUR.RANK, CUR.RANK || '_' || CUR.RANK_NAME AS CONCENTRATION, CUR.PRODUCT_DESCRIPTION, CUR.ISSUE_ID, CUR.TICK, CUR.DELTA AS CUR_DELTA, CUR.DELTA - PREV.DELTA AS DELTA_DOD  FROM CDWUSER.U_DM_CONC CUR     LEFT OUTER JOIN CDWUSER.U_DM_CONC PREV                      ON CUR.RANK_TYPE = PREV.RANK_TYPE                     AND CUR.RANK_NAME = PREV.RANK_NAME                     AND CUR.PRODUCT_DESCRIPTION = PREV.PRODUCT_DESCRIPTION                     AND CUR.TICK = PREV.TICK AND (PREV.COB_DATE = '2018-02-21')       WHERE CUR.RANK_NAME IN ('TopUKNRAMVinclPB','TopEuNRAMVinclPB') AND (CUR.COB_DATE = '2018-02-28')  AND CUR.RANK <= 10