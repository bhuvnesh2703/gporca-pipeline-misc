SELECT RANK() OVER (ORDER BY (abs(SUM(a.USD_EQ_PARTIAL_KAPPA))) DESC) || '_TopUKGroupVegaStockExclCVA' AS Concentration, CASE WHEN a.PARTIAL_DECOMP_PRODUCT_DESCRIPTION IS NULL THEN 'UNDEFINED' ELSE a.PARTIAL_DECOMP_PRODUCT_DESCRIPTION END PARTIAL_DECOMP_PRODUCT_DESCRIPTION, SUM(a.USD_EQ_PARTIAL_KAPPA) AS USD_EQ_PARTIAL_KAPPA FROM cdwuser.U_EXP_MSR_PARTIAL a WHERE a.cob_date IN ('2018-02-28') AND a.CCC_DIVISION = 'INSTITUTIONAL EQUITY DIVISION' AND a.CCC_BANKING_TRADING = 'TRADING' AND a.LE_GROUP = 'UK' AND a.CCC_BUSINESS_AREA NOT IN ('LENDING') AND (a.CASH_ISSUE_TYPE IN ('STOCK', 'ADR') OR a.Partial_Decomp_Sector IN ('STOCK', 'ADR')) AND ABS(a.USD_EQ_PARTIAL_KAPPA) > 0 AND ASSET_CLASS = 'EQ' AND CCC_DIVISION NOT IN ('FID DVA','FIC DVA') AND CCC_STRATEGY NOT IN ('MS DVA STR NOTES IED') GROUP BY a.PARTIAL_DECOMP_PRODUCT_DESCRIPTION ORDER BY ABS(SUM(a.USD_EQ_PARTIAL_KAPPA)) DESC FETCH FIRST 10 ROWS ONLY