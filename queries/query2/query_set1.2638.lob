SELECT CASE WHEN a.EXECUTIVE_MODEL IN ('AVERAGING-CONVERTIBLE', 'DEFAULTED-CONVERTIBLE', 'RESETTABLE-CONVERTIBLE') THEN 'MANDATORY' ELSE 'NON-MANDATORY' END AS MANDATORY_FLAG, a.COB_DATE, CASE WHEN a.CCC_PL_REPORTING_REGION='AMERICAS' THEN 'US' WHEN a.CCC_PL_REPORTING_REGION='EMEA' THEN 'EUROPE' ELSE 'ASIA' END AS CCC_PL_REPORTING_REGION, SUM (a.USD_EXPOSURE) / 1000 AS Net_Exposure FROM CDWUSER.U_EXP_MSR_PARTIAL a WHERE a.COB_DATE IN ('2018-02-28') AND a.CCC_DIVISION = 'INSTITUTIONAL EQUITY DIVISION' AND a.CCC_BANKING_TRADING = 'TRADING' AND (a.PRODUCT_TYPE_CODE in ('CONVRT') OR a.PARTIAL_DECOMP_PRODUCT_TYPE_CODE in ('CONVRT')) AND a.USD_EXPOSURE <> '0' AND a.TICKER NOT IN ('7048257', '7048274') GROUP BY CASE WHEN a.CCC_PL_REPORTING_REGION='AMERICAS' THEN 'US' WHEN a.CCC_PL_REPORTING_REGION='EMEA' THEN 'EUROPE' ELSE 'ASIA' END, a.COB_DATE, CASE WHEN a.EXECUTIVE_MODEL IN ('AVERAGING-CONVERTIBLE', 'DEFAULTED-CONVERTIBLE', 'RESETTABLE-CONVERTIBLE') THEN 'MANDATORY' ELSE 'NON-MANDATORY' END