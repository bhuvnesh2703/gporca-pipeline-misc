SELECT 'CASH EQUITIES' as CCC_BUSINESS_AREA, SUM (ABS (vega)) AS Grossvega FROM ( SELECT UNDERLIER_TICK || '.' || UNDERLIER_EXCH AS Underlier, SUM (COALESCE (a.USD_EQ_PARTIAL_KAPPA, 0))/1000 AS vega FROM CDWUSER.U_EQ_MSR a WHERE a.COB_DATE = '2018-02-28' AND a.LE_GROUP = 'UK' AND a.EXECUTIVE_MODEL IN ('VARIANCESWAP', 'CAPPEDVARIANCESWAP', 'VOLATILITYSWAP', 'CAPPEDVOLATILITYSWAP') AND a.CCC_DIVISION = 'INSTITUTIONAL EQUITY DIVISION' AND a.CCC_BUSINESS_AREA = 'CASH EQUITIES' GROUP BY UNDERLIER_TICK || '.' || UNDERLIER_EXCH ) x UNION ALL SELECT 'DERIVATIVES' as CCC_BUSINESS_AREA, SUM (ABS (vega)) AS Grossvega FROM ( SELECT UNDERLIER_TICK || '.' || UNDERLIER_EXCH AS Underlier, SUM (COALESCE (a.USD_EQ_PARTIAL_KAPPA, 0))/1000 AS vega FROM CDWUSER.U_EQ_MSR a WHERE a.COB_DATE = '2018-02-28' AND a.LE_GROUP = 'UK' AND a.EXECUTIVE_MODEL IN ('VARIANCESWAP', 'CAPPEDVARIANCESWAP', 'VOLATILITYSWAP', 'CAPPEDVOLATILITYSWAP') AND a.CCC_DIVISION = 'INSTITUTIONAL EQUITY DIVISION' AND a.CCC_BUSINESS_AREA = 'DERIVATIVES' GROUP BY UNDERLIER_TICK || '.' || UNDERLIER_EXCH ) x UNION ALL SELECT 'OTHER IED' as CCC_BUSINESS_AREA, SUM (ABS (vega)) AS Grossvega FROM ( SELECT UNDERLIER_TICK || '.' || UNDERLIER_EXCH AS Underlier, SUM (COALESCE (a.USD_EQ_PARTIAL_KAPPA, 0))/1000 AS vega FROM CDWUSER.U_EQ_MSR a WHERE a.COB_DATE = '2018-02-28' AND a.LE_GROUP = 'UK' AND a.EXECUTIVE_MODEL IN ('VARIANCESWAP', 'CAPPEDVARIANCESWAP', 'VOLATILITYSWAP', 'CAPPEDVOLATILITYSWAP') AND a.CCC_DIVISION = 'INSTITUTIONAL EQUITY DIVISION' AND a.CCC_BUSINESS_AREA = 'OTHER IED' GROUP BY UNDERLIER_TICK || '.' || UNDERLIER_EXCH ) x UNION ALL SELECT 'PRIME BROKERAGE' as CCC_BUSINESS_AREA, SUM (ABS (vega)) AS Grossvega FROM ( SELECT UNDERLIER_TICK || '.' || UNDERLIER_EXCH AS Underlier, SUM (COALESCE (a.USD_EQ_PARTIAL_KAPPA, 0))/1000 AS vega FROM CDWUSER.U_EQ_MSR a WHERE a.COB_DATE = '2018-02-28' AND a.LE_GROUP = 'UK' AND a.EXECUTIVE_MODEL IN ('VARIANCESWAP', 'CAPPEDVARIANCESWAP', 'VOLATILITYSWAP', 'CAPPEDVOLATILITYSWAP') AND a.CCC_DIVISION = 'INSTITUTIONAL EQUITY DIVISION' AND a.CCC_BUSINESS_AREA = 'PRIME BROKERAGE' GROUP BY UNDERLIER_TICK || '.' || UNDERLIER_EXCH ) x UNION ALL SELECT 'OTHER' as CCC_BUSINESS_AREA, SUM (ABS (vega)) AS Grossvega FROM ( SELECT UNDERLIER_TICK || '.' || UNDERLIER_EXCH AS Underlier, SUM (COALESCE (a.USD_EQ_PARTIAL_KAPPA, 0))/1000 AS vega FROM CDWUSER.U_EQ_MSR a WHERE a.COB_DATE = '2018-02-28' AND a.LE_GROUP = 'UK' AND a.EXECUTIVE_MODEL IN ('VARIANCESWAP', 'CAPPEDVARIANCESWAP', 'VOLATILITYSWAP', 'CAPPEDVOLATILITYSWAP') AND a.CCC_DIVISION = 'INSTITUTIONAL EQUITY DIVISION' AND a.CCC_BUSINESS_AREA NOT IN ('PRIME BROKERAGE', 'OTHER IED','DERIVATIVES','CASH EQUITIES') GROUP BY UNDERLIER_TICK || '.' || UNDERLIER_EXCH ) x