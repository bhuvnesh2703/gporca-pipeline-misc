SELECT a.Cob_date, CASE WHEN (A.CCC_BUSINESS_AREA IN ('NA ELECTRICITYNATURAL GAS') OR A.CCC_PRODUCT_LINE IN ('NA POWER & GAS')) THEN 'NA ELECTRICITYNATURAL GAS' WHEN ((A.CCC_BUSINESS_AREA in ('OIL LIQUIDS') AND A.CCC_PRODUCT_LINE NOT IN ('LEGACY OIL')) OR A.CCC_PRODUCT_LINE IN ('OIL & PRODUCTS')) THEN 'OIL LIQUIDS' WHEN ((A.CCC_BUSINESS_AREA IN ('METALS') AND A.CCC_PRODUCT_LINE NOT IN ('BULKS', 'BASE METALS')) OR A.CCC_PRODUCT_LINE IN ('PRECIOUS METALS')) THEN 'PRECIOUS METALS' WHEN (A.CCC_BUSINESS_AREA IN ('INVESTOR BUSINESS') OR A.CCC_PRODUCT_LINE IN ('COMMOD EXOTICS', 'COMMOD INDEX')) THEN 'INVESTOR BUSINESS' WHEN ((A.CCC_BUSINESS_AREA IN ('AGRICULTURALS','AP EU ELECRICNATURAL GAS', 'OLYMPUS','TMG')) OR (A.CCC_BUSINESS_AREA IN ('METALS') AND A.CCC_PRODUCT_LINE IN ('BULKS', 'BASE METALS')) OR (A.CCC_PRODUCT_LINE IN ('COMMOD LEGACY TRADING'))) THEN 'LEGACY TRADING' WHEN (A.CCC_BUSINESS_AREA IN ('COMMOND - FUNDING')) THEN 'COMMOD - FUNDING' WHEN CCC_PRODUCT_LINE = 'EU POWER & GAS' THEN 'EU POWER & GAS' ELSE 'OTHER' END AS CCC_BUSINESS_AREA, a.PRODUCT_TYPE_CODE, a.CCC_PRODUCT_LINE,a.TIME_BUCKET_CALENDAR, sum(a.USD_CM_Delta) as Dollar_Delta, sum(a.USD_CM_GAMMA)/20 as Dollar_Gamma, sum(a.USD_CM_KAPPA) as Dollar_Vega, sum(a.USD_CM_THETA) as Dollar_Theta, sum(case when a.PRODUCT_TYPE_CODE = 'TIMESPREAD' and A.PROD_POS_NAME_DESCRIPTION = 'WCS POST SPREAD' then 0 else cast(a.RAW_CM_DELTA as numeric(15,5)) end) as Raw_Delta, SUM (CASE WHEN a.PRODUCT_TYPE_CODE = 'TIMESPREAD' AND a.PROD_POS_NAME_DESCRIPTION = 'WCS POST SPREAD' THEN 0 WHEN (a.book = 'COMXO' and A.CCC_PRODUCT_LINE IN ('COMMOD EXOTICS')) then CAST(USD_CM_KAPPA_ABSOLUTE AS NUMERIC (15, 5)) ELSE CAST (a.RAW_CM_KAPPA AS NUMERIC (15, 5)) END) AS RAW_Vega FROM cdwuser.U_CM_MSR a WHERE A.COB_DATE IN ('2018-02-28','2018-02-21') AND (CCC_PL_REPORTING_REGION IN ('EUROPE','EMEA')) and ((CCC_BUSINESS_AREA NOT IN ('CREDIT', 'MS CVA MNE - COMMOD') AND CCC_DIVISION = 'COMMODITIES' ) /*OLD LOGIC*/ OR (A.CCC_DIVISION = 'FIXED INCOME DIVISION' AND A.CCC_BUSINESS_AREA = 'COMMODITIES' and a.CCC_STRATEGY NOT IN ('MS CVA MNE - COMMOD'))) /*NEW LOGIC; THERE IS NO CREDIT BUSINESS AREA IN CM ANYMORE CALLED CREDIT*/ and NOT(INCLUDE_IN_REG_CAAP_FL = 'N' and PRODUCT_SUB_TYPE_CODE in ('N EAST CONSUMPTI', 'N EAST CONSUMPTION', 'N EAST APPALACHI', 'N EAST APPALACHIA') and BOOK = '18003') AND PRODUCT_TYPE_CODE NOT IN ('CURRENCY', 'INTEREST RATE', 'INFLATION', 'TBD', 'MISC','CVA', 'FVA', 'ERROR') AND NOT(cob_date >= '2017-03-29' and cob_date <= '2017-07-05' and CCC_PRODUCT_LINE in( 'IB STRUCTURED', 'COMMOD EXOTICS') AND A.PRODUCT_TYPE_CODE = 'ZCS') Group By a.Cob_date, CASE WHEN (A.CCC_BUSINESS_AREA IN ('NA ELECTRICITYNATURAL GAS') OR A.CCC_PRODUCT_LINE IN ('NA POWER & GAS')) THEN 'NA ELECTRICITYNATURAL GAS' WHEN ((A.CCC_BUSINESS_AREA in ('OIL LIQUIDS') AND A.CCC_PRODUCT_LINE NOT IN ('LEGACY OIL')) OR A.CCC_PRODUCT_LINE IN ('OIL & PRODUCTS')) THEN 'OIL LIQUIDS' WHEN ((A.CCC_BUSINESS_AREA IN ('METALS') AND A.CCC_PRODUCT_LINE NOT IN ('BULKS', 'BASE METALS')) OR A.CCC_PRODUCT_LINE IN ('PRECIOUS METALS')) THEN 'PRECIOUS METALS' WHEN (A.CCC_BUSINESS_AREA IN ('INVESTOR BUSINESS') OR A.CCC_PRODUCT_LINE IN ('COMMOD EXOTICS', 'COMMOD INDEX')) THEN 'INVESTOR BUSINESS' WHEN ((A.CCC_BUSINESS_AREA IN ('AGRICULTURALS','AP EU ELECRICNATURAL GAS', 'OLYMPUS','TMG')) OR (A.CCC_BUSINESS_AREA IN ('METALS') AND A.CCC_PRODUCT_LINE IN ('BULKS', 'BASE METALS')) OR (A.CCC_PRODUCT_LINE IN ('COMMOD LEGACY TRADING'))) THEN 'LEGACY TRADING' WHEN (A.CCC_BUSINESS_AREA IN ('COMMOND - FUNDING')) THEN 'COMMOD - FUNDING' WHEN CCC_PRODUCT_LINE = 'EU POWER & GAS' THEN 'EU POWER & GAS' ELSE 'OTHER' END, a.PRODUCT_TYPE_CODE, a.CCC_PRODUCT_LINE,a.TIME_BUCKET_CALENDAR