SELECT a.COB_DATE , a.TIME_BUCKET_CALENDAR , a.TIME_BUCKET_QUARTER , a.PRODUCT_TYPE_CODE , CASE WHEN (a.PRODUCT_TYPE_CODE LIKE 'TEXAS%' OR a.PRODUCT_TYPE_CODE LIKE '%ERCOT%') THEN 'Texas' WHEN a.PRODUCT_TYPE_CODE LIKE 'WEST%' THEN 'West' WHEN (a.PRODUCT_TYPE_CODE LIKE 'EAST%' OR a.PRODUCT_TYPE_CODE LIKE ('MIDWEST%') OR a.PRODUCT_TYPE_CODE LIKE 'GRNPWR%') THEN 'East' WHEN a.PRODUCT_TYPE_CODE IN ('NATGAS') THEN 'Natgas' WHEN a.PRODUCT_TYPE_CODE IN ('COAL', 'COAL-US', 'EMISSIONS-US', 'WEATHER') THEN 'Other Fuels' ELSE 'other' END AS region , CASE WHEN (extract(month from a.FUTURES_EQUIVALENT_DATE) - extract(month from a.COB_DATE)) + 12 * (extract(year from a.FUTURES_EQUIVALENT_DATE) - extract(year from a.COB_DATE)) < 36 THEN 'Liquid' WHEN (extract(month from a.FUTURES_EQUIVALENT_DATE) - extract(month from a.COB_DATE)) + 12 * (extract(year from a.FUTURES_EQUIVALENT_DATE) - extract(year from a.COB_DATE)) < 72 THEN 'Moderately Liquid' ELSE 'Illiquid' END AS TIME_SPREAD_BUCKET , SUM(a.USD_CM_DELTA) AS CM_DELTA FROM CDWUSER.U_CM_MSR a WHERE a.cob_date in ('02/28/2018', '01/31/2018' ) AND ((CCC_DIVISION = 'COMMODITIES' AND CCC_BUSINESS_AREA = 'NA ELECTRICITYNATURAL GAS') or (CCC_BUSINESS_AREA = 'COMMODITIES' AND CCC_PRODUCT_LINE = 'NA POWER & GAS')) AND PRODUCT_TYPE_CODE NOT IN ('CURRENCY', 'INTEREST RATE', 'INFLATION', 'TBD', 'MISC','CVA', 'FVA') AND CCC_TAPS_COMPANY IN ('0105', '0513', '1133', '1614', '2042', '4442', '4919', '4920', '5122', '6399', '7025', '7038', '7114', '7323', '7410', '7504', '7516', '8296', '8304', '8562', '8747', '8934', '7706', '7549', '6706', '5427', '7414', '7465', '7493', '7278', '7279', '7705', '7624', '7681', '7800', '5656', '7708', '7707', '7801', '7627', '7759', '7710', '7762', '7630', '7628', '7761', '7684', '5923', '7609') GROUP BY a.COB_DATE , a.TIME_BUCKET_CALENDAR , a.TIME_BUCKET_QUARTER , a.FUTURES_EQUIVALENT_DATE , a.PRODUCT_TYPE_CODE