SELECT process_id, bu_risk_system, grid_measure_name, scenario_name, cob_date, SUM (grid_measure_value) grid_measure_value FROM oeruser.u_grid_msr WHERE cob_date in ('2018-02-28','2018-01-31') and process_id = 2212 AND grid_measure_name IN ('USD_IREQ_SPOT', 'USD_EQEQ_SPOT') AND CCC_STRATEGY IN ('HYBRIDS INT RATES NA') AND grid_measure_value IS NOT NULL GROUP BY process_id, bu_risk_system, grid_measure_name, scenario_name, cob_date