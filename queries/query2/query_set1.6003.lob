SELECT ISSUER_COUNTRY_CODE, CASE WHEN a.ISSUER_COUNTRY_CODE IN ('XS', 'VGB', 'USA', 'SWE', 'PRT', 'NZL', 'NOR', 'NLD', 'LUX', 'JPN', 'JEY', 'ITA', 'ISL', 'IRL', 'IMN', 'GRC', 'GGY', 'GBR', 'FRA', 'FIN', 'ESP', 'DNK', 'DEU', 'CYP', 'CYM', 'CHE', 'CAN', 'BMU', 'BEL', 'AUT', 'AUS') THEN 'G10' ELSE 'EM' END AS EMFLAG, COALESCE(SUM (A.USD_PV10_BENCH), 0) AS USD_PV10_BENCH FROM cdwuser.U_DM_CC A WHERE a.cob_date IN ('2018-02-28') AND A.CREDIT_TYPE_T1T2_FLAG IN ('CREDIT-CORP_T1T2', 'EQUITY_T1T2', 'Tier1_Sov') AND ISSUER_COUNTRY_CODE not in ('VEN') AND IS_UK_GROUP = 'Y' AND CCC_DIVISION NOT IN ('FID DVA','FIC DVA') AND CCC_STRATEGY NOT IN ('MS DVA STR NOTES IED') GROUP BY ISSUER_COUNTRY_CODE