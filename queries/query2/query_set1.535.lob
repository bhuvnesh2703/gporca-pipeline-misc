SELECT cob_date, a.POSITION_ULT_ISSUER_PARTY_DARWIN_NAME, SUM (a.USD_EXPOSURE) AS netexp, a.EXPIRATION_DATE, a.CCC_PL_REPORTING_REGION, a.issuer_country_code FROM cdwuser.u_cr_msr a WHERE a.COB_DATE in ('2018-02-28','2018-02-21') AND a.LE_GROUP ='UK' AND A.CCC_PRODUCT_LINE in ('DISTRESSED TRADING') AND a.USD_EXPOSURE is not null AND a.CCC_PL_REPORTING_REGION in ('ASIA PACIFIC','JAPAN') GROUP BY cob_date,a.POSITION_ULT_ISSUER_PARTY_DARWIN_NAME, a.EXPIRATION_DATE, a.CCC_PL_REPORTING_REGION, a.issuer_country_code