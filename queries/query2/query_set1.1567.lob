SELECT COB_DATE,CCC_PL_REPORTING_REGION,PROD_POS_NAME_DESCRIPTION, SUM(cast(USD_FX_DELTA as numeric(15,5))) as USD_DELTA, SUM(cast(USD_FX_GAMMA as numeric(15,5))) as USD_GAMMA FROM cdwuser.U_FX_MSR Where COB_DATE IN ('2018-02-28','2018-02-27') AND ((CCC_BUSINESS_AREA not in ('CREDIT','MS CVA MNE - COMMOD', 'COMMODS FINANCING', 'INVESTMENTS AND STR TRANS','INVESTOR BUSINESS') AND CCC_DIVISION = 'COMMODITIES' and CCC_PRODUCT_LINE in ('PRECIOUS METALS', 'INV STR TRANS - METALS')) /*OLD LOGIC*/ OR (CCC_DIVISION = 'FIXED INCOME DIVISION' AND CCC_BUSINESS_AREA IN ('COMMODITIES') and CCC_PRODUCT_LINE in ('PRECIOUS METALS'))) /*NEW LOGIC*/ group by COB_DATE,CCC_PL_REPORTING_REGION,PROD_POS_NAME_DESCRIPTION