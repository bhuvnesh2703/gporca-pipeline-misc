SELECT a.COB_DATE ,a.PARENT_LEGAL_ENTITY , a.ctpy_netting_group_code , case when a.POSITION_CREDIT_PARTY_DARWIN_NAME in ('LCH LIMITED') then 'LCH.CLEARNET LIMITED' else a.POSITION_CREDIT_PARTY_DARWIN_NAME end as POSITION_CREDIT_PARTY_DARWIN_NAME , 0 as rank ,sum(A.USD_PV01SPRD) as USD_PV01SPRD FROM OERUSER.U_EXP_MSR a WHERE a.cob_date IN ('2017-03-31','2017-04-28','2017-05-31','2017-06-30','2017-07-31','2017-08-31','2017-09-29','2017-10-31','2017-11-30','2017-12-29','2018-01-31','2018-02-28') AND a.PARENT_LEGAL_ENTITY in ('0111') and a.POSITION_CREDIT_PARTY_DARWIN_NAME in( 'ICE CLEAR EUROPE LIMITED', 'CHICAGO MERCANTILE EXCHANGE INC.', 'ICE CLEAR CREDIT LLC', 'LCH.CLEARNET LIMITED','LCH LIMITED', 'MORGAN STANLEY & CO. LLC', 'MORGAN STANLEY & CO. INTERNATIONAL PLC', 'MORGAN STANLEY BANK, N.A.', 'MORGAN STANLEY CAPITAL GROUP INC.', 'N/A' ) and a.USD_PV01SPRD is not null GROUP BY a.COB_DATE ,a.PARENT_LEGAL_ENTITY ,a.ctpy_netting_group_code , case when a.POSITION_CREDIT_PARTY_DARWIN_NAME in ('LCH LIMITED') then 'LCH.CLEARNET LIMITED' else a.POSITION_CREDIT_PARTY_DARWIN_NAME end