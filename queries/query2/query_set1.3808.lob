select a.cob_date, A.CCC_PL_REPORTING_REGION, A.CCC_BUSINESS_AREA, A.CCC_PRODUCT_LINE, A.CCC_DIVISION, CASE WHEN A.CCC_TAPS_COMPANY IN ('0201','0103','0205','0206','0530','5924') THEN 'MSCO' ELSE A.CCC_TAPS_COMPANY END AS LE, sum(coalesce(A.SLIDE_EQ_MIN_20_USD,0)) as SLIDE_EQ_MIN_20_USD, sum(coalesce(A.SLIDE_EQ_MIN_10_USD,0)) as SLIDE_EQ_MIN_10_USD, sum(coalesce(A.SLIDE_EQ_MIN_05_USD,0)) as SLIDE_EQ_MIN_05_USD, sum(coalesce(A.SLIDE_EQ_PLS_10_USD,0)) as SLIDE_EQ_PLS_10_USD,sum(coalesce(A.SLIDE_EQ_PLS_05_USD,0)) as SLIDE_EQ_PLS_05_USD, sum(coalesce(A.SLIDE_EQ_PLS_20_USD,0)) as SLIDE_EQ_PLS_20_USD from cdwuser.u_exp_msr a where A.cob_date = '2018-02-28' AND a.CCC_TAPS_COMPANY IN ('0201','0103','0205','0206','0530','5924') group by a.cob_date, A.CCC_PL_REPORTING_REGION, A.CCC_BUSINESS_AREA, A.CCC_PRODUCT_LINE, A.CCC_DIVISION, CASE WHEN A.CCC_TAPS_COMPANY IN ('0201','0103','0205','0206','0530','5924') THEN 'MSCO' ELSE A.CCC_TAPS_COMPANY END