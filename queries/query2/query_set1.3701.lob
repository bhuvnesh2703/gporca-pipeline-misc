SELECT     a.COB_DATE,     a.CURVE_CURRENCY,     SUM (COALESCE (a.USD_CONVX,0) ) AS usd_ir_gamma FROM cdwuser.U_IR_MSR_INTRPLT a WHERE (a.COB_DATE = '2018-02-28' or a.COB_DATE = '2018-02-27') and A.CCC_TAPS_COMPANY in ('0362','0816') AND      (a.VAR_EXCL_FL <> 'Y') AND     a.CCC_DIVISION IN ('FIXED INCOME DIVISION') AND     /*a.CCC_BUSINESS_AREA NOT IN ('COMMODITIES') AND*/       CCC_BANKING_TRADING IN ('TRADING') GROUP BY     a.COB_DATE,     a.CURVE_CURRENCY