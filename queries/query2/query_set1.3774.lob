SELECT A.COB_DATE, A.RATING2, A.SECTYPE2, ccc_taps_company, SUM (A.USD_PV10_BENCH) AS USD_PV10_BENCH FROM CDWUSER.U_DM_CC A WHERE a.COB_DATE IN ('03/31/2017','04/28/2017','05/31/2017','06/30/2017','07/31/2017','08/31/2017','09/29/2017','10/31/2017','11/30/2017','12/29/2017','01/31/2018','02/28/2018') AND A.CREDIT_CORPS_CCC_BUSINESS_AREA IN ('CREDIT-CORPORATES') AND ( A.CCC_STRATEGY NOT LIKE '%PAR LOANS TRADING%' OR A.CCC_STRATEGY NOT LIKE '%PRIVATE LOANS TRADING%') AND A.CREDIT_TYPE_T1T2_FLAG = 'CREDIT-CORP_T1T2' AND A.CCC_STRATEGY NOT IN ('DISTRESSED TRADING1', 'NON MSMS1') AND A.CCC_TAPS_COMPANY IN ( '0201','0103','0205','0206','0530','5924') GROUP BY A.RATING2, A.SECTYPE2, ccc_taps_company, A.COB_DATE