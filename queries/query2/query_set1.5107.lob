SELECT SUM ((USD_FX) :: numeric(15,5)) AS FX, cob_date, book, currency_of_measure, vertical_system, CCC_PRODUCT_LINE, CCC_STRATEGY, ccc_business_area, a.ccc_division, ccc_banking_trading_localreg, product_type_code FROM cdwuser.U_FX_MSR a WHERE a.COB_DATE IN ( '2018-02-28', '2018-01-31', '2017-12-29', '2017-11-30', '2017-10-31', '2017-09-29' ) and CCC_TAPS_COMPANY IN ('1633') and VAR_EXCL_FL <> 'Y' AND a.ccc_division IN ('FIXED INCOME DIVISION') and a.CCC_PL_REPORTING_REGION not in ('AMERICAS') AND usd_fx <> 0 and currency_of_measure NOT IN ('AED', 'BHD', 'CNH', 'CNY', 'HKD', 'OMR', 'QAR', 'SAR', 'UB1', 'UBD', 'USD') AND ccc_banking_trading_localreg = 'TRADING' GROUP BY cob_date, book, currency_of_measure, vertical_system, CCC_PRODUCT_LINE, CCC_STRATEGY, ccc_business_area, a.ccc_division, ccc_banking_trading_localreg, product_type_code