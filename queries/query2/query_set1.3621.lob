SELECT COB_DATE, A.CCC_DIVISION, 'LENDING' AS CCC_BUSINESS_AREA, SUM(COALESCE(A.USD_PV10_BENCH,0))/1000 AS BPV10 FROM CDWUSER.U_DM_CC A WHERE COB_DATE IN ( '01/31/2018','02/28/2018') AND ( CCC_BUSINESS_AREA = 'LENDING' OR ( CCC_BUSINESS_AREA = 'CREDIT-CORPORATES' AND CCC_PRODUCT_LINE = 'PRIMARY - LOANS' ) ) AND CCC_STRATEGY NOT IN ( 'CORPORATE LOAN STRATEGY','PROJECT FINANCE','EVENT - INV GRADE','EVENT - NON INV GRADE' ) AND A.VERTICAL_SYSTEM NOT LIKE 'PIPELINE%' AND A.VAR_EXCL_FL IN ('N') AND ( ( ( BOOK LIKE 'HFS%' AND BOOK LIKE '%UNHEDG%' ) OR BOOK IN ('BIXLU', 'BIXNU', 'BNYUU') ) OR ( BOOK LIKE 'REL%' AND BOOK LIKE '%UNHEDG%' ) OR ( ( BOOK LIKE 'HFS%' AND BOOK LIKE '%HEDG%' AND BOOK NOT LIKE '%UNHEDG%' ) OR BOOK IN ( 'ALOAN', 'APLJV', 'ASCLV', 'BALON', 'BHYLG', 'BLNUT', 'BNYUT', 'MSBIP', 'HYLLG' ) ) OR BOOK IN ('HFIEA','HFIET','HFIEU','HFINA','HFINT','HFIEB', 'HFINB') OR ( BOOK LIKE 'REL%' OR BOOK = 'PMGPB' ) OR ( ( BOOK LIKE '%LAF%' AND BOOK LIKE '%ELTH HFS%' ) OR BOOK = 'LAF - NA - NIG WORKOUT HFS MSSFI-LNWMF' ) OR BOOK LIKE '%ELTH FVO%' ) GROUP BY A.COB_DATE, A.CCC_DIVISION UNION ALL SELECT A.COB_DATE, 'BANK RESOURCE MANAGEMENT' AS CCC_DIVISION, 'CVA' AS CCC_BUSINESS_AREA, SUM(A.USD_PV10_BENCH)/1000 AS BPV10 FROM CDWUSER.U_DM_FIRMWIDE A WHERE COB_DATE IN ( '01/31/2018','02/28/2018') AND ( CCC_BUSINESS_AREA IN ('CPM') OR CCC_STRATEGY IN ('MS CVA MPE - DERIVATIVES', 'MS CVA MNE - DERIVATIVES') ) AND A.VAR_EXCL_FL IN ('N') AND A.DIVISION_GROUP IN ('ISG CORE') GROUP BY A.COB_DATE, A.DIVISION_GROUP UNION ALL SELECT A.COB_DATE, A.CCC_DIVISION, A.CCC_BUSINESS_AREA, SUM(A.USD_BPV10_FID) /1000 AS BPV10 FROM CDWUSER.U_DM_FIRMWIDE A WHERE COB_DATE IN ( '01/31/2018','02/28/2018') AND CCC_BUSINESS_AREA <> 'LENDING' AND CCC_BUSINESS_AREA NOT LIKE 'CPM%' AND CCC_BUSINESS_AREA <> 'COMMODITIES' AND CCC_BUSINESS_AREA <> 'DERIVS FUNDING' AND A.USD_BPV10_FID IS NOT NULL AND A.DIVISION_GROUP IN ('ISG CORE') AND NOT ( A.CCC_PRODUCT_LINE IN ('DISTRESSED TRADING') AND A.CASH_TYPE <> 'INDEX' ) AND NOT ( A.CCC_BUSINESS_AREA = 'SECURITIZED PRODUCTS GRP' AND SPG_DESC IN ('CMBS IO REREMIC','CMBS IO','CMBS SECURITY','RMBS PRIME RESIDUAL','CMBS INDEX','CORPORATE INDEX') AND CCC_PRODUCT_LINE IN ('CRE LENDING', 'CREL BANK HFI', 'CRE LENDING SEC/HFS') ) GROUP BY A.COB_DATE, A.CCC_DIVISION, A.CCC_BUSINESS_AREA UNION ALL SELECT A.COB_DATE, CASE WHEN A.CCC_BUSINESS_AREA IN ('COMMODITIES') THEN 'COMMODITIES' ELSE CCC_DIVISION END AS CCC_DIVISION, A.CCC_BUSINESS_AREA, SUM(COALESCE(A.USD_PV10_BENCH,0))/1000 AS BPV10 FROM CDWUSER.U_DM_FIRMWIDE A WHERE COB_DATE IN ( '01/31/2018','02/28/2018') AND A.DIVISION_GROUP IN ('ISG CORE') AND ( A.CCC_DIVISION NOT IN ('FIXED INCOME DIVISION') OR A.CCC_BUSINESS_AREA IN ('COMMODITIES') ) AND A.CCC_DIVISION NOT IN ('FIC DVA','FID DVA') AND A.CCC_BUSINESS_AREA NOT LIKE 'CPM%' AND A.CCC_BUSINESS_AREA NOT IN ('DERIVS FUNDING') AND A.CCC_PRODUCT_LINE NOT IN ('DISTRESSED TRADING') AND A.VAR_EXCL_FL = 'N' GROUP BY A.COB_DATE, CASE WHEN A.CCC_BUSINESS_AREA IN ('COMMODITIES') THEN 'COMMODITIES' ELSE CCC_DIVISION END, A.CCC_BUSINESS_AREA