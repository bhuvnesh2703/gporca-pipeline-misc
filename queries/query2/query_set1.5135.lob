SELECT f.COB_DATE, case when f.BOOK like '%JUMBO%' then 'FRM' when f.BOOK like '%YRARM%' then 'ARM' else '1mARM' end as Type, book, case when f.FACILITY_TYPE = 'RATELOCK' then 'rateLock' else 'long' end as FACILITY_TYPE, SUM (CASE WHEN f.COUPON > 0 THEN f.USD_NOTIONAL * f.COUPON / 1200 * (1 / (1 - POWER (1 + f.COUPON / 1200, - CASE WHEN f.BOOK LIKE '%15Y%' THEN 15 * 12 ELSE 30 * 12 END)) - 1) ELSE 0.0 END) AS Sched, CASE WHEN x.COB_DATE IS NULL THEN 'New' ELSE 'Old' END AS Status, '20' || SUBSTRING(f.VINTAGE,2) as VINTAGE2, sum(COALESCE(f.usd_notional, 0)) ::numeric(15,5) as usd_notional FROM cdwuser.U_OT_MSR f LEFT OUTER JOIN ( SELECT f.COB_DATE, extract(YEAR from (f.COB_DATE)) * 100 + extract(MONTH from (f.COB_DATE)) AS Curr_Month, f.TAPSCUSIP, f.Product_DESCRIPTION, sum(COALESCE(f.usd_notional, 0)) ::numeric(15,5) as usd_notional FROM cdwuser.U_OT_MSR f WHERE f.cob_date in ( '2018-01-31' ) AND f.CCC_DIVISION IN ('PRIVATE BANKING GROUP', 'RETAIL BANKING GROUP', 'PWM US BRANCH', 'CENTRAL PORTFOLIO', 'HELD FOR INVESTMENT - (HFI)', 'HELD FOR SALE - (HFS)') /* f.CCC_STRATEGY IN ('MSPBNA - VH MORTGAGE') */AND f.PRODUCT_TYPE_CODE LIKE 'LOAN%' AND f.FACILITY_TYPE IN ('LONG') AND f.BOOK NOT LIKE '%ORIG%' and VAR_EXCL_FL<> 'Y' AND PARENT_LEGAL_ENTITY In ('6635(G)') GROUP BY f.COB_DATE, f.TAPSCUSIP, f.Product_DESCRIPTION ) x ON (extract(YEAR from (f.COB_DATE - interval '1 MONTH' )) * 100 + extract(MONTH from (f.COB_DATE - interval '1 MONTH' )), f.TAPSCUSIP, f.Product_DESCRIPTION) = (x.Curr_Month, x.TAPSCUSIP, x. Product_DESCRIPTION) WHERE f.cob_date in ( '2018-02-28' ) AND CCC_DIVISION IN ('PRIVATE BANKING GROUP', 'RETAIL BANKING GROUP', 'PWM US BRANCH', 'CENTRAL PORTFOLIO', 'HELD FOR INVESTMENT - (HFI)', 'HELD FOR SALE - (HFS)') AND /* f.CCC_STRATEGY IN ('MSPBNA - VH MORTGAGE')*/ f.PRODUCT_TYPE_CODE LIKE 'LOAN%' AND f.BOOK NOT LIKE '%ORIG%' and VAR_EXCL_FL<> 'Y' AND PARENT_LEGAL_ENTITY In ('6635(G)') group by f.COB_DATE, f.BOOK, case when BOOK like '%JUMBO%' then 'FRM' when BOOK like '%YRARM%' then 'ARM' else '1mARM' end, f.FACILITY_TYPE, f.VINTAGE, f.COUPON, CASE WHEN x.COB_DATE IS NULL THEN 'New' ELSE 'Old' END