SELECT cob_date ,source_scn_name ,SUM(raw_pnl) / 1000 PNL FROM DWUSER.U_RAW_SCENARIO_PNL a WHERE cob_date = ( SELECT MAX(cob_date) FROM DWUSER.U_RAW_SCENARIO_PNL a WHERE source_scn_name = 'FRB2017_SA_FX_All' AND CCC_BUSINESS_AREA = 'FXEM MACRO TRADING' AND COB_DATE <= '2018-02-28' AND COB_DATE >= '2018-02-13' ) AND CCC_BUSINESS_AREA = 'FXEM MACRO TRADING' AND PRODUCT_TYPE in ('FXOPT', 'FXBSKT') AND (source_scn_name LIKE 'FRB2017_SA_FX_All%' or source_scn_name LIKE '%BHC2017_S1_FX_ALL%') AND BU_RISK_SYSTEM LIKE 'FXOPT%' GROUP BY A.cob_date ,A.source_scn_name