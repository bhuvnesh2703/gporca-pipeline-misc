select GICS_LEVEL_1_NAME, position_ult_issuer_party_darwin_name, ccc_business_area, ccc_product_line, sum(case when COB_DATE = '2018-02-28' then usd_net_exposure else 0 end) as netexp, abs(sum(case when COB_DATE = '2018-02-28' then usd_net_exposure else 0 end)) as abs, sum(case when COB_DATE = '2018-02-28' then 0 else usd_net_exposure end) as netexpprevious, sum(case when COB_DATE = '2018-02-28' then usd_pv10_bench else 0 end) as pv10 from cdwuser.U_DM_CC a where a.COB_DATE in ('2018-02-28','2018-02-27') and a.ccc_division = ('FIXED INCOME DIVISION') and (a.usd_net_exposure <> 0 and a.usd_net_exposure is not null) AND a.CCC_TAPS_COMPANY ='0302' AND a.CCC_BUSINESS_AREA NOT IN ('NON CORE','CPM','LENDING') AND a.CCC_BANKING_TRADING = 'TRADING' AND book not in ('OBTRS')
AND (a.FID1_INDUSTRY_NAME_LEVEL1 NOT IN ('SOVEREIGN', 'GOVERNMENT SPONSORED') 
OR a.ISSUER_COUNTRY_CODE NOT IN ('DNK','BMU','CYM','AUS','AUT','BEL','CAN','CHE','DEU','ESP','FIN','FRA','GBR','GRC','IRL','ITA','JPN','LUX','NLD','NOR','NZL','PRT','SWE','USA','JEY','GGY','SUP','XS','XCI','VGB','CYP')) group by GICS_LEVEL_1_NAME, position_ult_issuer_party_darwin_name, ccc_business_area, ccc_product_line