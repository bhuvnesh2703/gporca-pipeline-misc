SELECT COB_DATE,CCC_PL_REPORTING_REGION, CASE WHEN CCC_BUSINESS_AREA = 'COMMODITIES' THEN CCC_PRODUCT_LINE ELSE CCC_BUSINESS_AREA END AS CCC_BUSINESS_AREA, PROD_POS_NAME_DESCRIPTION, sum(cast(coalesce(USD_CM_LEASE_RATE,0) as numeric (15,5)) + cast(coalesce(USD_IR_RHO,0) as numeric (15,5)))/10000 as Interest_Rate FROM cdwuser.U_EXP_MSR WHERE COB_DATE IN ('2018-02-28','2018-02-27') and ((CCC_DIVISION='COMMODITIES' AND CCC_BUSINESS_AREA in ('METALS') AND PRODUCT_TYPE_CODE IN ('PRECIOUSMETAL', 'FUND')) /* OLD LOGIC*/ or (CCC_DIVISION = 'FIXED INCOME DIVISION' AND CCC_BUSINESS_AREA = 'COMMODITIES' and (CCC_PRODUCT_LINE IN ('PRECIOUS METALS')) AND CCC_STRATEGY NOT IN ('MS CVA MNE - COMMOD'))/* NEW LOGIC*/ ) and PRODUCT_TYPE_CODE in ('CURRENCY','INV STR TRANS - METALS') group by COB_DATE,CCC_PL_REPORTING_REGION, TIME_BUCKET_CALENDAR, EXPIRATION_DATE,PROD_POS_NAME_DESCRIPTION, CASE WHEN CCC_BUSINESS_AREA = 'COMMODITIES' THEN CCC_PRODUCT_LINE ELSE CCC_BUSINESS_AREA END