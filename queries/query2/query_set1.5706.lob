SELECT case WHEN ACCOUNT IN ('07700A931') THEN 'Defunded Assets' ELSE A.BOOK_STRATEGY END AS BOOK_STRATEGY, a.cob_date, a.BOOK, a.ACCOUNT, a.CURRENCY_MAPPING, a.CURRENCY_OF_MEASURE, Case WHEN var_excl_fl ='Y' THEN 'Excluded' ELSE 'Included' END AS Var_Inclusion, sum(a.USD_EXPOSURE) as USD_EXPOSURE, sum(coalesce(case when currency_of_measure not in ('UBD', 'USD') then usd_fx else 0 end, 0)) AS usd_fx, SUM(CASE WHEN book in ('INDMF','MSDEP','TSTPM','PRFEQ','TRPFD') THEN COALESCE(a.USD_IR_KAPPA/10,0) ELSE 0 END) as USD_IR_Kappa, SUM(a.USD_MARKET_VALUE) AS USD_MARKET_VALUE, sum(case when book in ('TDET6', 'TRPFD', 'PRFEQ') and vertical_system='C1_NY' then 0 else coalesce(usd_pv01sprd, 0) end) AS usd_pv01sprd, SUM(coalesce(CASE WHEN book in('SWPIN','TDET6', 'TAIRR', 'TAEMR', 'TEEMR', 'HKFAP', 'TTIRR', 'HKFAM', 'TKFVS', 'HKFAS', 'TBAGR') THEN -usd_notional WHEN (book in('CALLT', 'CALTB', 'LCALT', 'LCATB')) THEN usd_MARKET_VALUE else usd_notional END, 0)) as usd_notional from cdwuser.U_DM_TREASURY a where cob_date in ('2018-02-28', '2018-02-27', '2018-01-31', '2017-12-29', '2017-11-30') and ccc_division = 'TREASURY CAPITAL MARKETS' and LE_GROUP = 'UK' and BOOK not in ('TSYGLRCSH') GROUP BY a.cob_date, case WHEN ACCOUNT IN ('07700A931') THEN 'Defunded Assets' ELSE A.BOOK_STRATEGY END, a.BOOK, a.account, a.CURRENCY_MAPPING, a.CURRENCY_OF_MEASURE, Case WHEN var_excl_fl ='Y' THEN 'Excluded' ELSE 'Included' END union all SELECT A.BOOK_STRATEGY, a.cob_date, a.BOOK, a.ACCOUNT, a.CURRENCY_MAPPING, a.CURRENCY_OF_MEASURE, Case WHEN var_excl_fl ='Y' THEN 'Excluded' ELSE 'Included' END AS Var_Inclusion, 0 as USD_EXPOSURE, 0 as usd_fx, SUM(CASE WHEN book in ('INDMF','MSDEP','TSTPM','PRFEQ','TRPFD') THEN COALESCE(a.USD_IR_KAPPA/10,0) ELSE COALESCE(A.GRID_MEASURE_VALUE/10,0) END) as USD_IR_Kappa, 0 AS USD_MARKET_VALUE, 0 AS usd_pv01sprd, 0 usd_notional from cdwuser.U_DM_TREASURY a where cob_date in ('2018-02-28', '2018-02-27', '2018-01-31', '2017-12-29', '2017-11-30') and ccc_division = 'TREASURY CAPITAL MARKETS' and LE_GROUP = 'UK' and BOOK not in ('TSYGLRCSH') and a.grid_measure_name = 'USD_KAPPA' GROUP BY a.cob_date, a.BOOK_STRATEGY, a.BOOK, a.account, a.CURRENCY_MAPPING, a.CURRENCY_OF_MEASURE, Case WHEN var_excl_fl ='Y' THEN 'Excluded' ELSE 'Included' END