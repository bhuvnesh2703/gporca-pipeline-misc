SELECT SUM (usd_ir_unified_pv01) AS IR_PV01, cob_date, ccc_business_area, a.ccc_division, CASE WHEN a.term_new <= 0.25 THEN '03M' WHEN a.term_new <= 0.5 THEN '06M' WHEN a.term_new <= 1 THEN '01Y' WHEN a.term_new <= 2 THEN '02Y' WHEN a.term_new <= 5 THEN '05Y' ELSE '05Y+' END AS term_bucket, a.POSITION_ULTIMATE_CREDIT_PARTY_DARWIN_NAME, a.TERM_NEW, CASE WHEN PRODUCT_TYPE_CODE IN ('FX') THEN 'FX_LINEAR' WHEN PRODUCT_TYPE_CODE IN ('FXOPT') THEN 'FX_OPTION' ELSE 'OTHER' END AS GroupPrduct FROM cdwuser.U_IR_MSR_INTRPLT a where cob_date in ('2018-02-28') AND a.ccc_division IN ('FIXED INCOME DIVISION') AND usd_ir_unified_pv01 <> 0 AND currency_of_measure IN ('AED') GROUP BY cob_date, ccc_business_area, POSITION_ULTIMATE_CREDIT_PARTY_DARWIN_NAME, a.ccc_division, CASE WHEN a.term_new <= 0.25 THEN '03M' WHEN a.term_new <= 0.5 THEN '06M' WHEN a.term_new <= 1 THEN '01Y' WHEN a.term_new <= 2 THEN '02Y' WHEN a.term_new <= 5 THEN '05Y' ELSE '05Y+' END, a.TERM_NEW, CASE WHEN PRODUCT_TYPE_CODE IN ('FX') THEN 'FX_LINEAR' WHEN PRODUCT_TYPE_CODE IN ('FXOPT') THEN 'FX_OPTION' ELSE 'OTHER' END