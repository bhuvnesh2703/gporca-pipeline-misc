SELECT A.COB_DATE, A.DIVISION_GROUP, CASE WHEN A.CCC_PRODUCT_LINE IN ('DSP INDEX PRODUCTS TRADING','DSP INDEX PRODUCTS') THEN 'CREDIT_CORP' ELSE CR_BREAKDOWN END AS CR_BREAKDOWN, CAST(SUM(A.USD_BPV10) AS NUMERIC (15,5))/1000 AS BPV10 FROM CDWUSER.U_DM_FIRMWIDE A WHERE COB_DATE >= '01/01/2016' AND COB_DATE <= '02/28/2018' AND A.USD_BPV10 IS NOT NULL AND A.CCC_BUSINESS_AREA NOT LIKE 'CPM%' AND A.CCC_BUSINESS_AREA NOT IN ('DERIVS FUNDING') AND A.CCC_PRODUCT_LINE NOT IN ('DISTRESSED TRADING') GROUP BY A.COB_DATE, A.DIVISION_GROUP, CASE WHEN A.CCC_PRODUCT_LINE IN ('DSP INDEX PRODUCTS TRADING','DSP INDEX PRODUCTS') THEN 'CREDIT_CORP' ELSE CR_BREAKDOWN END UNION ALL SELECT A.COB_DATE, A.DIVISION_GROUP, 'CVA' AS CR_BREAKDOWN, CAST(SUM(A.USD_PV10_BENCH) AS NUMERIC(15,5))/1000 AS BPV10 FROM CDWUSER.U_DM_FIRMWIDE A WHERE COB_DATE >= '07/01/2016' AND COB_DATE <= '02/28/2018' AND ( A.CCC_BUSINESS_AREA IN ('CPM', 'CPM TRADING (MPE)', 'CREDIT', 'COMMODS FINANCING') OR CCC_STRATEGY IN ( 'MS CVA MPE - DERIVATIVES', 'MS CVA MNE - DERIVATIVES','FUNDING COLLATERAL COMM', 'CMD LEGACY LOANS & CLAIMS', 'CVA RISK MANAGEMENT', 'NON RES BASE LEND' , 'RES BASE LEND' ) ) GROUP BY A.COB_DATE, A.DIVISION_GROUP