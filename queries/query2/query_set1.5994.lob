SELECT abc.ccc_Product_line, SUM(CM_DELTA) AS CM_DELTA, SUM(CM_Vega) AS CM_Vega FROM ( SELECT a.PRODUCT_SUB_TYPE_CODE, CASE WHEN CCC_DIVISION ='INSTITUTIONAL EQUITY DIVISION' THEN 'EQUITIES' ELSE ccc_Product_line END AS ccc_Product_line, a.TIME_BUCKET_ANNUAL, SUM(COALESCE(a.USD_CM_DELTA,0)) AS CM_Delta, /* raw cm kappa changed for usd cm kappa absolute for BOOK = 'COMXO' to fix incorrect CM Vega risk flowing in for this particular book */ SUM(CASE WHEN CCC_PRODUCT_LINE = 'COMMOD EXOTICS' AND BOOK = 'COMXO' THEN COALESCE(a.USD_CM_KAPPA_ABSOLUTE,0) ELSE COALESCE(a.RAW_CM_KAPPA,0) END) AS CM_Vega FROM cdwuser.U_CM_MSR a WHERE a.cob_date IN ('2018-02-28') AND a.CCC_BUSINESS_AREA NOT IN ('LENDING') AND LE_GROUP = 'UK' AND CCC_DIVISION NOT IN ('FID DVA','FIC DVA') AND CCC_STRATEGY NOT IN ('MS DVA STR NOTES IED') GROUP BY a.PRODUCT_SUB_TYPE_CODE, a.TIME_BUCKET_ANNUAL, CASE WHEN CCC_DIVISION ='INSTITUTIONAL EQUITY DIVISION' THEN 'EQUITIES' ELSE ccc_Product_line END ) abc GROUP BY abc.ccc_Product_line ORDER BY CM_DELTA DESC