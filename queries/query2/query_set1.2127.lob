SELECT COB_DATE, CASE WHEN (CCC_BUSINESS_AREA = 'METALS' and PRODUCT_TYPE_CODE in ('PRECIOUSMETAL', 'FUND')) OR (CCC_PRODUCT_LINE in ('PRECIOUS METALS')) then 'Precious Metals' WHEN CCC_PRODUCT_LINE = 'COMMOD LEGACY TRADING' OR (CCC_BUSINESS_AREA = 'METALS' AND CCC_PRODUCT_LINE IN ('BASE METALS','BULKS'))THEN 'Legacy Trading' END AS CCC_BUSINESS_AREA, SUM ((USD_CM_DELTA)) AS DOLLAR_GREEK FROM CDWUSER.U_CM_MSR a WHERE NOT(INCLUDE_IN_REG_CAAP_FL = 'N' and PRODUCT_SUB_TYPE_CODE in ('N EAST CONSUMPTI', 'N EAST CONSUMPTION', 'N EAST APPALACHI', 'N EAST APPALACHIA') and BOOK = '18003') AND ((CCC_BUSINESS_AREA NOT IN ('CREDIT', 'MS CVA MNE - COMMOD') AND CCC_DIVISION = 'COMMODITIES' ) /*OLD LOGIC*/ OR (A.CCC_DIVISION = 'FIXED INCOME DIVISION' AND A.CCC_BUSINESS_AREA = 'COMMODITIES' and a.CCC_STRATEGY NOT IN ('MS CVA MNE - COMMOD'))) AND /*NEW LOGIC; THERE IS NO CREDIT BUSINESS AREA IN CM ANYMORE CALLED CREDIT*/ PRODUCT_TYPE_CODE NOT IN ('CURRENCY', 'INTEREST RATE', 'INFLATION', 'TBD', 'MISC','CVA', 'FVA', 'ERROR') AND COB_DATE IN ('2018-02-28', '2018-02-27', '2017-12-29', '2017-09-29', '2017-06-30', '2017-03-31', '2016-12-30', '2016-09-30') AND a.CCC_PL_REPORTING_REGION IN ('EMEA') GROUP BY COB_DATE, CCC_BUSINESS_AREA, PRODUCT_TYPE_CODE, CCC_PRODUCT_LINE