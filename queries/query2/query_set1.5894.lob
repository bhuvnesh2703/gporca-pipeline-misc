select T.COB_DATE, T.CURRENCY_OF_POSITION, case when T.PRODUCT_TYPE_CODE='GVTBOND' then 'GVTBOND' else 'NON-GVTBOND' END AS GVTBOND_FLAG, SUM(T.USD_NOTIONAL) AS NOTIONAL, sum(t.usd_ir_unified_pv01) as pv01 from cdwuser.U_EXP_MSR t where cob_date in ('2018-02-28', '2018-02-21', '2018-01-31', '2017-12-29', '2017-11-30', '2017-10-31','2017-09-29','2017-08-31', '2017-12-29') and t.CCC_TAPS_COMPANY IN ('7713','7714','7715') AND T.CCC_DIVISION='TREASURY CAPITAL MARKETS' GROUP BY T.COB_DATE, T.CURRENCY_OF_POSITION, case when T.PRODUCT_TYPE_CODE='GVTBOND' then 'GVTBOND' else 'NON-GVTBOND' END