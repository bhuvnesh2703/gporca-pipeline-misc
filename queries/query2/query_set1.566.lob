SELECT a.COB_DATE, CASE WHEN a.GICS_LEVEL_2_NAME In ('NOT CLASSIFIED','N/A','UNDEFINED','PRIVATE INDIVIDUALS','GOVERNMENT') THEN 'Others' WHEN a.GICS_LEVEL_1_NAME = 'FINANCIALS' THEN GICS_LEVEL_2_NAME ELSE a.GICS_LEVEL_1_NAME END AS GICS_LEVEL_1_NAME, CASE WHEN a.ISSUER_COUNTRY_CODE IN ('ALB','AND','AUT','BLR','BEL','BIH','BGR','CYP','HRV','CZE','DNK','EST','FRO','FIN','FRA','GEO','DEU','GRC','GGY','VAT','HUN','ISL','IRL','IMN','ITA','JEY','LVA','LIE','LTU','LUX','MKD','MLT','MDA','MCO','MNE','NLD','NOR','POL','PRT','ROU','RUS','SMR','SRB','SVK','SVN','ESP','SJM','SWE','CHE','UKR') THEN 'Europe' WHEN a.ISSUER_COUNTRY_CODE IN ('GBR') THEN 'UK' WHEN a.ISSUER_COUNTRY_CODE IN ('USA','CAN') THEN 'NAM' ELSE 'Other' END AS FLAG_REGION, a.ISSUER_COUNTRY_CODE, a.POSITION_ULT_ISSUER_PARTY_DARWIN_NAME, sum(coalesce(a.USD_PV10_BENCH,0)) as USD_PV10_BENCH FROM cdwuser.U_EXP_MSR a WHERE a.COB_DATE in ('2018-02-28','2018-02-21') AND a.CCC_PL_REPORTING_REGION = 'EMEA' AND a.CCC_BUSINESS_AREA = 'CREDIT-CORPORATES' AND a.CCC_BANKING_TRADING = 'TRADING' AND a.CCC_PRODUCT_LINE NOT IN ('DISTRESSED TRADING','PRIMARY - LOANS','PAR LOANS TRADING') AND a.FID1_SENIORITY NOT IN ('SUBT1','SUBUT2','AT1') GROUP BY a.COB_DATE, CASE WHEN a.GICS_LEVEL_2_NAME In ('NOT CLASSIFIED','N/A','UNDEFINED','PRIVATE INDIVIDUALS','GOVERNMENT') THEN 'Others' WHEN a.GICS_LEVEL_1_NAME = 'FINANCIALS' THEN GICS_LEVEL_2_NAME ELSE a.GICS_LEVEL_1_NAME END, a.ISSUER_COUNTRY_CODE, a.POSITION_ULT_ISSUER_PARTY_DARWIN_NAME