SELECT 'greek' AS type ,cob_date ,condition_name ,Sum(scenario_pnl) AS pnl FROM dwuser.u_modular_scenarios_ccar where COB_DATE = '2018-02-28'  AND attribution NOT LIKE '%GAMMA%'  AND stress_scenario IN ( 'FRB2017_SA' ) AND risk_system NOT LIKE '%SILO' AND ( CCAR_BUSINESS_CATEGORY NOT LIKE 'NOT%' OR CCC_BUSINESS_AREA = 'GLOBAL EQUITY ADMIN & DEV' ) AND scenario_type = 'GREEK' AND ccc_business_area IN ( 'LIQUID FLOW RATES' ,'STRUCTURED RATES' )  GROUP BY cob_date ,condition_name  union all  SELECT 'greek' AS type ,cob_date ,condition_name ,Sum(scenario_pnl) AS pnl FROM dwuser.u_modular_scenarios_ccar where COB_DATE = '2018-01-31'  AND attribution NOT LIKE '%GAMMA%'  AND stress_scenario IN ( 'FRB2017_SA' ) AND risk_system NOT LIKE '%SILO' AND ( CCAR_BUSINESS_CATEGORY NOT LIKE 'NOT%' OR CCC_BUSINESS_AREA = 'GLOBAL EQUITY ADMIN & DEV' ) AND scenario_type = 'GREEK' AND ccc_business_area IN ( 'LIQUID FLOW RATES' ,'STRUCTURED RATES' )  GROUP BY cob_date ,condition_name