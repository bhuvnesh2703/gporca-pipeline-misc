select      cob_date,     SCENARIO_NAME,     ccc_business_area,     ccc_product_line, CCC_PL_REPORTING_REGION,     sum(POS_TV_MINUS_BASE) as scenario_pnl from CDWUSER.u_ied_surface s WHERE s.cob_date in ('2018-02-28','2018-02-21') and     s.CCC_DIVISION IN ('INSTITUTIONAL EQUITY DIVISION') AND     s.CCC_BANKING_TRADING IN ('TRADING') AND     s.SILO_SRC = 'IED' AND     s.SCENARIO_NAME IN      ('S-30%','V+.1','S-30%/V+.1')  group by      cob_date,     SCENARIO_NAME,     ccc_business_area,     ccc_product_line, CCC_PL_REPORTING_REGION