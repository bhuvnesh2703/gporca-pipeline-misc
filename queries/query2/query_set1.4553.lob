SELECT SUM(COALESCE(USD_PV10_BENCH,0)) AS USD_PV10_BENCH FROM CDWUSER.U_DM_FIRMWIDE A WHERE COB_DATE = '02/28/2018' AND A.VERTICAL_SYSTEM <> 'PIPELINE_NY' AND A.VAR_EXCL_FL <> 'Y' AND A.ISSUER_COUNTRY_TIER= '1' AND A.CREDIT_TYPE_CD = 'EXCLUDED' AND A.DIVISION_GROUP = 'ISG CORE' AND ( A.CCC_BUSINESS_AREA = 'LENDING' OR CCC_STRATEGY IN ( 'INVESTMENT GRADE PRIMARY1','NON IG PRIMARY - HY BOND1', 'INVESTMENT GRADE PRIMARY','NON IG PRIMARY - HY BOND' ) ) AND A.COUNTRY_OF_RISK_EM_FLAG = 'NONEM'