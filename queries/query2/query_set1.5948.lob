SELECT X.COB_DATE, x.BOOK, case when X.BOOK_STRATEGY in ('Parent', 'Non Bank Subsidiary', 'Non-US Bank Subsidiaries', 'MSAIL Assets', 'MSHK Assets', 'Other Securities', 'Spec Financing ISG', 'Inter Company Financing', 'Reval Hedges', 'CTA Hedges', 'Funding Swaps', 'ASC 815 Debt', 'ASC 815 Hedges', 'Non-ASC 815 Hedges', 'IR Basis Swaps', 'MSAIL OIS', 'Cross-Currency Swaps') then 'MTM Risk' else 'Non-MTM' end as MTM_Flag, case when X.BOOK_STRATEGY in ('Parent', 'Non Bank Subsidiary', 'Non-US Bank Subsidiaries', 'MSAIL Assets', 'MSHK Assets', 'Other Securities', 'Spec Financing ISG', 'Inter Company Financing', 'Reval Hedges', 'CTA Hedges', 'Funding Swaps', 'ASC 815 Debt', 'ASC 815 Hedges', 'Non-ASC 815 Hedges', 'IR Basis Swaps', 'MSAIL OIS', 'Cross-Currency Swaps') or x.book in ('TSTPM') then 'Capital Risk' else 'Non-Capital' end as Capital_Flag, X.BUCKETED_TERM, SUM(X.usd_pv01) AS usd_PV01 FROM (SELECT BOOK_STRATEGY, BUCKETED_TERM, a.BOOK, a.COB_DATE, sum(usd_ir_unified_pv01) as usd_pv01 FROM Cdwuser.U_DM_TREASURY A where cob_date in ('2018-02-28') and ccc_division = 'TREASURY CAPITAL MARKETS' and BOOK not in ('TSYGLRCSH') and var_excl_fl <>'Y' group by BOOK_STRATEGY, BUCKETED_TERM, a.BOOK, a.COB_DATE )x GROUP BY x.book, X.COB_DATE, case when X.BOOK_STRATEGY in ('Parent', 'Non Bank Subsidiary', 'Non-US Bank Subsidiaries', 'MSAIL Assets', 'MSHK Assets', 'Other Securities', 'Spec Financing ISG', 'Inter Company Financing', 'Reval Hedges', 'CTA Hedges', 'Funding Swaps', 'ASC 815 Debt', 'ASC 815 Hedges', 'Non-ASC 815 Hedges', 'IR Basis Swaps', 'MSAIL OIS', 'Cross-Currency Swaps') then 'MTM Risk' else 'Non-MTM' end, case when X.BOOK_STRATEGY in ('Parent', 'Non Bank Subsidiary', 'Non-US Bank Subsidiaries', 'MSAIL Assets', 'MSHK Assets', 'Other Securities', 'Spec Financing ISG', 'Inter Company Financing', 'Reval Hedges', 'CTA Hedges', 'Funding Swaps', 'ASC 815 Debt', 'ASC 815 Hedges', 'Non-ASC 815 Hedges', 'IR Basis Swaps', 'MSAIL OIS', 'Cross-Currency Swaps') or x.book in ('TSTPM') then 'Capital Risk' else 'Non-Capital' end, X.BUCKETED_TERM