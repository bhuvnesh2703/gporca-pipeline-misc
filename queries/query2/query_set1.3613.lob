SELECT COB_DATE, CASE WHEN TERM_NEW IN (3) THEN 5 WHEN TERM_NEW IN (7, 8) THEN 10 WHEN TERM_NEW IN (12) THEN 15 WHEN TERM_NEW IN (30, 40 , 50, 60, 75) THEN 30 ELSE TERM_NEW END AS TERM_NEW, CCC_PRODUCT_LINE, CCC_STRATEGY, SUM (USD_IR_UNIFIED_PV01) AS usd_pv01, CURRENCY_OF_MEASURE, CASE WHEN CURRENCY_OF_MEASURE IN ('USD', 'EUR', 'GBP', 'JPY', 'PLN', 'SEK') THEN CURRENCY_OF_MEASURE ELSE 'Others' END AS CURRENCY_OF_MEASURE1, CASE WHEN term_new < 5 THEN '0-5Y' WHEN term_new >= 5 AND term_new < 10 THEN '5-10Y' ELSE '10Y+' END AS term_bucket, CASE WHEN (a.CCC_BUSINESS_AREA IN ('CPM TRADING (MPE)', 'CPM', 'CREDIT', 'MS CVA MNE - FID', 'MS CVA MNE - COMMOD', 'COMMODS FINANCING') OR a.CCC_STRATEGY IN ('MS CVA MPE - DERIVATIVES', 'MS CVA MNE - DERIVATIVES')) THEN 'CVA' ELSE a.CCC_DIVISION END AS CCC_DIVISION, a.IS_UK_GROUP, CASE WHEN a.CCC_PL_REPORTING_REGION = 'EMEA' THEN 'EMEA' WHEN a. CCC_PL_REPORTING_REGION = 'AMERICAS' THEN 'AMERICAS' WHEN a.CCC_PL_REPORTING_REGION IN ('ASIA PACIFIC', 'JAPAN') THEN 'ASIA' ELSE 'OTHER' END AS CCC_PL_REPORTING_REGION, CASE WHEN (A.CCC_DIVISION = 'INSTITUTIONAL EQUITY DIVISION' AND A.CCC_BUSINESS_AREA NOT IN ('CASH EQUITIES', 'DERIVATIVES', 'OTHER IED', 'PRIME BROKERAGE')) THEN 'OTHERS' WHEN (A.CCC_DIVISION = 'FIXED INCOME DIVISION' AND A .CCC_BUSINESS_AREA NOT IN ('CREDIT-CORPORATES', 'FXEM MACRO TRADING', 'EM CREDIT TRADING', 'LIQUID FLOW RATES', 'STRUCTURED RATES', 'SECURITIZED PRODUCTS GRP')) THEN 'OTHERS FID' ELSE A.CCC_BUSINESS_AREA END AS CCC_BUSINESS_AREA, CASE WHEN a.CCC_TAPS_COMPANY = '0319' THEN 'MSIM' WHEN a.CCC_TAPS_COMPANY IN ('4663', '7281', '5274', '5254', '8179', '7280', '6262', '1311', '8292', '0721', '4391', '0517') THEN 'MSBIL' WHEN a.CCC_TAPS_COMPANY = '0302' THEN 'MSIP' WHEN a.IS_UK_GROUP = 'Y' THEN 'OtherUKG' ELSE 'NOTUKG' END AS entityclassification FROM CDWUSER.U_DM_IR a WHERE cob_date in ('2018-02-28') GROUP BY COB_DATE, CASE WHEN term_new < 5 THEN '0-5Y' WHEN term_new >= 5 AND term_new < 10 THEN '5-10Y' ELSE '10Y+' END, CURRENCY_OF_MEASURE, CASE WHEN (a.CCC_BUSINESS_AREA IN ('CPM TRADING (MPE)', 'CPM', 'CREDIT', 'MS CVA MNE - FID', 'MS CVA MNE - COMMOD', 'COMMODS FINANCING') OR a.CCC_STRATEGY IN ('MS CVA MPE - DERIVATIVES', 'MS CVA MNE - DERIVATIVES')) THEN 'CVA' ELSE a.CCC_DIVISION END, a.IS_UK_GROUP, CASE WHEN a.CCC_PL_REPORTING_REGION = 'EMEA' THEN 'EMEA' WHEN a.CCC_PL_REPORTING_REGION = 'AMERICAS' THEN 'AMERICAS' WHEN a.CCC_PL_REPORTING_REGION IN ('ASIA PACIFIC', 'JAPAN') THEN 'ASIA' ELSE 'OTHER' END, CASE WHEN (A.CCC_DIVISION = 'INSTITUTIONAL EQUITY DIVISION' AND A.CCC_BUSINESS_AREA NOT IN ('CASH EQUITIES', 'DERIVATIVES', 'OTHER IED', 'PRIME BROKERAGE')) THEN 'OTHERS' WHEN (A.CCC_DIVISION = 'FIXED INCOME DIVISION' AND A.CCC_BUSINESS_AREA NOT IN ('CREDIT-CORPORATES', 'FXEM MACRO TRADING', 'EM CREDIT TRADING', 'LIQUID FLOW RATES', 'STRUCTURED RATES', 'SECURITIZED PRODUCTS GRP')) THEN 'OTHERS FID' ELSE A.CCC_BUSINESS_AREA END, CASE WHEN a.CCC_TAPS_COMPANY = '0319' THEN 'MSIM' WHEN a.CCC_TAPS_COMPANY IN ('4663', '7281', '5274', '5254', '8179', '7280', '6262', '1311', '8292', '0721', '4391', '0517') THEN 'MSBIL' WHEN a.CCC_TAPS_COMPANY = '0302' THEN 'MSIP' WHEN a.IS_UK_GROUP = 'Y' THEN 'OtherUKG' ELSE 'NOTUKG' END, TERM_NEW, CCC_PRODUCT_LINE, CCC_STRATEGY