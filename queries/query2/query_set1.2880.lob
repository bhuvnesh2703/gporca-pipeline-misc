SELECT a.COB_DATE, a.CCC_PL_REPORTING_REGION, a.CCC_BUSINESS_AREA, a.CCC_STRATEGY, SUM (a.USD_DELTA) AS USD_DELTA, SUM (a.USD_EQ_GAMMA) AS USD_EQ_GAMMA, SUM (a.USD_THETA_GAMMA) AS USD_THETA_GAMMA, SUM (a.SLIDE_EQ_MIN_05_USD) AS SLIDE_EQ_MIN_05_USD, SUM (a.SLIDE_EQ_MIN_20_USD) AS SLIDE_EQ_MIN_20_USD, SUM (a.SLIDE_EQ_MIN_05_USD) + SUM(a.USD_DELTA) * 0.05 AS DELTAADJ_SLIDE_EQ_MIN_05_USD, SUM (a.SLIDE_EQ_MIN_20_USD) + SUM(a.USD_DELTA) * 0.2 AS DELTAADJ_SLIDE_EQ_MIN_20_USD, SUM (a.SLIDE_KAPPA_MIN_20_USD) AS SLIDE_KAPPA_MIN_20_USD, SUM (a.SLIDE_KAPPA_PLS_20_USD) AS SLIDE_KAPPA_PLS_20_USD, SUM (a.CORR_EQEQ)/1000000 AS CORR_EQEQ, SUM (a.USD_EQ_KAPPA_PLS100BP_TIMEADJ) AS USD_TA_KAPPA, SUM (a.USD_EQ_KAPPA) AS USD_KAPPA FROM CDWUSER.U_DM_EQ a WHERE a.COB_DATE in ('2018-02-28','2018-02-21') AND a.CCC_DIVISION = 'INSTITUTIONAL EQUITY DIVISION' AND a.CCC_BANKING_TRADING = 'TRADING' GROUP BY a.COB_DATE, a.CCC_PL_REPORTING_REGION, a.CCC_BUSINESS_AREA, a.CCC_STRATEGY