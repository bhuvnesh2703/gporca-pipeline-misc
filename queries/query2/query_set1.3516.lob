SELECT a.COB_DATE, a.CURVE_NAME, a.CURRENCY_OF_MEASURE, a.PRODUCT_TYPE_CODE, a.COUNTRY_CD_OF_RISK, a.CCC_PRODUCT_LINE, a.BOOK, a.CCC_BUSINESS_AREA, sum(coalesce( a.USD_IRPV01SPRD_DERIVED,0)) as USD_IRPV01SPRD_DERIVED, a.CCC_STRATEGY FROM CDWUSER.U_DM_IR a WHERE a.CCC_BUSINESS_AREA in ('LIQUID FLOW RATES','STRUCTURED RATES') AND a.COB_DATE >= '2017-09-29' AND a.CURVE_NAME in ('fxusdeur','fxusdgbp','eurnw_6m','gbpnw_6m') AND a.CCC_PL_REPORTING_REGION = 'EMEA' GROUP BY a.COB_DATE, a.CURVE_NAME, a.CURRENCY_OF_MEASURE, a.PRODUCT_TYPE_CODE, a.COUNTRY_CD_OF_RISK, a.CCC_PRODUCT_LINE, a.BOOK, a.CCC_BUSINESS_AREA, a.CCC_STRATEGY