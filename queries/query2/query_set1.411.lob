SELECT A.COB_DATE, CASE WHEN A.FID1_INDUSTRY_NAME_LEVEL1 = 'SOVEREIGN' THEN 'SOV' ELSE 'NON_SOV' END AS SOV_FLAG, CASE WHEN (a.VERTICAL_SYSTEM LIKE '%EQUITY%' AND a.PRODUCT_TYPE_CODE NOT IN ('GVTBOND',  'RATEFUT')) THEN 'OTHER EQ' ELSE 'OTHER FID' END AS CATEGORY, CASE WHEN A.PRODUCT_TYPE_CODE IN ('GVTBOND' ) THEN ' Government Bonds' WHEN A.PRODUCT_TYPE_CODE LIKE '%RATEFUT%' THEN 'Rate Futures' ELSE 'OTHERS' END AS PRODUCT_TYPE_CODE, SUM (A.USD_PV10_BENCH) AS BENCH_PV10, SUM (A.USD_PV01SPRD) AS SPRD_PV01 FROM CDWUSER.U_EXP_MSR A WHERE  COB_DATE='2018-02-28' AND A.ISSUER_COUNTRY_CODE = 'BRA' AND CCC_BUSINESS_AREA NOT LIKE '%CPM%' AND (UPPER (a.POSITION_ULT_ISSUER_PARTY_DARWIN_NAME) NOT LIKE '%MORGAN STANLEY%') AND A.CCC_BANKING_TRADING_LOCALREG = 'TRADING' AND A.CCC_DIVISION IN ('INSTITUTIONAL EQUITY DIVISION') GROUP BY A.COB_DATE, CASE WHEN (a.VERTICAL_SYSTEM LIKE '%EQUITY%' AND a.PRODUCT_TYPE_CODE NOT IN ('GVTBOND' , 'RATEFUT')) THEN 'OTHER EQ' ELSE 'OTHER FID' END, CASE WHEN A.PRODUCT_TYPE_CODE IN ('GVTBOND' ) THEN ' Government Bonds' WHEN A.PRODUCT_TYPE_CODE LIKE '%RATEFUT%' THEN 'Rate Futures' ELSE 'OTHERS' END, CASE WHEN A.FID1_INDUSTRY_NAME_LEVEL1 = 'SOVEREIGN' THEN 'SOV' ELSE 'NON_SOV' END