SELECT a.COB_DATE, a.BOOK_STRATEGY, a.BOOK, a.CURRENCY_MAPPING, a.CURRENCY_OF_MEASURE, SUM(coalesce(CASE WHEN book in('SWPIN','TDET6', 'TAIRR', 'TAEMR', 'TEEMR', 'HKFAP', 'TTIRR', 'HKFAM', 'TKFVS', 'HKFAS', 'TBAGR') THEN -usd_notional WHEN (book in('CALLT', 'CALTB', 'LCALT', 'LCATB')) THEN usd_MARKET_VALUE else usd_notional END, 0)) as usd_notional from cdwuser.U_DM_TREASURY a where cob_date in ('2018-02-28', '2018-02-21', '2018-01-31', '2017-12-29', '2017-11-30') and a.CCC_DIVISION = 'TREASURY CAPITAL MARKETS' and a.BOOK not in ('TSYGLRCSH') GROUP BY a.COB_DATE, a.BOOK_STRATEGY, a.BOOK, a.CURRENCY_MAPPING, a.CURRENCY_OF_MEASURE