select distinct case when a.LIMIT_NAME in ('MARK-TO-MARKET IR PV01', 'IR PV01 Treasury MTM') then 'IR PV01 Treasury MTM' else a.LIMIT_NAME end as LIMIT_NAME, case when a.TEMP_LIMIT_EXPIRATION_DATE >= a.COB_DATE then a.TEMP_LIMIT_VALUE else a.LIMIT_VALUE end as LIMIT_VALUE, a.LIMIT_APPLIED_TO from cdwuser.U_FLOW_LIMITS a where a.COB_DATE = '2/21/2018' and a.LIMIT_ID in ( '6149', '7801', '6150', '7802', '7803', '7804', '7806' ) and a.PARENT_DESCRIPTION = 'FIRM TREASURY'