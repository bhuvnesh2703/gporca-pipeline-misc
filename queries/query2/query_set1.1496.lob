SELECT COB_DATE, SUM(JET) AS JET, SUM(CRUDE) AS CRUDE, SUM(OTHER) AS OTHER FROM( SELECT COB_DATE, PROD_POS_NAME_DESCRIPTION, PRODUCT_TYPE_CODE, CASE WHEN PRODUCT_TYPE_CODE = 'JET' THEN SUM(RAW_CM_KAPPA) ELSE 0 END AS JET, CASE WHEN PRODUCT_TYPE_CODE = 'CRUDE' THEN SUM(RAW_CM_KAPPA) ELSE 0 END AS CRUDE, CASE WHEN PRODUCT_TYPE_CODE NOT IN ('JET', 'CRUDE') THEN SUM(RAW_CM_KAPPA) ELSE 0 END AS OTHER FROM CDWUSER.U_CM_MSR WHERE COB_DATE BETWEEN ('2018-01-29') AND ('2018-02-28') AND (CCC_BUSINESS_AREA IN ('OIL LIQUIDS', 'TMG', 'OLYMPUS') OR (CCC_PRODUCT_LINE = 'OIL & PRODUCTS' AND CCC_STRATEGY = 'EMEA OIL & PRODUCTS')) AND (CCC_DIVISION='COMMODITIES' OR (CCC_DIVISION = 'FIXED INCOME DIVISION' AND CCC_BUSINESS_AREA = 'COMMODITIES')) AND /*AND CCC_TRD_BOOK <> 'DEVELOPMENT'*/ CCC_PL_REPORTING_REGION = 'EMEA' GROUP BY COB_DATE, PROD_POS_NAME_DESCRIPTION, PRODUCT_TYPE_CODE, CASE WHEN PRODUCT_TYPE_CODE = 'JET' THEN RAW_CM_KAPPA ELSE 0 END, CASE WHEN PRODUCT_TYPE_CODE = 'CRUDE' THEN RAW_CM_KAPPA ELSE 0 END, CASE WHEN PRODUCT_TYPE_CODE NOT IN ('JET','CRUDE') THEN RAW_CM_KAPPA ELSE 0 END ) X GROUP BY COB_DATE