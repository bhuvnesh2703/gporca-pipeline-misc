select a.COB_DATE, a.CURRENCY_OF_MEASURE, a.TERM_NEW, a.CCC_LE_GROUP_BTI, a.CCC_BUSINESS_AREA, a.CCC_DIVISION, a.CCC_PRODUCT_LINE, sum (a.USD_IRPV01SPRD) as SPV01, a.CURVE_NAME, CASE WHEN CURRENCY_OF_MEASURE IN ('EUR','GBP','JPY','USD') THEN CURRENCY_OF_MEASURE ELSE 'OTHER' END AS CCY, CASE WHEN CURVE_NAME IN ('eurnw_3m','eurnw_disc') THEN 'EUR_3M-OIS' WHEN CURVE_NAME IN ('eurnw_1m') THEN 'EUR_3M-1M' WHEN CURVE_NAME IN ('gbpnw_3m') THEN 'GBP_3M-OIS' WHEN CURVE_NAME IN ('gbpnw_1m') THEN 'GBP_3M-1M' WHEN CURVE_NAME IN ('usdn_3m','usdnw_3m','usdn_disc') THEN 'USD_3M-OIS' WHEN CURVE_NAME IN ('usdlibor1m','usdn_1m','usdnw_1m') THEN 'USD_3M-1M' ELSE 'OTHER' END AS CURVE from CDWUSER.U_IR_MSR_INTRPLT a WHERE a.cob_date in ('2018-02-28','2018-02-27') AND LE_GROUP = 'UK' AND a.BOOK NOT IN ('SECTD') AND CCC_STRATEGY NOT IN ('CVA RISK MANAGEMENT','FVA RISK MANAGEMENT','CPM - OTHER','CPM CREDIT', 'CPM FUNDING','MS CVA MNE - DERIVATIVES','MS CVA MPE - DERIVATIVES') group by a.COB_DATE, a.CURRENCY_OF_MEASURE, a.TERM_NEW, a.CCC_LE_GROUP_BTI, a.CCC_BUSINESS_AREA, a.CCC_DIVISION, a.CCC_PRODUCT_LINE, a.CURVE_NAME