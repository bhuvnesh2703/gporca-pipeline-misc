SELECT SUM(USD_IRPV01SPRD) AS SPV01,SUM(USD_IR_UNIFIED_PV01) as pv01 , TERM_BUCKET, COB_DATE, curve_name, CURRENCY_OF_MEASURE, CCC_BUSINESS_AREA, vertical_system, CASE WHEN (a.VERTICAL_SYSTEM LIKE '%EQUITY%' AND a.PRODUCT_TYPE_CODE NOT IN ('GVTBOND',  'RATEFUT')) THEN 'OTHER EQ' ELSE 'OTHER FID' END AS CATEGORY, CASE WHEN A.PRODUCT_TYPE_CODE IN ('GVTBOND' ) THEN ' Government Bonds' WHEN A.PRODUCT_TYPE_CODE LIKE '%RATEFUT%' THEN 'Rate Futures' ELSE 'OTHERS' END AS PRODUCT_TYPE_CODE FROM CDWUSER.U_EXP_MSR a where COB_DATE='2018-02-28' AND A.CCC_DIVISION='INSTITUTIONAL EQUITY DIVISION' AND CURRENCY_OF_MEASURE like '%BR%' AND NOT a.PRODUCT_DESCRIPTION LIKE '%BLFT%' AND CURVE_NAME  IN ('brlnw_spd','fxusdbr1') AND CCC_BUSINESS_AREA NOT LIKE '%CPM%' GROUP BY COB_DATE,TERM_bucket, curve_name,CURRENCY_OF_MEASURE,CCC_BUSINESS_AREA,vertical_system, CASE WHEN (a.VERTICAL_SYSTEM LIKE '%EQUITY%' AND a.PRODUCT_TYPE_CODE NOT IN ('GVTBOND',  'RATEFUT')) THEN 'OTHER EQ' ELSE 'OTHER FID' END, CASE WHEN A.PRODUCT_TYPE_CODE IN ('GVTBOND' ) THEN ' Government Bonds' WHEN A.PRODUCT_TYPE_CODE LIKE '%RATEFUT%' THEN 'Rate Futures' ELSE 'OTHERS' END