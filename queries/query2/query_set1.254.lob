SELECT     a.COB_DATE,     a.CURVE_CURRENCY,     SUM (COALESCE (CAST(a.USD_CONVX AS DOUBLE PRECISION),0) ) AS usd_ir_gamma FROM cdwuser.U_IR_MSR_INTRPLT a WHERE (a.COB_DATE = '2018-02-28' or a.COB_DATE = '2018-02-27') and A.CCC_TAPS_COMPANY in ('4391', '4341') AND      (a.VAR_EXCL_FL <> 'Y') AND     a.CCC_DIVISION IN ('FIXED INCOME DIVISION') AND     /*a.CCC_BUSINESS_AREA NOT IN ('COMMODITIES') AND*/       CCC_BANKING_TRADING='TRADING' GROUP BY     a.COB_DATE,     a.CURVE_CURRENCY