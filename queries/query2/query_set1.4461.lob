SELECT CASE WHEN CCC_PRODUCT_LINE = 'EU GOVERNMENTS' THEN 'EU GOV' END AS EU_GOV_FLAG, COB_DATE, COUNTRY_CD_OF_RISK, CCC_PRODUCT_LINE, TERM_NEW, currency_code AS CURRENCY_CODE, CASE WHEN TERM_NEW < 1.5 THEN '0-1' WHEN TERM_NEW > 1.5 AND TERM_NEW <= 6 THEN '2-5' WHEN TERM_NEW > 6 AND TERM_NEW <= 13.5 THEN '7-12' WHEN TERM_NEW > 13.5 AND TERM_NEW <= 30 THEN '15-30' WHEN TERM_NEW > 30 AND TERM_NEW <= 40 THEN '40' ELSE '50' END AS TERM_BUCKET_NEW, SUM(COALESCE(USD_IR_UNIFIED_PV01,0)) AS PV01, SUM(USD_PV01SPRD) AS PV01_SPREAD, SUM(USD_EXPOSURE) AS NET_EXP FROM CDWUSER.U_EXP_TRENDS A WHERE COB_DATE IN ('2018-02-28','2018-02-21') AND PARENT_LEGAL_ENTITY = '0517(G)' AND CCC_BANKING_TRADING = 'TRADING' AND TERM_BUCKET <> '-0' AND CCC_DIVISION = 'FIXED INCOME DIVISION' GROUP BY EU_GOV_FLAG, COB_DATE, COUNTRY_CD_OF_RISK, CCC_PRODUCT_LINE, TERM_NEW, currency_code, TERM_BUCKET_NEW