SELECT STDDEV(ABS(DAILY_CHANGE)) as STDDEV from ( select raw.cob_date, sum(raw.raw_pnl)-LEAD(sum(raw.raw_pnl),1) OVER (ORDER BY COB_DATE DESC) as DAILY_CHANGE from cdwuser.u_raw_scenario_pnl raw where raw.cob_date >= ('2017-03-31') and raw.cob_date <> ('2017-07-21') and raw.bu_risk_system LIKE 'C1_SCENARIOS_EVE_NY' and upper(raw.source_scn_name) ='PARALLEL IR 1.0 BPS' and raw.book in ('TDEBT', 'TDCLH') group by raw.cob_date ) a