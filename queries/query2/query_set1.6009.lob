SELECT SUM(COALESCE(a.SLIDE_EQ_MIN_30_USD,0)) AS D30, SUM(COALESCE(a.SLIDE_EQ_MIN_20_USD,0)) AS D20, SUM(COALESCE(a.SLIDE_EQ_MIN_10_USD,0)) AS D10, SUM(COALESCE(a.SLIDE_EQ_MIN_05_USD,0)) AS D05, SUM(COALESCE(a.SLIDE_EQ_PLS_05_USD,0)) AS P05, SUM(COALESCE(a.SLIDE_EQ_PLS_10_USD,0)) AS P10, SUM(COALESCE(a.SLIDE_EQ_PLS_20_USD,0)) AS P20 FROM cdwuser.U_EQ_MSR a WHERE a.cob_date IN ('2018-02-28') AND a.LE_GROUP = 'UK' AND a.CCC_BUSINESS_AREA NOT IN ('LENDING') AND CCC_DIVISION NOT IN ('FID DVA','FIC DVA') AND CCC_STRATEGY NOT IN ('MS DVA STR NOTES IED')