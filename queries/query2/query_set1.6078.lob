SELECT      CUR.COB_DATE,     CUR.RANK || '_' || CUR.RANK_NAME AS CONCENTRATION,     CUR.PRODUCT_DESCRIPTION,     CUR.TICK,     CUR.KAPPA AS KAPPA_COB,     PREV.KAPPA AS KAPPA_COMP,     CUR.KAPPA - PREV.KAPPA AS KAPPA_DOD,     CUR.DESK FROM CDWUSER.U_DM_CONC CUR        LEFT OUTER JOIN CDWUSER.U_DM_CONC PREV            ON CUR.RANK_TYPE = PREV.RANK_TYPE                     AND CUR.RANK_NAME = PREV.RANK_NAME                     AND CUR.PRODUCT_DESCRIPTION = PREV.PRODUCT_DESCRIPTION                     AND CUR.TICK = PREV.TICK                     AND PREV.RANK_TYPE = 'VEGA' AND (PREV.COB_DATE = '2018-02-27')   WHERE CUR.RANK_TYPE = 'VEGA' AND  CUR.RANK_NAME in ('TopUKGroupVegaStock','TopUKGroupVega','TopEuVegaStock','TopEuVega')   AND (CUR.COB_DATE = '2018-02-28')            AND CUR.RANK <= 10 ORDER BY CUR.RANK_NAME,CUR.RANK,CUR.COB_DATE