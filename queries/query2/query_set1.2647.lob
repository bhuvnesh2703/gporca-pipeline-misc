SELECT a.COB_DATE,a.CCC_PL_REPORTING_REGION, sum(case when ( account like '07%' AND a.POSITION_ULT_ISSUER_PARTY_DARWIN_NAME like '%MORGAN STANLEY%') then A.USD_PV01SPRD else 0 end) as SP01 FROM CDWUSER.U_EXP_MSR A WHERE a.COB_DATE > '2017-02-28' AND a.COB_DATE <= '2018-02-28' and (a.CCC_DIVISION='FID DVA' OR a.CCC_DIVISION='FIC DVA' OR A.CCC_STRATEGY='MS DVA STR NOTES IED') Group by a.COB_DATE,a.CCC_PL_REPORTING_REGION