select a.COB_DATE, a.CURRENCY_OF_MEASURE, a.CURRENCY_MAPPING, a.BOOK, a.BUCKETED_TERM, sum(coalesce(USD_IR_UNIFIED_PV01,0)) as USD_IR_UNIFIED_PV01 from cdwuser.U_DM_TREASURY a where CCC_DIVISION = 'TREASURY CAPITAL MARKETS' and cob_date in ('2018-02-28', '2018-02-27') and CCC_BUSINESS_AREA in ('TSY DEBT') and CCC_STRATEGY not in ('STRUCTURED N', 'BANK LOANS', 'COMMERCIAL PAPER TSY', 'L-TERM DEBT SN', 'L-TERM DEBT SN ALLOC') and book not in ('AEOEQ','LIABB','TRPFD','TDET2','TDET4','TDET5','TDET6','TDET7','TDET8','PRFEQ', 'TSYGLRCSH', 'CALTB', 'CALLT', 'LCALT', 'LCATB') group by a.COB_DATE, a.CURRENCY_OF_MEASURE, a.CURRENCY_MAPPING, a.BOOK, a.BUCKETED_TERM