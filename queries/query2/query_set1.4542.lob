SELECT COB_DATE, DIVISION_GROUP, A.HYT2_CATEGORY, CASE WHEN A.CREDIT_TYPE_CD = 'CREDIT-HY_T1T2' OR A.CCC_PRODUCT_LINE = 'DISTRESSED TRADING' THEN 'HIGH_YIELD' ELSE NULL END AS CATEGORY, CASE WHEN A.CCC_PRODUCT_LINE = 'DISTRESSED TRADING' AND A.PRODUCT_TYPE_CODE IN ( 'EQUITY','STOCK','ADR','SWAP','WARRANT','WARRNT' ) THEN 'EQUITY_HEDGE' WHEN A.CCC_PRODUCT_LINE = 'DISTRESSED TRADING' AND A.PRODUCT_TYPE_CODE IN ( 'GVTBOND','ETF','OPTION','CRDINDEX','CDSOPTIDX','FUTURE' ) THEN 'OTHER_HEDGE' ELSE 'NOT_HEDGE' END AS HEDGE, CASE WHEN CCC_BUSINESS_AREA IN ('FXEM MACRO TRADING', 'EM CREDIT TRADING') THEN 'FXEM' ELSE 'NON-FXEM' END AS IS_FXEM, CASE WHEN CCC_BUSINESS_AREA IN ( 'CREDIT DERIVATIVE PROD', 'DSP - CREDIT' )THEN 'CDP' ELSE 'NON_CDP' END AS IS_CDP, CASE WHEN CCC_PRODUCT_LINE IN ('DISTRESSED TRADING') AND CCC_STRATEGY NOT IN ('PAR LOANS TRADING') THEN CCC_PRODUCT_LINE WHEN CCC_PRODUCT_LINE IN ('PAR LOANS TRADING') OR CCC_STRATEGY IN ('PAR LOANS TRADING') THEN 'PAR LOANS TRADING' WHEN CCC_BUSINESS_AREA = 'CREDIT-CORPORATES' AND CCC_PRODUCT_LINE NOT IN ('DISTRESSED TRADING', 'PAR LOANS TRADING') AND CCC_STRATEGY <> 'PAR LOANS TRADING' THEN 'OTHER_CORPORATES' WHEN CCC_BUSINESS_AREA IN ( 'SECURITIZED PRODUCTS GRP', 'PROP WORKOUT', 'LEVERAGED LOANS', 'RESIDENTIAL' ) THEN CCC_BUSINESS_AREA WHEN DIVISION_GROUP = 'ISG NON CORE' AND CCC_BUSINESS_AREA NOT IN ('PROP WORKOUT', 'LEVERAGED LOANS', 'RESIDENTIAL') THEN 'OTHER_NON_CORE' ELSE 'OTHER_TRADING' END AS BREAKDOWN, CASE WHEN DIVISION_GROUP IN ('ISG NON CORE', 'TREASURY', 'GWMG', 'OTHER ISG') THEN DIVISION_GROUP WHEN CCC_DIVISION = 'INSTITUTIONAL EQUITY DIVISION' AND DIVISION_GROUP = 'ISG CORE' THEN 'EQUITY' WHEN ( CCC_DIVISION = 'COMMODITIES' OR CCC_BUSINESS_AREA = 'COMMODITIES' ) AND DIVISION_GROUP = 'ISG CORE' THEN 'COMM' WHEN CCC_DIVISION NOT IN ('INSTITUTIONAL EQUITY DIVISION', 'COMMODITIES', 'FIXED INCOME DIVISION') AND DIVISION_GROUP IN ('ISG CORE', 'ISG NON CORE') THEN 'OTHER ISG' WHEN DIVISION_GROUP = 'ISG CORE' AND CCC_DIVISION = 'FIXED INCOME DIVISION' AND CCC_BUSINESS_AREA NOT IN ('COMMODITIES') THEN 'FID' ELSE 'NON ISG' END AS DIVISION_CATEGORY, CASE WHEN DIVISION_GROUP IN ('ISG CORE') THEN DIVISION_GROUP WHEN DIVISION_GROUP IN ('ISG NON CORE') THEN DIVISION_GROUP ELSE 'OTHER' END AS DIVISION_GROUP_FILTER, A.PRODUCT_SOVEREIGN_CD, A.COUNTRY_CD_OF_RISK, A.COUNTRY_OF_RISK_TIER, SUM(COALESCE(USD_EXPOSURE, 0))/1000000 AS USD_EXPOSURE FROM CDWUSER.U_DM_FIRMWIDE A WHERE COB_DATE IN ( '2/28/2018', '2/27/2018', '2/21/2018', '2/14/2018', '2/7/2018', '1/31/2018', '1/24/2018', '1/17/2018', '1/10/2018', '1/3/2018', '12/27/2017', '12/20/2017', '12/13/2017', '12/6/2017', '11/29/2017', '11/22/2017', '11/15/2017') AND VAR_EXCL_FL <> 'Y' AND BU_RISK_SYSTEM <> 'PIPELINE_NY' AND CCC_DIVISION NOT IN ('FIC DVA','FID DVA') AND CCC_BUSINESS_AREA <> 'GLOBAL STRUCT PRODUCTS' AND ( ( ( A.CREDIT_TYPE_CD = 'CREDIT-HY_T1T2' OR A.CCC_PRODUCT_LINE = 'DISTRESSED TRADING' ) AND DIVISION_GROUP NOT IN ('GWMG', 'WMG') ) OR COUNTRY_OF_RISK_TIER = '2' ) GROUP BY COB_DATE, DIVISION_GROUP, A.HYT2_CATEGORY, CASE WHEN A.CREDIT_TYPE_CD = 'CREDIT-HY_T1T2' OR A.CCC_PRODUCT_LINE = 'DISTRESSED TRADING' THEN 'HIGH_YIELD' ELSE NULL END, CASE WHEN A.CCC_PRODUCT_LINE = 'DISTRESSED TRADING' AND A.PRODUCT_TYPE_CODE IN ( 'EQUITY','STOCK','ADR','SWAP','WARRANT','WARRNT' ) THEN 'EQUITY_HEDGE' WHEN A.CCC_PRODUCT_LINE = 'DISTRESSED TRADING' AND A.PRODUCT_TYPE_CODE IN ( 'GVTBOND','ETF','OPTION','CRDINDEX','CDSOPTIDX','FUTURE' ) THEN 'OTHER_HEDGE' ELSE 'NOT_HEDGE' END, CASE WHEN CCC_BUSINESS_AREA IN ('FXEM MACRO TRADING', 'EM CREDIT TRADING') THEN 'FXEM' ELSE 'NON-FXEM' END, CASE WHEN CCC_BUSINESS_AREA IN ( 'CREDIT DERIVATIVE PROD', 'DSP - CREDIT' )THEN 'CDP' ELSE 'NON_CDP' END, CASE WHEN CCC_PRODUCT_LINE IN ('DISTRESSED TRADING') AND CCC_STRATEGY NOT IN ('PAR LOANS TRADING') THEN CCC_PRODUCT_LINE WHEN CCC_PRODUCT_LINE IN ('PAR LOANS TRADING') OR CCC_STRATEGY IN ('PAR LOANS TRADING') THEN 'PAR LOANS TRADING' WHEN CCC_BUSINESS_AREA = 'CREDIT-CORPORATES' AND CCC_PRODUCT_LINE NOT IN ('DISTRESSED TRADING', 'PAR LOANS TRADING') AND CCC_STRATEGY <> 'PAR LOANS TRADING' THEN 'OTHER_CORPORATES' WHEN CCC_BUSINESS_AREA IN ( 'SECURITIZED PRODUCTS GRP', 'PROP WORKOUT', 'LEVERAGED LOANS', 'RESIDENTIAL' ) THEN CCC_BUSINESS_AREA WHEN DIVISION_GROUP = 'ISG NON CORE' AND CCC_BUSINESS_AREA NOT IN ('PROP WORKOUT', 'LEVERAGED LOANS', 'RESIDENTIAL') THEN 'OTHER_NON_CORE' ELSE 'OTHER_TRADING' END, CASE WHEN DIVISION_GROUP IN ('ISG NON CORE', 'TREASURY', 'GWMG', 'OTHER ISG') THEN DIVISION_GROUP WHEN CCC_DIVISION = 'INSTITUTIONAL EQUITY DIVISION' AND DIVISION_GROUP = 'ISG CORE' THEN 'EQUITY' WHEN ( CCC_DIVISION = 'COMMODITIES' OR CCC_BUSINESS_AREA = 'COMMODITIES' ) AND DIVISION_GROUP = 'ISG CORE' THEN 'COMM' WHEN CCC_DIVISION NOT IN ('INSTITUTIONAL EQUITY DIVISION', 'COMMODITIES', 'FIXED INCOME DIVISION') AND DIVISION_GROUP IN ('ISG CORE', 'ISG NON CORE') THEN 'OTHER ISG' WHEN DIVISION_GROUP = 'ISG CORE' AND CCC_DIVISION = 'FIXED INCOME DIVISION' AND CCC_BUSINESS_AREA NOT IN ('COMMODITIES') THEN 'FID' ELSE 'NON ISG' END, A.PRODUCT_SOVEREIGN_CD, A.COUNTRY_CD_OF_RISK, A.COUNTRY_OF_RISK_TIER