SELECT      A.COB_DATE,      A.CCC_DIVISION,      A.CURRENCY_OF_MEASURE,      CASE          WHEN A.PRODUCT_TYPE_CODE IN ('CAP', 'FLOOR', 'SWAPTION') THEN 'OPTION'          ELSE A.PRODUCT_TYPE_CODE      END      AS PRODUCT_TYPE_CODE,      QIS_RUN_TYPE,      BU_RISK_SYSTEM,      SUM(COALESCE(USD_FX, 0)) AS USD_FX FROM      cdwuser.U_QIS_EXP_MSR A WHERE     COB_DATE >= '2018-01-31' AND     COB_DATE <= '2018-02-28' AND     QIS_RUN_TYPE = 'MEXICO_LOCALVAR' GROUP BY      A.PRODUCT_TYPE_CODE,      A.CURRENCY_OF_MEASURE,      A.CCC_DIVISION,      A.COB_DATE,      A.QIS_RUN_TYPE,      A.BU_RISK_SYSTEM