SELECT     a.COB_DATE,     a.POSITION_ULT_ISSUER_PARTY_DARWIN_NAME,     CASE WHEN a.ccc_division = 'NON CORE' THEN 'SPG'             WHEN a.CCC_PRODUCT_LINE IN ('NON INVSMT GRADE PRIMARY', 'NON IG PRIMARY - LOANS', 'PRIMARY - LOANS') THEN 'NON IG PRIMARY - LOANS'              WHEN a.CCC_PRODUCT_LINE = 'DISTRESSED TRADING' THEN a.CCC_PRODUCT_LINE     ELSE 'xxx' END AS business_line,     SUM(a.USD_EXPOSURE) as SUM_USD_EXPOSURE FROM CDWUSER.U_CR_MSR a WHERE     1 = 1 AND (a.COB_DATE = '2018-02-28' or a.COB_DATE = '2018-02-27') and A.CCC_PL_REPORTING_REGION in ('JAPAN') AND      (a.VAR_EXCL_FL <> 'Y' AND a.MEASURE_VAR_EXCL_FL <> 'Y') AND     (a.CCC_PRODUCT_LINE IN ('NON INVSMT GRADE PRIMARY', 'NON IG PRIMARY - LOANS', 'PRIMARY - LOANS') OR     a.ACCOUNT = 'L87TKSG34' OR     a.CCC_DIVISION = 'NON CORE' OR     a.CCC_STRATEGY LIKE '%NON MSMS%') AND     a.USD_EXPOSURE IS NOT NULL GROUP BY     a.COB_DATE,     a.CCC_PRODUCT_LINE,     a.CCC_DIVISION,     a.POSITION_ULT_ISSUER_PARTY_DARWIN_NAME