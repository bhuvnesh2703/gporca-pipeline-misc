SELECT      a.cob_date AS VARDATE,      a.CURRENCY_OF_MEASURE,     Sum(USD_IR_UNIFIED_PV01) AS PV01 FROM     cdwuser.u_IR_msr a  WHERE     (a.COB_DATE = '2018-02-28' or a.COB_DATE = '2018-01-31') and A.CCC_PL_REPORTING_REGION in ('JAPAN','ASIA PACIFIC') AND A.CCC_TAPS_COMPANY in ('0302','0347','0853','4043','4298','4863','6120', '6899','6837','6893','4044','5869','0856','6325','0301','0893','0993') AND          A.CCC_DIVISION IN ('INSTITUTIONAL EQUITY DIVISION')  AND      (a.ccc_business_area <> 'INTERNATIONAL WEALTH MGMT') AND          ccc_banking_trading = 'TRADING'  group by     cob_date,     a.CURRENCY_OF_MEASURE