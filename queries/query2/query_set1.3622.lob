SELECT A.COB_DATE, CASE WHEN A.CCC_BUSINESS_AREA IN ('COMMODITIES') THEN 'COMMODITIES' ELSE CCC_DIVISION END AS CCC_DIVISION, A.CCC_BUSINESS_AREA, ( ROUND (SUM(A.CM_DELTA) + SUM(A.ER_CM_DELTA_DECOMP)) )/1000 AS CM_DELTA FROM CDWUSER.U_DM_FIRMWIDE A WHERE COB_DATE IN ( '01/31/2018','02/28/2018') AND A.DIVISION_GROUP IN ('ISG CORE') AND NOT ( A.CCC_PRODUCT_LINE IN ('IB STRUCTURED') AND A.PRODUCT_TYPE_CODE IN ('ZCS') ) GROUP BY A.COB_DATE, CASE WHEN A.CCC_BUSINESS_AREA IN ('COMMODITIES') THEN 'COMMODITIES' ELSE CCC_DIVISION END, A.CCC_BUSINESS_AREA