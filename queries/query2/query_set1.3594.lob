SELECT COB_DATE, COUNTRY_CD_OF_RISK, PRODUCT_TYPE_CODE, SUM (usd_ir_unified_pv01) AS pv01 FROM CDWUSER.U_IR_MSR where cob_date between '2018-02-13' AND '2018-02-28' AND ccc_business_area IN ('INTEREST RATES', 'LIQUID FLOW RATES', 'STRUCTURED RATES') AND PRODUCT_TYPE_CODE IN ('BONDFUT') AND usd_ir_unified_pv01 IS NOT NULL AND usd_ir_unified_pv01 <> 0 AND COUNTRY_CD_OF_RISK IN ('DEU', 'USA', 'FRA', 'ITA', 'GBR') GROUP BY COB_DATE, COUNTRY_CD_OF_RISK, PRODUCT_TYPE_CODE