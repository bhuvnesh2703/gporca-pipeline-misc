select a.cob_date,BASEL_III_GROUP_BTI, case when A.FID1_SENIORITY IN ('AT1', 'SUBT1', 'SUBUT2') Then 'Junior Subordinate' when A.CCC_PRODUCT_LINE IN ('DISTRESSED TRADING') then 'Distressed Trading' WHEN (a.CCC_BUSINESS_AREA IN ('CREDIT-SECURITIZED PRODS', 'SECURITIZED PRODUCTS GRP', 'COMMERCIAL RE (PTG)', 'RESIDENTIAL') AND A.SPG_DESC NOT IN ('CORPORATE BONDS', 'CORPORATE DEFAULT SWAP', 'SWAP', 'GOVERNMENT')) then 'SPG' else 'spreadsensexpo' end as population, CASE WHEN a.issuer_country_code IN ('ESP', 'GRC', 'IRL', 'ITA', 'PRT') THEN 'Peripherals' WHEN a.issuer_country_code = 'GBR' THEN 'UK' ELSE 'Other' END AS COUNTRY, SUM (Case when A.CCC_PRODUCT_LINE IN ('DISTRESSED TRADING') then (coalesce(A.USD_EXPOSURE,0) + coalesce(A.USD_DELTA,0)) else a.USD_EXPOSURE end) AS USD_NET_EXPOSURE, SUM (coalesce(A.USD_PV10_BENCH,a.USD_CREDIT_PV10PCT)) AS USD_PV10_BENCH, SUM (case when a.vertical_system like'%EQUITY%' then A.USD_PV01SPRD/1000 else A.USD_PV01SPRD end) AS USD_PV01SPRD FROM cdwuser.U_EXP_MSR A WHERE a.COB_DATE in ('2018-02-28','2018-02-21','2018-01-31','2017-12-29','2017-11-30','2017-09-29') and a.CCC_BUSINESS_AREA in ( 'CREDIT-CORPORATES','SECURITIZED PRODUCTS GRP') and A.CCC_PL_REPORTING_REGION = 'EMEA' and CCC_PRODUCT_LINE not in ('CREL BANK HFI','CRE LENDING SEC/HFS','WAREHOUSE') group by a.cob_date,BASEL_III_GROUP_BTI, case when A.FID1_SENIORITY IN ('AT1', 'SUBT1', 'SUBUT2') Then 'Junior Subordinate' when A.CCC_PRODUCT_LINE IN ('DISTRESSED TRADING') then 'Distressed Trading' WHEN (a.CCC_BUSINESS_AREA IN ('CREDIT-SECURITIZED PRODS', 'SECURITIZED PRODUCTS GRP', 'COMMERCIAL RE (PTG)', 'RESIDENTIAL') AND A.SPG_DESC NOT IN ('CORPORATE BONDS', 'CORPORATE DEFAULT SWAP', 'SWAP', 'GOVERNMENT')) then 'SPG' else 'spreadsensexpo' end, CASE WHEN a.issuer_country_code IN ('ESP', 'GRC', 'IRL', 'ITA', 'PRT') THEN 'Peripherals' WHEN a.issuer_country_code = 'GBR' THEN 'UK' ELSE 'Other' END