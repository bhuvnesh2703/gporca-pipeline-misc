SELECT B.COB_DATE, B.EQ_REGION, CAST(SUM(B.EQ_DELTA) AS NUMERIC(15,5)) AS EQ_DELTA FROM ( SELECT A.COB_DATE, A.CCC_DIVISION, CASE WHEN A.CCC_DIVISION IN ( 'BANK RESOURCE MANAGEMENT','COMMODITIES', 'FID UNDEFINED', 'FIXED INCOME DIVISION','INSTITUTIONAL EQUITY DIVISION') THEN 'ISG_GROUP' ELSE 'NON_ISG' END AS ISG_FLAG, CASE WHEN A.ISSUER_COUNTRY_CODE IN ('USA', 'CAN', 'XS') THEN 'NA' WHEN A.ISSUER_COUNTRY_CODE IN ( 'ALB', 'AND', 'AUT', 'BLR', 'BEL', 'BIH', 'BGR', 'CYP', 'HRV', 'CZE', 'DNK', 'EST', 'FRO', 'FIN', 'FRA', 'GEO', 'DEU', 'GRC', 'GGY', 'VAT', 'HUN', 'ISL', 'IRL', 'IMN', 'ITA', 'JEY', 'LVA', 'LIE', 'LTU', 'LUX', 'MKD', 'MLT', 'MDA', 'MCO', 'MNE', 'NLD', 'NOR', 'POL', 'PRT', 'ROU', 'RUS', 'SMR', 'SRB', 'SVK', 'SVN', 'ESP', 'SJM', 'SWE', 'CHE', 'UKR', 'GBR', 'DZA', 'AGO', 'BEN', 'BWA', 'BFA', 'BDI', 'CMR', 'CPV', 'CAF', 'TCD', 'COM', 'COD', 'CIV', 'DJI', 'EGY', 'GNQ', 'ERI', 'ETH', 'GAB', 'GMB', 'GHA', 'GIB', 'GIN', 'GNB', 'KEN', 'LSO', 'LBR', 'LBY', 'MDG', 'MWI', 'MLI', 'MRT', 'MUS', 'MYT', 'MAR', 'MOZ', 'NAM', 'NER', 'NGA', 'REU', 'RWA', 'BLM', 'SHN', 'MAF', 'STP', 'SEN', 'SYC', 'SLE', 'SOM', 'ZAF', 'SSD', 'SDN', 'SWZ', 'TZA', 'TGO', 'TUN', 'UGA', 'ESH', 'ZMB', 'ZWE' ) THEN 'EMEA' WHEN A.ISSUER_COUNTRY_CODE IN ( 'AFG', 'ALA', 'ARM', 'AZE', 'BHR', 'BGD', 'BTN', 'IOT', 'BRN', 'KHM', 'CHN', 'HKG', 'CXR', 'CCK', 'FLK', 'GUM', 'IND', 'IDN', 'IRN', 'IRQ', 'ISR', 'JOR', 'KAZ', 'PRK', 'KOR', 'KWT', 'KGZ', 'LAO', 'LBN', 'MYS', 'MDV', 'MNG', 'MMR', 'NPL', 'OMN', 'PAK', 'PSE', 'PHL', 'QAT', 'SAU', 'SGP', 'LKA', 'SYR', 'TWN', 'TJK', 'THA', 'TUR', 'TKM', 'ARE', 'UMI', 'UZB', 'VNM', 'YEM', 'ASM', 'ATA', 'AUS', 'BVT', 'COK', 'FJI', 'PYF', 'ATF', 'HMD', 'KIR', 'MHL', 'FSM', 'NRU', 'NCL', 'NZL', 'NIU', 'NFK', 'MNP', 'PLW', 'PNG', 'PCN', 'WSM', 'SLB', 'TLS', 'TKL', 'TON', 'TUV', 'VUT', 'WLF' ) THEN 'NJPASIA' WHEN A.ISSUER_COUNTRY_CODE IN ('JPN') THEN 'JAPAN' ELSE 'OTHER' END AS EQ_REGION, A.ISSUER_COUNTRY_CODE, SUM (COALESCE (A.USD_EQ_DELTA_DECOMP,0)) / 1000 AS EQ_DELTA FROM CDWUSER.U_DM_EQ A WHERE COB_DATE >= '07/01/2016' AND COB_DATE <= '02/28/2018' AND A.CCC_BANKING_TRADING IN ('TRADING') GROUP BY A.COB_DATE, A.CCC_DIVISION, A.ISSUER_COUNTRY_CODE, CASE WHEN A.CCC_DIVISION IN ( 'BANK RESOURCE MANAGEMENT','COMMODITIES', 'FID UNDEFINED', 'FIXED INCOME DIVISION','INSTITUTIONAL EQUITY DIVISION') THEN 'ISG_GROUP' ELSE 'NON_ISG' END ) B WHERE B.ISG_FLAG IN ('ISG_GROUP') GROUP BY B.COB_DATE, B.EQ_REGION