SELECT COB_DATE, sum (USD_EXPOSURE) as NEXPOSURE, a.LE_GROUP, CCC_PL_REPORTING_REGION FROM CDWUSER.U_CR_MSR a WHERE cob_date in ('2018-02-28','2018-02-21') and ccc_business_area in ('SECURITIZED PRODUCTS GRP') and COUNTRY_CD_OF_RISK in ('ITA','ESP','GRC','PRT','IRL') and PRODUCT_TYPE_CODE NOT IN ('CRDINDEX','DEFSWAP','LOAN') and (ccc_product_line not like '%CRE%' and ccc_product_line not like '%WAREHOUSE%') GROUP BY COB_DATE, ccc_gl_company_code, a.LE_GROUP, CCC_PL_REPORTING_REGION