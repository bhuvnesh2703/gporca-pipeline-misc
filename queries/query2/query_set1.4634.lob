SELECT COB_DATE, CASE WHEN A.CCC_PRODUCT_LINE IN ('DSP INDEX PRODUCTS TRADING','DSP INDEX PRODUCTS') THEN 'CREDIT-CORPORATES' ELSE A.CCC_BUSINESS_AREA END AS CCC_BUSINESS_AREA, ( SUM ( CASE WHEN A.VERTICAL_SYSTEM LIKE '%STS%' THEN A.USD_FX_PARTIAL_KAPPA WHEN A.VERTICAL_SYSTEM LIKE '%FXOPT%' THEN A.USD_FX_KAPPA END ) ) / 1000 AS FX_VEGA FROM CDWUSER.U_DM_FIRMWIDE A WHERE COB_DATE >= '07/01/2016' AND COB_DATE <= '02/28/2018' AND CCC_TAPS_COMPANY in ('0302','0347','0853','4863','4043','6120','6837','6899','4044','0856','5869','7458') AND CCC_PRODUCT_LINE NOT LIKE 'MS CVA MPE%' AND CCC_BUSINESS_AREA NOT LIKE 'CPM%' AND ( CCC_PRODUCT_LINE NOT LIKE 'NON IG PRIMARY%' OR CCC_STRATEGY NOT LIKE 'NON IG PRIMARY%' ) AND ( CCC_HIERARCHY_LEVEL9 NOT IN ( 'MS CVA MPE DERIVS CPM', 'MPE CVA RISK MGMT', 'MPE FVA RISK MGMT', 'MNE CVA RISK MGMT', 'MNE FVA RISK MGMT', 'CORE MPE CVA', 'MONOLINE MPE CVA', 'MS CVA MNE DERIVS FID' ) OR CCC_STRATEGY NOT IN ( 'MS CVA MPE DERIVS CPM', 'MPE CVA RISK MGMT', 'MPE FVA RISK MGMT', 'MNE CVA RISK MGMT', 'MNE FVA RISK MGMT', 'CORE MPE CVA', 'MONOLINE MPE CVA', 'MS CVA MNE DERIVS FID' ) ) AND CCC_BUSINESS_AREA NOT IN ('LENDING') AND CCC_BUSINESS_AREA NOT LIKE '%CVA%' AND VAR_EXCL_FL <> 'Y' AND VERTICAL_SYSTEM NOT IN ('PIPELINE_NY') AND ( CCC_PRODUCT_LINE NOT IN ( 'PRIMARY - BONDS','PRIMARY - IG BONDS','PRIMARY - NIG BONDS','NON INVSMT GRADE PRIMARY', 'NON IG PRIMARY - LOANS','PRIMARY - LOANS', 'NON IG PRIMARY - HY BOND','INVESTMENT GRADE PRIMARY' ) AND CCC_STRATEGY NOT IN ( 'NON IG PRIMARY - HY BOND', 'INVESTMENT GRADE PRIMARY' ) ) AND CCC_DIVISION = 'FIXED INCOME DIVISION' AND CCC_BUSINESS_AREA NOT IN ('COMMODITIES') AND DIVISION_GROUP = 'ISG CORE' AND NOT ( A.CCC_BUSINESS_AREA = 'SECURITIZED PRODUCTS GRP' AND SPG_DESC IN ('CMBS IO REREMIC','CMBS IO','CMBS SECURITY','RMBS PRIME RESIDUAL','CMBS INDEX','CORPORATE INDEX') AND CCC_PRODUCT_LINE IN ('CRE LENDING', 'CREL BANK HFI', 'CRE LENDING SEC/HFS') ) GROUP BY COB_DATE, CASE WHEN A.CCC_PRODUCT_LINE IN ('DSP INDEX PRODUCTS TRADING','DSP INDEX PRODUCTS') THEN 'CREDIT-CORPORATES' ELSE A.CCC_BUSINESS_AREA END