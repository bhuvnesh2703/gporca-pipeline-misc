SELECT a.COB_DATE, a.ACCOUNT, a.CCC_PRODUCT_LINE, a.VERTICAL_SYSTEM, a.WM_BANKING_PRODUCT_GROUP, a.WM_BANKING_PRODUCT_SUB_GROUP, a.BOOK, a.PRODUCT_TYPE_NAME, a.PRODUCT_SUB_TYPE_CODE, a.MRD_RATING, case when a.vintage in ('04-1', '04-2', '04-3', '04-4', '05-1', '05-2', '05-3', '05-4', '06-1', '06-2', '06-3', '06-4', '07-1', '07-2', '07-3', '07-4', '08-1', '08-2', '08-3', '08-4', '09-1', '09-2', '09-3', '09-4', 'Pre04') then '5& ealiier series' else a.vintage end as vintage, SUM (USD_PV01SPRD) AS USD_PV01SPRD FROM ( SELECT a.POSITION_ID, a.COB_DATE, a.ACCOUNT, a.CCC_PRODUCT_LINE, a.VERTICAL_SYSTEM, a.BOOK, a.PRODUCT_TYPE_CODE, a.PRODUCT_SUB_TYPE_CODE, a.WM_BANKING_PRODUCT_GROUP, a.WM_BANKING_PRODUCT_SUB_GROUP, a.BANKING_PRODUCT_DESCRIPTION, a.PRODUCT_TYPE_NAME, a.MRD_RATING, a.VINTAGE, SUM(COALESCE((USD_PV01SPRD) :: numeric(15,5),0))/1000 AS USD_PV01SPRD FROM cdwuser.u_dm_wm_position a WHERE A.COB_DATE IN ( '2018-02-28', '2018-02-27', '2018-01-31', '2017-12-29', '2017-11-30', '2017-10-31' ) AND A.CCC_TAPS_COMPANY = '1633' AND (A.VAR_EXCL_FL <> 'Y' OR A.BOOK IN ('MSDPB3M', 'MSDPT3M')) AND (A.ccc_banking_trading = 'BANKING' OR (A.CCC_HIERARCHY_LEVEL2 IN ('WEALTH MANAGEMENT', 'GLOBAL WEALTH MANAGEMENT') OR a.CCC_business_area = 'US BANKS-LIQUIDITY') AND A.PRODUCT_TYPE_CODE = 'REPO' AND A.CCC_BUSINESS_AREA NOT IN ('NON CORE MARKETS', 'CORE MARKETS')) and a.CCC_PRODUCT_LINE ='CRE LENDING SEC/HFS' and a.PRODUCT_SUB_TYPE_CODE ='CMBX' GROUP BY a.POSITION_ID, a.COB_DATE, a.ACCOUNT, a.CCC_PRODUCT_LINE, a.VERTICAL_SYSTEM, a.BOOK, a.PRODUCT_TYPE_CODE, a.PRODUCT_SUB_TYPE_CODE, a.WM_BANKING_PRODUCT_GROUP, a.WM_BANKING_PRODUCT_SUB_GROUP, a.BANKING_PRODUCT_DESCRIPTION, a.PRODUCT_TYPE_NAME, a.MRD_RATING, a.VINTAGE, a.VINTAGE_YEAR ) A GROUP BY a.COB_DATE, a.ACCOUNT, a.CCC_PRODUCT_LINE, a.VERTICAL_SYSTEM, a.WM_BANKING_PRODUCT_GROUP, a.WM_BANKING_PRODUCT_SUB_GROUP, a.BOOK, a.PRODUCT_TYPE_NAME, a.PRODUCT_SUB_TYPE_CODE, a.MRD_RATING, case when a.vintage in ('04-1', '04-2', '04-3', '04-4', '05-1', '05-2', '05-3', '05-4', '06-1', '06-2', '06-3', '06-4', '07-1', '07-2', '07-3', '07-4', '08-1', '08-2', '08-3', '08-4', '09-1', '09-2', '09-3', '09-4', 'Pre04') then '5& ealiier series' else a.vintage end