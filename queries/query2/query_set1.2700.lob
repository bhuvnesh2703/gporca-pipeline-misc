SELECT a.COB_DATE, a.ISSUER_COUNTRY_CODE_DECOMP, sum(a.USD_EQ_DELTA_DECOMP) as USD_EQ_DELTA, abs(sum(a.USD_EQ_DELTA_DECOMP)) as ABS_USD_EQ_DELTA FROM CDWUSER.U_DECOMP_MSR a WHERE a.COB_DATE in ('2018-02-28','2018-02-21') AND a.CCC_BANKING_TRADING = 'TRADING' AND a.CCC_DIVISION = 'INSTITUTIONAL EQUITY DIVISION' GROUP BY a.COB_DATE, a.ISSUER_COUNTRY_CODE_DECOMP