SELECT SUM (a.USD_FX) AS usd_fx, a.ccc_business_area, a.CCC_TAPS_COMPANY, a.PARENT_LEGAL_ENTITY, a.currency_of_measure, a.COB_DATE, a.CURVE_CURRENCY_PAIR, a.ccc_PL_reporting_region, a.VERTICAL_SYSTEM, a.book, a.PRODUCT_HIERARCHY_LEVEL7, a.CCC_BANKING_TRADING, a.PRODUCT_TYPE_CODE, a.EXPIRATION_DATE, CASE WHEN a.CURRENCY_OF_MEASURE IN ('HKD', 'SAR', 'AED', 'QAR', 'KWD', 'BHD', 'OMR') THEN 'PEGGED' WHEN a.CURRENCY_OF_MEASURE IN ( 'AUD' , 'NZD', 'CAD', 'NOK', 'SEK', 'DKK', 'CHF', 'EUR', 'GBP', 'JPY') THEN 'G10' WHEN a.CURRENCY_OF_MEASURE IN ('USD', 'UBD') THEN 'USD' WHEN a.CURRENCY_OF_MEASURE IN ('THB', 'TWD', 'KRW', 'KRX', 'CNY', 'CNX', 'CNH', 'VND', 'MYR', 'LKR', 'IDR', 'SGD', 'INR', 'MNT', 'PHP') THEN 'ASIA' WHEN a.CURRENCY_OF_MEASURE IN ('BRL', 'BR1', 'BRX', 'COP', 'CLP', 'PEN', 'ARS', 'MXN' ) THEN 'LATAM' WHEN a.CURRENCY_OF_MEASURE IN ('RUB', 'RBX', 'RU1', 'ZAR', 'TRY', 'HRV', 'CZK', 'HUF', 'PLN', 'ILS', 'NGN', 'EGP' , 'TRL', 'KES', 'MAD', 'ROL', 'GHC', 'ISK', 'KZT', 'AZN', 'HRK', 'UAH', 'GHS', 'BGN') THEN 'EMEA' ELSE 'OTHER' END AS CURRENCY_GROUP FROM cdwuser.U_FX_MSR a where cob_date in ('2018-02-28') AND a.CCC_DIVISION IN ('FIXED INCOME DIVISION') AND a.CCC_business_area IN ('FXEM MACRO TRADING') AND a.CURRENCY_OF_MEASURE = 'AED' AND a.USD_FX IS NOT NULL AND a.USD_FX <> 0 AND EXPIRATION_DATE > '2018-02-28' GROUP BY a.ccc_business_area, a.CCC_TAPS_COMPANY, a.PARENT_LEGAL_ENTITY, a.currency_of_measure, a.COB_DATE, a.CURVE_CURRENCY_PAIR, a.ccc_PL_reporting_region, a.VERTICAL_SYSTEM, a.book, a.PRODUCT_HIERARCHY_LEVEL7, a.CCC_BANKING_TRADING, a.PRODUCT_TYPE_CODE, a.EXPIRATION_DATE