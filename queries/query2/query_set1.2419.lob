SELECT     A.COB_DATE,     A.CCC_BUSINESS_AREA,     A.CCC_PRODUCT_LINE, A.MRD_RATING,     CASE          WHEN BOOK = 'EQCVA' AND BU_RISK_SYSTEM LIKE 'STS%' THEN POSITION_ULT_ISSUER_PARTY_DARWIN_NAME         WHEN POSITION_ULTIMATE_CREDIT_PARTY_DARWIN_NAME = 'N/A' THEN POSITION_ULT_ISSUER_PARTY_DARWIN_NAME          WHEN A.CURVE_NAME='cpcr_voya_bond' THEN 'VOYA FINANCIAL, INC.'         ELSE POSITION_ULT_ISSUER_PARTY_DARWIN_NAME      END AS CTPY_NAME,     CASE          WHEN A.MRD_RATING IN ('A', 'AA', 'AAA', 'BBB') THEN 'IG'          WHEN A.MRD_RATING IN ('BB', 'B', 'D', 'CCC', 'C', 'CC') THEN 'NIG'         ELSE 'NULL'      END AS RATING_FLAG,     SUM(A.USD_DEFAULT_PNL) AS NET_JTD FROM      cdwuser.U_EXP_MSR A WHERE  (a.cob_date = '2018-02-28') AND     ( A.CCC_PRODUCT_LINE = 'XVA HEDGING') and      A.CURVE_NAME NOT IN ('cpcr_mpefund') GROUP BY     A.COB_DATE,     A.CCC_BUSINESS_AREA,     A.CCC_PRODUCT_LINE, A.MRD_RATING,     CASE          WHEN BOOK = 'EQCVA' AND BU_RISK_SYSTEM LIKE 'STS%' THEN POSITION_ULT_ISSUER_PARTY_DARWIN_NAME         WHEN POSITION_ULTIMATE_CREDIT_PARTY_DARWIN_NAME = 'N/A' THEN POSITION_ULT_ISSUER_PARTY_DARWIN_NAME          WHEN A.CURVE_NAME='cpcr_voya_bond' THEN 'VOYA FINANCIAL, INC.'         ELSE POSITION_ULT_ISSUER_PARTY_DARWIN_NAME     END,     CASE          WHEN A.MRD_RATING IN ('A', 'AA', 'AAA', 'BBB') THEN 'IG'          WHEN A.MRD_RATING IN ('BB', 'B', 'D', 'CCC', 'C', 'CC') THEN 'NIG'         ELSE 'NULL'      END