SELECT      a.cob_date AS VARDATE,      a.CURRENCY_OF_MEASURE,     Sum(USD_IR_UNIFIED_PV01) AS PV01 FROM     cdwuser.u_IR_msr a  WHERE     (a.COB_DATE = '2018-02-28' or a.COB_DATE = '2018-02-27') and A.CCC_PL_REPORTING_REGION in ('JAPAN') AND      A.CCC_DIVISION IN ('INSTITUTIONAL EQUITY DIVISION')  AND      (a.ccc_business_area <> 'INTERNATIONAL WEALTH MGMT') AND      (a.PRODUCT_TYPE_CODE IN ('BONDFUT','BONDFUTOPT') OR (a.VAR_EXCL_FL <> 'Y' AND a.MEASURE_VAR_EXCL_FL <> 'Y')) AND       CCC_BANKING_TRADING IN ('TRADING') group by     cob_date,     a.CURRENCY_OF_MEASURE