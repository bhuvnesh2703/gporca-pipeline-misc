SELECT cob_date, LE, case when TERM_NEW > 0 and TERM_NEW <=2 then 'Interest Rate Sensitivity, DV01 2 Year' when TERM_NEW > 2 and TERM_NEW <=5 then 'Interest Rate Sensitivity, DV01 5 Year' when TERM_NEW > 5 and TERM_NEW <=10 then 'Interest Rate Sensitivity, DV01 10 Year' when TERM_NEW > 10 then 'Interest Rate Sensitivity, DV01 30 Year' end as Bucket, sum(USD_PV01)*1000 as USD_PV01 FROM ( select cob_date, TERM_NEW, case when CCC_TAPS_COMPANY in ( '0201','0103','0205','0206','0530','5924') then 'MSCO' when CCC_TAPS_COMPANY in ('0111') then 'MSCS' when CCC_TAPS_COMPANY in ('1633') then 'MSBNA' when CCC_TAPS_COMPANY in ('0362') then 'MSMS' when PARENT_LEGAL_ENTITY in ('0302(G)') then 'MSIP' when PARENT_LEGAL_ENTITY in ('0517(G)') then 'MSBIL' when CCC_TAPS_COMPANY in ('8890', '8840') then 'MSMCB' when CCC_TAPS_COMPANY in ('5745') then 'MSCAP' when CCC_TAPS_COMPANY in ('0105', '0513', '1133', '1614', '2042', '4442', '4919', '4920', '5122', '6399', '7025', '7038', '7114', '7323', '7410', '7504', '7516', '8296', '8304', '8562', '8747', '8934', '7706', '7549', '6706', '5427', '7414', '7465', '7493', '7278', '7279', '7705', '7624', '7681', '7800', '5656', '7708', '7707', '7801', '7627', '7759', '7710', '7762', '7630', '7628', '7761', '7684', '5923', '7609') then 'MSCGI' end as LE, sum(COALESCE(USD_IR_UNIFIED_PV01, 0)) AS USD_PV01 FROM CDWUSER.U_IR_MSR_INTRPLT WHERE COB_DATE in ('2018-02-28') and (CCC_TAPS_COMPANY in ('8890', '8840','1633','0362','0201','0103','0205','0206','0530','5924','0111','0105', '0513', '1133', '1614', '2042', '4442', '4919', '4920', '5122', '6399', '7025', '7038', '7114', '7323', '7410', '7504', '7516', '8296', '8304', '8562', '8747', '8934', '7706', '7549', '6706', '5427', '7414', '7465', '7493', '7278', '7279', '7705', '7624', '7681', '7800', '5656', '7708', '7707', '7801', '7627', '7759', '7710', '7762', '7630', '7628', '7761', '7684', '5923', '7609','5745') or PARENT_LEGAL_ENTITY in ('0302(G)','0517(G)') ) and var_excl_fl <> 'Y' and bu_risk_system 