SELECT a.Cob_date, CASE WHEN ((a.CCC_BUSINESS_AREA IN ('NA ELECTRICITYNATURAL GAS')) /*OLD LOGIC*/ or (A.CCC_PRODUCT_LINE in('NA POWER & GAS'))) /*NEW LOGIC*/ THEN 'NA Power and Gas' WHEN ((a.CCC_BUSINESS_AREA IN ('METALS')) /*OLD LOGIC*/ OR (a.CCC_PRODUCT_LINE in ('PRECIOUS METALS')OR A.CCC_STRATEGY IN ('BASE METALS', 'BULKS'))) /*NEW LOGIC*/ THEN 'Metals' WHEN ((a.CCC_BUSINESS_AREA IN ('OIL LIQUIDS', 'TMG', 'OLYMPUS')) /*OLD LOGIC*/ or (A.CCC_PRODUCT_LINE IN ('OIL & PRODUCTS') OR A.CCC_STRATEGY IN ('TMG', 'OLYMPUS'))) /*NEW LOGIC*/ THEN 'Oil Business' else 'Other' END AS CCC_BUSINESS_AREA, sum(a.USD_CM_Delta) as dollar_delta, sum(case when (a.PRODUCT_TYPE_CODE = 'TIMESPREAD' and a.PROD_POS_NAME_DESCRIPTION = 'WCS POST SPREAD' ) OR a.product_type_code in ('CLEAN FREIGHT','DIRTY FREIGHT','COAL','DRY FREIGHT','EMISSIONS-EU','IRON ORE','NATGAS','PLASTICS') then 0 else cast(a.RAW_CM_DELTA as numeric(15,5)) end) as raw_delta FROM cdwuser.U_CM_MSR a WHERE ((CCC_BUSINESS_AREA NOT IN ('CREDIT', 'MS CVA MNE - COMMOD') AND CCC_DIVISION = 'COMMODITIES' ) /*OLD LOGIC*/ OR (A.CCC_DIVISION = 'FIXED INCOME DIVISION' AND A.CCC_BUSINESS_AREA = 'COMMODITIES' and a.CCC_STRATEGY NOT IN ('MS CVA MNE - COMMOD'))) /*NEW LOGIC*/ AND A.PRODUCT_TYPE_CODE NOT IN ('CURRENCY', 'INTEREST RATE', 'INFLATION', 'TBD', 'MISC','CVA', 'FVA', 'ERROR') AND A.COB_DATE IN ('2018-02-28','2018-02-27','2018-01-31','2017-12-29','2017-09-29','2017-06-30') and NOT(a.INCLUDE_IN_REG_CAAP_FL = 'N' and a.PRODUCT_SUB_TYPE_CODE in ('N EAST CONSUMPTI', 'N EAST CONSUMPTION', 'N EAST APPALACHI', 'N EAST APPALACHIA') and a.BOOK = '18003') group by a.Cob_date, a.CCC_BUSINESS_AREA, ccc_product_line, CCC_STRATEGY,a.PROD_POS_NAME_DESCRIPTION