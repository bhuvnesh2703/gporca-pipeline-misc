SELECT DISTINCT COB_DATE, LIMIT_ID, LIMIT_NAME, LIMIT_APPLIED_TO, PARENT_DESCRIPTION, CASE WHEN TEMP_LIMIT_VALUE IS NULL THEN LIMIT_VALUE WHEN (TEMP_LIMIT_VALUE IS NOT NULL AND TEMP_LIMIT_EXPIRATION_DATE > COB_DATE) THEN TEMP_LIMIT_VALUE ELSE LIMIT_VALUE END AS LIMIT_AMOUNT, SUM_OF FROM cdwuser.U_FLOW_LIMITS WHERE COB_DATE IN ( '2018-02-27', '2018-02-28', '2018-02-28', '2018-02-27', '2018-01-31', '2017-12-29', '2017-11-30', '2017-10-31', '2017-09-29', '2017-08-31' ) and ((LIMIT_ID IN ('3', '4', '9411', '921', '172', '7227', '8972') and parent_description like ('%CRA%')) or LIMIT_ID IN ('7296', '8972', '7981', '6390', '10226'))