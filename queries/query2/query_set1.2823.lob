select cob_date, i.MEASURE_CURRENCY_CCY1 as currency, case when i.TERM_OF_MEASURE / 365 >= 0 and i.TERM_OF_MEASURE / 365 < 1 then '0-1' when i.TERM_OF_MEASURE / 365 >= 1 and i.TERM_OF_MEASURE / 365 < 3 then '1-3' when i.TERM_OF_MEASURE / 365 >= 3 and i.TERM_OF_MEASURE / 365 <5 then '3-5' when i.TERM_OF_MEASURE / 365 >= 5 and i.TERM_OF_MEASURE / 365 <10 then '5-10' when i.TERM_OF_MEASURE / 365 >= 10 then '10+' else 'N/A' end as Term_Bucket, sum(coalesce(i.USD_IR_UNIFIED_PV01,0)) as PV01 from cdwuser.U_IR_MSR_INTRPLT i where i.COB_DATE in ('2018-02-28', '2018-02-21') and i.DIVISION = 'IED' and i.CCC_BANKING_TRADING = 'TRADING' and i.CCC_PL_REPORTING_REGION = 'EMEA' group by cob_date, MEASURE_CURRENCY_CCY1, case when i.TERM_OF_MEASURE / 365 >= 0 and i.TERM_OF_MEASURE / 365 < 1 then '0-1' when i.TERM_OF_MEASURE / 365 >= 1 and i.TERM_OF_MEASURE / 365 < 3 then '1-3' when i.TERM_OF_MEASURE / 365 >= 3 and i.TERM_OF_MEASURE / 365 <5 then '3-5' when i.TERM_OF_MEASURE / 365 >= 5 and i.TERM_OF_MEASURE / 365 <10 then '5-10' when i.TERM_OF_MEASURE / 365 >= 10 then '10+' else 'N/A' end UNION ALL select cob_date, i.MEASURE_CURRENCY_CCY1 as currency, 'TOTAL' as Term_Bucket, sum(coalesce(i.USD_IR_UNIFIED_PV01,0)) as PV01 from cdwuser.U_IR_MSR_INTRPLT i where i.COB_DATE in ('2018-02-28', '2018-02-21') and i.DIVISION = 'IED' and i.CCC_BANKING_TRADING = 'TRADING' and i.CCC_PL_REPORTING_REGION = 'EMEA' group by cob_date, MEASURE_CURRENCY_CCY1