SELECT COB_DATE, PRODUCT_DESCRIPTION, COUNTRY_CD_OF_RISK, PRODUCT_TYPE_CODE, SUM (usd_ir_unified_pv01) AS pv01 FROM CDWUSER.U_IR_MSR WHERE COB_DATE IN ('2018-02-28','2018-02-27') and ccc_business_area IN ('INTEREST RATES', 'LIQUID FLOW RATES', 'STRUCTURED RATES') AND PRODUCT_TYPE_CODE IN ('AGN','BOND','BONDFUT','GVTBOND','RATEFUT','GOVTBONDIL') AND usd_ir_unified_pv01 IS NOT NULL AND usd_ir_unified_pv01 <> 0 AND BOOK in ('DRP','ZZ') AND COUNTRY_CD_OF_RISK in ('GBR') GROUP BY COB_DATE, PRODUCT_DESCRIPTION, COUNTRY_CD_OF_RISK, PRODUCT_TYPE_CODE