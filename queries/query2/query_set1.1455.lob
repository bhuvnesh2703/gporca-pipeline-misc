Select A.COB_DATE, A.PRODUCT_TYPE_CODE, sum(A.USD_CM_DELTA) as DOLLAR_DELTA FROM cdwuser.U_CM_MSR A Where A.PRODUCT_TYPE_CODE in ('PRECIOUSMETAL', 'FUND') AND A.COB_DATE >= '2017-11-30' AND ((A.CCC_BUSINESS_AREA in ('METALS') AND A.CCC_DIVISION IN ('COMMODITIES')) /*OLG LOGIC*/ or (A.CCC_DIVISION = 'FIXED INCOME DIVISION' AND A.CCC_BUSINESS_AREA = 'COMMODITIES' and a.CCC_PRODUCT_LINE IN ('PRECIOUS METALS'))) /*NEW LOGIC*/ AND A.PRODUCT_TYPE_CODE NOT IN ('CURRENCY', 'INTEREST RATE', 'INFLATION', 'TBD', 'MISC','CVA', 'FVA', 'ERROR') Group By A.COB_DATE, A.PRODUCT_TYPE_CODE