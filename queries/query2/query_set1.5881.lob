select a.COB_DATE, CASE WHEN a.CURRENCY_OF_MEASURE IN ('USD', 'GBP', 'EUR', 'JPY') THEN a.CURRENCY_OF_MEASURE ELSE 'OTHER' END AS CURRENCY, a.currency_of_measure, a.BOOK, Case WHEN var_excl_fl ='Y' THEN 'Excluded' ELSE 'Included' END AS Var_Inclusion, sum(coalesce(USD_IR_UNIFIED_PV01,0)) as USD_IR_UNIFIED_PV01 from cdwuser.U_dm_treasury a where CCC_DIVISION = 'TREASURY CAPITAL MARKETS' and cob_date in ('2018-02-28', '2018-02-27', '2018-01-31', '2017-12-29', '2017-11-30', '2017-10-31','2017-09-29','2017-08-31', '2017-12-29') and a.BOOK not in ('TSYGLRCSH') and (MTM_BOOK_POPULATION = 'MTM BOOKS' or BOOK = 'TSTOL') group by a.COB_DATE, CASE WHEN a.CURRENCY_OF_MEASURE IN ('USD', 'GBP', 'EUR', 'JPY') THEN a.CURRENCY_OF_MEASURE ELSE 'OTHER' END, a.BOOK, a.currency_of_measure, Case WHEN var_excl_fl ='Y' THEN 'Excluded' ELSE 'Included' END, a.BOOK