select A.COB_DATE, a.XCCY_MAPPING AS FUNDING_FLAG, a.BOOK, a.CURRENCY_OF_MEASURE, a.EXPIRATION_DATE, SUM(-(A.USD_NOTIONAL/1000)) AS USD_NOTIONAL from cdwuser.U_DM_TREASURY a WHERE COB_DATE IN ('2018-01-31', '2018-02-28') AND A.DATASET_TYPE = 'LRV' AND A.EXPIRATION_DATE > A.COB_DATE AND A.BOOK NOT IN ('TLNFX', 'TZUFX', 'TSTJY', 'TSGFX', 'THKFX') AND A.XCCY_MAPPING = 'XCCY SWAP' GROUP BY A.COB_DATE, a.XCCY_MAPPING, a.BOOK, a.CURRENCY_OF_MEASURE, a.EXPIRATION_DATE