SELECT      A.COB_DATE,     A.CCC_DIVISION,     A.CURRENCY_OF_MEASURE,     CASE          WHEN A.PRODUCT_TYPE_CODE IN ('CAP', 'FLOOR', 'SWAPTION') THEN 'OPTION'          WHEN A.ACCOUNT IN ('083000DW6') AND A.PRODUCT_TYPE_CODE ='FX' AND              A.CURRENCY_OF_POSITION_CCY1 ='USD' AND A.VERTICAL_SYSTEM IN ('STS') THEN 'CASH'           ELSE A.PRODUCT_TYPE_CODE      END      AS PRODUCT_TYPE_CODE,     A.QIS_RUN_TYPE,     A.BU_RISK_SYSTEM,     SUM(COALESCE(A.USD_FX,0)) AS USD_FX FROM      cdwuser.U_QIS_EXP_MSR A WHERE      A.COB_DATE IN ('2018-02-28','2018-02-27') AND     A.QIS_RUN_TYPE = 'MEXICO_LOCALVAR' GROUP BY      CASE          WHEN A.PRODUCT_TYPE_CODE IN ('CAP','FLOOR','SWAPTION') THEN 'OPTION'         WHEN A.ACCOUNT IN ('083000DW6') AND A.PRODUCT_TYPE_CODE = 'FX' AND              A.CURRENCY_OF_POSITION_CCY1 = 'USD' AND A.VERTICAL_SYSTEM IN ('STS') THEN 'CASH'         ELSE A.PRODUCT_TYPE_CODE     END,     A.CURRENCY_OF_MEASURE,     A.CCC_DIVISION,     A.COB_DATE,     A.QIS_RUN_TYPE,     A.BU_RISK_SYSTEM