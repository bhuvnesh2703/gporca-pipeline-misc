SELECT A.TERM_BUCKET ,CASE WHEN A.CURRENCY_PAIR LIKE '%USD%' OR A.CURRENCY_PAIR LIKE '%UBD%' THEN 'USDBRL' WHEN A.CURRENCY_PAIR LIKE '%JPY%' THEN 'BRLJPY' WHEN A.CURRENCY_PAIR LIKE '%EUR%' THEN 'EURBRL' ELSE 'Other' END AS CURRENCY_PAIR ,SUM(A.USD_FX_KAPPA) AS VEGA FROM CDWUSER.U_EXP_MSR A where COB_DATE='2018-02-28' AND A.CCC_DIVISION IN ('INSTITUTIONAL EQUITY DIVISION') AND A.CURRENCY_PAIR LIKE '%BR%' AND A.USD_FX_KAPPA <> 0 AND CCC_BUSINESS_AREA NOT LIKE '%CPM%' AND A.USD_FX_KAPPA IS NOT NULL GROUP BY A.TERM_BUCKET ,CASE WHEN A.CURRENCY_PAIR LIKE '%USD%' OR A.CURRENCY_PAIR LIKE '%UBD%' THEN 'USDBRL' WHEN A.CURRENCY_PAIR LIKE '%JPY%' THEN 'BRLJPY' WHEN A.CURRENCY_PAIR LIKE '%EUR%' THEN 'EURBRL' ELSE 'Other' END