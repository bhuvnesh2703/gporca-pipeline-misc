SELECT CASE WHEN CURRENCY_OF_MEASURE IN ('USD', 'EUR', 'GBP', 'JPY') THEN CURRENCY_OF_MEASURE WHEN CURRENCY_OF_MEASURE IN ('AUD','CAD','CHF','DKK','NOK','NZD','SEK') THEN 'OthersMajor' ELSE 'OthersEM' END AS CURRENCY_OF_MEASURE1, term_new_group, SUM (USD_IR_UNIFIED_PV01) AS usd_pv01 FROM cdwuser.U_DM_IR a WHERE a.cob_date IN ('2018-02-28') AND IS_UK_GROUP = 'Y' AND USD_IR_UNIFIED_PV01 IS NOT NULL AND CCC_DIVISION NOT IN ('FID DVA','FIC DVA') AND CCC_STRATEGY NOT IN ('MS DVA STR NOTES IED') GROUP BY term_new_group, CASE WHEN CURRENCY_OF_MEASURE IN ('USD', 'EUR', 'GBP', 'JPY') THEN CURRENCY_OF_MEASURE WHEN CURRENCY_OF_MEASURE IN ('AUD','CAD','CHF','DKK','NOK','NZD','SEK') THEN 'OthersMajor' ELSE 'OthersEM' END