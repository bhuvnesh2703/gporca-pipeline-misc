SELECT X.*, CASE WHEN MTE >= 0 AND MTE < 4 THEN '0-3M' WHEN MTE >= 4 AND MTE < 7 THEN '3-6M' WHEN MTE >= 7 AND MTE < 12 THEN '6-12M' WHEN MTE >= 12 THEN '>1YR' END as DIFF_GROUP FROM ( SELECT COB_DATE, EXPIRATION_DATE, TIME_BUCKET_ANNUAL || '.' || TIME_BUCKET_QUARTER AS TIME_BUCKET, CASE WHEN PRODUCT_TYPE_CODE IN ('DIST', 'GAS') THEN 'Distillates' WHEN PRODUCT_TYPE_CODE IN ('JET') THEN 'Jet Fuel' WHEN PRODUCT_TYPE_CODE IN ('CRUDE') THEN 'Crude' ELSE 'Others' END AS PRODUCT_TYPE, CASE WHEN EXPIRATION_DATE <= COB_DATE THEN 0 ELSE (DATE_PART('year', EXPIRATION_DATE) - DATE_PART('year', COB_DATE)) * 12 + (DATE_PART('month', EXPIRATION_DATE) - DATE_PART('month',COB_DATE)) END AS MTE, SUM(USD_CM_Delta) AS DOLLAR_DELTA, SUM(RAW_CM_Delta/1000) AS RAW_DELTA FROM CDWUSER.U_CM_MSR a WHERE (CCC_DIVISION='COMMODITIES' OR (CCC_DIVISION = 'FIXED INCOME DIVISION' AND CCC_BUSINESS_AREA = 'COMMODITIES')) AND BU_RISK_RUN_CUSTOM1 = 'NOSTORAGE' AND (CCC_BUSINESS_AREA IN ('OIL LIQUIDS', 'TMG', 'OLYMPUS') OR (CCC_PRODUCT_LINE = 'OIL & PRODUCTS' AND CCC_STRATEGY = 'EMEA OIL & PRODUCTS')) AND PRODUCT_TYPE_CODE NOT IN ('CURRENCY', 'INTEREST RATE', 'INFLATION', 'TBD', 'MISC','CVA', 'FVA','COAL','CLEAN FREIGHT','DIRTY FREIGHT') AND (CCC_BUSINESS_AREA NOT IN ('INVESTMENTS AND STR TRANS','INVESTOR BUSINESS') OR CCC_PRODUCT_LINE NOT IN ('COMMOD EXOTICS','COMMOD INDEX')) AND COB_DATE IN ('2018-02-28','2018-02-27','2018-01-31','2017-12-29','2017-09-29','2017-06-30') AND CCC_PL_REPORTING_REGION = 'EMEA' GROUP BY COB_DATE, EXPIRATION_DATE, TIME_BUCKET_ANNUAL || '.' || TIME_BUCKET_QUARTER, PRODUCT_TYPE, MTE ) X