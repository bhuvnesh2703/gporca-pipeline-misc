SELECT COB_DATE, STRESS_SCENARIO, CURVE_NAME, SHOCK_APPLIED, TERM, CASE WHEN TERM / 365 <= 0.25 THEN '0.25' WHEN TERM / 365 <= 0.5 THEN '0.5' ELSE ROUND(TERM / 365,0) END AS TENOR FROM DWUSER.U_MODULAR_SCENARIOS_PRA WHERE COB_DATE = '2017-02-08' AND STRESS_SCENARIO IN ( 'PRA_BOE_NARRATIVE_2016', 'PRA_CHINA_CRISIS', 'PRA_EUROPEAN_POLITICAL_CRISIS', 'PRA_FINANCIAL_CRISIS', 'PRA_LIQUIDITY_CRUNCH', 'PRA_OIL_SLUMP', 'PRA_RST_2015', 'PRA_RST_BANKING_IMPLOSION', 'PRA_RST_GERMANY_EXIT', 'PRA_SECULAR_STAGNATION', 'PRA_STRUCTURAL_ACCIDENT' ) AND PRODUCT_TYPE = 'SWAP' AND SCENARIO_TYPE = 'GREEK' AND ATTRIBUTION = 'IR DELTA' AND CURVE_NAME IN ('EURNW_3M', 'USDN_3M', 'GBPNW_DISC', 'JPYNW_DISC') AND PCA_COMPONENT = 0 AND CONDITION_SET NOT LIKE '%CVA%' GROUP BY COB_DATE, STRESS_SCENARIO, CURVE_NAME, SHOCK_APPLIED, TERM, CASE WHEN TERM / 365 <= 0.25 THEN '0.25' WHEN TERM / 365 <= 0.5 THEN '0.5' ELSE ROUND(TERM / 365,0) END