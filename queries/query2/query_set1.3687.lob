SELECT     a.COB_DATE,     a.CURRENCY_OF_MEASURE,     a.TERM_NEW,     SUM (a.USD_IR_UNIFIED_PV01) AS usd_ir_unified_pv01 FROM     cdwuser.U_IR_MSR_INTRPLT a WHERE (a.COB_DATE = '2018-02-28') and     (( A.CCC_PL_REPORTING_REGION in ('JAPAN') AND      1=1) OR     a.BOOK IN ('FXJVILDDIC', 'MASHK', 'MIZ')     ) AND     (a.PRODUCT_TYPE_CODE IN ('BONDFUT', 'BONDFUTOPT') OR (a.VAR_EXCL_FL <> 'Y' AND     a.MEASURE_VAR_EXCL_FL <> 'Y')) AND     a.CCC_DIVISION IN ('FIXED INCOME DIVISION', 'BANK RESOURCE MANAGEMENT') AND     a.CCC_BUSINESS_AREA NOT IN ('COMMODITIES') AND     a.USD_IR_UNIFIED_PV01 IS NOT NULL GROUP BY     a.COB_DATE,     a.CURRENCY_OF_MEASURE,     a.TERM_NEW HAVING SUM     (a.USD_IR_UNIFIED_PV01) <> 0