select a.cob_date, a.book, a.product_type_code, CASE WHEN a.book in ('AFSC2') and a.PRODUCT_TYPE_CODE in ('ABS', 'AGNCMO', 'BOND', 'CLO', 'CMBS', 'SWAPTION') THEN 'CREDIT CORP' WHEN A.BOOK IN ('PBCAR') AND A.PRODUCT_TYPE_CODE IN ('ABS', 'AGNCMO', 'BOND', 'CLO', 'CMBS', 'SWAPTION') THEN 'CREDIT AUTO' WHEN A.BOOK IN ('PBCLO') AND A.PRODUCT_TYPE_CODE IN ('AGNCMO', 'BOND', 'CLO', 'CMBS', 'SWAPTION') THEN 'CREDIT CLO' WHEN A.BOOK IN ('PBCMB') AND A.PRODUCT_TYPE_CODE IN ('ABS', 'AGNCMO', 'BOND', 'CLO', 'CMBS', 'SWAPTION') THEN 'CREDIT CMBS' ELSE 'OTHER' END AS AFS_TYPE, COALESCE(USD_NOTIONAL, 0) ::numeric(15,5) /1000 as USD_NOTIONAL, COALESCE(USD_EXPOSURE, 0) ::numeric(15,5) /1000 as USD_EXPOSURE, COALESCE(USD_IR_UNIFIED_PV01, 0) ::numeric(15,5) as USD_PV01, COALESCE(USD_PV01SPRD, 0) ::numeric(15,5) as USD_PV01SPRD, COALESCE(USD_PV10_BENCH, 0) ::numeric(15,5) as USD_PV10_BENCH from cdwuser.U_DM_WM A where a.cob_date in ( '2018-02-28', '2018-01-31', '2017-12-29', '2017-11-30', '2017-10-31', '2017-09-29', '2017-08-31', '2017-07-31' ) and a.book in ('PBCAR', 'PBCLO', 'PBCMB', 'AFSC2') and a.ccc_taps_company in ('6635')