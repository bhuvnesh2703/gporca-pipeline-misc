SELECT a.cob_date, a.TAPSCUSIP, a.COUNTRY_CD_OF_RISK, CASE WHEN a.TAPSCUSIP IS NULL THEN PRODUCT_DESCRIPTION ELSE PRODUCT_DESCRIPTION || ' : ' || TAPSCUSIP END AS desc, SUM (a.USD_IR_UNIFIED_PV01) AS agn_pv01 FROM CDWUSER.U_EXP_MSR a WHERE COB_DATE IN ('2018-02-28') AND CCC_BUSINESS_AREA = 'LIQUID FLOW RATES' AND CCC_PL_REPORTING_REGION = 'EMEA' AND (PRODUCT_TYPE_CODE in ('BOND','AGN') AND PRODUCT_HIERARCHY_LEVEL7 NOT IN ('COVERED BOND-OTHER','HYPOTHEKEN PFANDBRIEFE','JUMBO PFANDBRIEFE','PFANDBRIEFE')) GROUP BY a.cob_date, a.TAPSCUSIP, a.COUNTRY_CD_OF_RISK, CASE WHEN a.TAPSCUSIP IS NULL THEN PRODUCT_DESCRIPTION ELSE PRODUCT_DESCRIPTION || ' : ' || TAPSCUSIP END