SELECT     a.COB_DATE,     CASE         WHEN a.CURRENCY_OF_RISK_CCY1 = 'CNY' THEN 'CNH'         WHEN a.BASE_CCY = 'CNY' THEN 'CNH'         WHEN a.BASE_CCY <> 'UNDEFINED' AND a.CURRENCY_OF_RISK_CCY1 = 'UNDEFINED' THEN a.BASE_CCY         WHEN a.BASE_CCY = 'UNDEFINED' AND a.CURRENCY_OF_RISK_CCY1 <> 'RISK_CURRENCY' THEN a.CURRENCY_OF_RISK_CCY1     ELSE a.CURRENCY_OF_RISK_CCY1 END AS New_Currency_Group,     SUM (COALESCE(CAST(a.SLIDE_FXKAPPA_MIN_20PCT_USD AS DOUBLE PRECISION), 0) ) AS D20,     SUM (COALESCE(CAST(a.SLIDE_FXKAPPA_MIN_10PCT_USD AS DOUBLE PRECISION), 0) ) AS D10,     SUM ( COALESCE(CAST(a.SLIDE_FXKAPPA_MIN_05PCT_USD AS DOUBLE PRECISION), 0) ) AS D05,     SUM (COALESCE(CAST(a.SLIDE_FXKAPPA_MIN_03PCT_USD AS DOUBLE PRECISION), 0) ) AS D03,     SUM (COALESCE(CAST(a.SLIDE_FXKAPPA_MIN_01PCT_USD AS DOUBLE PRECISION), 0) ) AS D01,     SUM (COALESCE(CAST(a.SLIDE_FXKAPPA_MIN_0P5PCT_USD AS DOUBLE PRECISION), 0) ) AS D005,     SUM (COALESCE(CAST(a.SLIDE_FXKAPPA_USD AS DOUBLE PRECISION),0)) AS CT,     SUM (COALESCE(CAST(a.SLIDE_FXKAPPA_PLS_20PCT_USD AS DOUBLE PRECISION), 0) ) AS U20,     SUM (COALESCE(CAST(a.SLIDE_FXKAPPA_PLS_10PCT_USD AS DOUBLE PRECISION), 0) ) AS U10,     SUM (COALESCE(CAST(a.SLIDE_FXKAPPA_PLS_05PCT_USD AS DOUBLE PRECISION), 0) ) AS U05,     SUM (COALESCE(CAST(a.SLIDE_FXKAPPA_PLS_03PCT_USD AS DOUBLE PRECISION), 0) ) AS U03,     SUM (COALESCE(CAST(a.SLIDE_FXKAPPA_PLS_01PCT_USD AS DOUBLE PRECISION), 0) ) AS U01,     SUM (COALESCE(CAST(a.SLIDE_FXKAPPA_PLS_0P5PCT_USD AS DOUBLE PRECISION), 0) ) AS U005 FROM cdwuser.U_FX_MSR_INTRPLT a WHERE (a.COB_DATE = '2018-02-28' or a.COB_DATE = '2018-02-21') and a.vertical_system LIKE ('FXOPT%') AND     (a.ccc_business_area IN ('FXEM CREDIT TRADING', 'FXEM MACRO TRADING')) AND     (a.book NOT IN ('BASKET', 'BASKET HEDGES', /* 'CNH BASKET' ,*/ 'CEEMEA MULTICCY', 'CORRELATION SWAP', 'CORRELATION SWAP 2', 'CORRELATION SWAP 3', 'CORRELATION SWAP 4','DUAL CURRENCY', 'MULTICCY SPEC')) AND     (a.product_type_code <> 'FXBSKT') AND     /*a.CCC_BUSINESS_AREA NOT IN ('COMMODITIES') AND*/       CCC_BANKING_TRADING='TRADING' AND     (a.VAR_EXCL_FL <> 'Y') GROUP BY     a.COB_DATE,     CASE         WHEN a.CURRENCY_OF_RISK_CCY1 = 'CNY' THEN 'CNH'         WHEN a.BASE_CCY = 'CNY' THEN 'CNH'         WHEN a.BASE_CCY <> 'UNDEFINED' AND a.CURRENCY_OF_RISK_CCY1 = 'UNDEFINED' THEN a.BASE_CCY         WHEN a.BASE_CCY = 'UNDEFINED' AND a.CURRENCY_OF_RISK_CCY1 <> 'RISK_CURRENCY' THEN a.CURRENCY_OF_RISK_CCY1     ELSE a.CURRENCY_OF_RISK_CCY1 END