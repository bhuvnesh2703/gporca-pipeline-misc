SELECT a.COB_DATE ,a.BOOK ,CASE when a.BOOK_STRATEGY in ('Parent','Non Bank Subsidiary','Non-US Bank Subsidiaries','Inter Company Funding','Investment Securities','Spec Financing ISG','MSAIL Assets','Other Securities','Reval Hedges','CTA Hedges', 'Non-Bank Investment Portfolios') then 'Assets' ELSE 'Liabilities' END AS Assets_Liabilities_flag ,a.BANK_FLAG ,a.TERM_BUCKET ,a.BUCKETED_TERM ,a.CURRENCY_OF_MEASURE ,CASE WHEN A.CURRENCY_MAPPING IN ('EUR', 'GBP', 'JPY', 'USD') THEN A.CURRENCY_MAPPING ELSE 'OTHER' END AS CURRENCY_BUCKET ,a.VAR_EXCL_FL ,CASE WHEN var_excl_fl = 'Y' THEN 'Excluded' ELSE 'Included' END AS Var_Inclusion ,a.CCC_BUSINESS_AREA ,sum(usd_ir_unified_pv01) as usd_pv01 FROM cdwuser.U_DM_TREASURY a where a.cob_date in ('2018-02-28') AND a.ccc_division = 'TREASURY CAPITAL MARKETS' AND a.BOOK NOT IN ('TSYGLRCSH') GROUP BY a.COB_DATE ,a.BOOK ,CASE when a.BOOK_STRATEGY in ('Parent','Non Bank Subsidiary','Non-US Bank Subsidiaries','Inter Company Funding','Investment Securities','Spec Financing ISG','MSAIL Assets','Other Securities','Reval Hedges','CTA Hedges', 'Non-Bank Investment Portfolios') then 'Assets' ELSE 'Liabilities' END ,a.BANK_FLAG ,a.TERM_BUCKET ,a.BUCKETED_TERM ,a.CURRENCY_OF_MEASURE ,CASE WHEN A.CURRENCY_MAPPING IN ('EUR', 'GBP', 'JPY', 'USD') THEN A.CURRENCY_MAPPING ELSE 'OTHER' END ,a.VAR_EXCL_FL ,CASE WHEN var_excl_fl = 'Y' THEN 'Excluded' ELSE 'Included' END ,a.CCC_BUSINESS_AREA