SELECT a.COB_DATE , 'OIL LIQUIDS' as CCC_BUSINESS_AREA , CASE WHEN A.PRODUCT_TYPE_CODE IN ('ETHANOL', 'NAPHTHA', 'GAS', 'NGL') THEN 'LIGHTENDS' ELSE A.PRODUCT_TYPE_CODE END AS PRODUCT_TYPE , A.POSITION_EXPIRY_YEAR , A.POSITION_EXPIRY_MONTH , CASE WHEN (A.POSITION_EXPIRY_MONTH - extract(month from A.COB_DATE)) + 12 * (A.POSITION_EXPIRY_YEAR - extract(year from A.COB_DATE)) < 3 THEN 'Liquid' WHEN (A.POSITION_EXPIRY_MONTH - extract(month from A.COB_DATE)) + 12 * (A.POSITION_EXPIRY_YEAR - extract(year from A.COB_DATE)) < 6 THEN 'Moderately Liquid' ELSE 'Illiquid' END AS TIME_SPREAD_BUCKET , SUM(A.USD_CM_DELTA) AS CM_DELTA FROM CDWUSER.U_CM_MSR A WHERE a.cob_date in ('02/28/2018', '01/31/2018' ) And ((A.CCC_DIVISION = 'COMMODITIES' AND A.CCC_BUSINESS_AREA IN ('OIL LIQUIDS', 'TMG', 'OLYMPUS')) /*OLD LOGIC*/ OR (A.CCC_DIVISION = 'FIXED INCOME DIVISION' AND A.CCC_BUSINESS_AREA = 'COMMODITIES' AND (A.CCC_PRODUCT_LINE IN ('OIL & PRODUCTS') OR A.CCC_STRATEGY IN ('TMG', 'OLYMPUS')))) /*NEW LOGIC*/ AND A.PRODUCT_TYPE_CODE not IN ('CURRENCY', 'INTEREST RATE', 'INFLATION', 'TBD', 'MISC', 'CVA', 'FVA') AND CCC_TAPS_COMPANY IN ('0105', '0513', '1133', '1614', '2042', '4442', '4919', '4920', '5122', '6399', '7025', '7038', '7114', '7323', '7410', '7504', '7516', '8296', '8304', '8562', '8747', '8934', '7706', '7549', '6706', '5427', '7414', '7465', '7493', '7278', '7279', '7705', '7624', '7681', '7800', '5656', '7708', '7707', '7801', '7627', '7759', '7710', '7762', '7630', '7628', '7761', '7684', '5923', '7609') GROUP BY A.COB_DATE , A.CCC_BUSINESS_AREA , A.PRODUCT_TYPE_CODE , A.POSITION_EXPIRY_YEAR , A.POSITION_EXPIRY_MONTH