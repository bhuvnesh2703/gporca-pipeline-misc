SELECT 'UK Group.IED.Total.ExclCVA' AS Cut, COB_DATE, SUM(DELTA) AS DELTA, SUM(Abs(Delta)) AS GROSS_DELTA FROM (SELECT a.COB_DATE, a.ISSUE_ID_DECOMP, SUM(COALESCE(a.USD_EQ_DELTA_DECOMP,0))/1000 AS Delta FROM CDWUSER.U_DECOMP_MSR a WHERE a.COB_DATE = '2018-02-28' AND a.CCC_DIVISION = 'INSTITUTIONAL EQUITY DIVISION' AND a.LE_GROUP = 'UK' AND(a.CCC_BUSINESS_AREA NOT IN ('CPM TRADING (MPE)', 'CPM','CREDIT', 'MS CVA MNE - FID', 'MS CVA MNE - COMMOD') AND a.CCC_STRATEGY NOT IN ('MS CVA MPE - DERIVATIVES', 'MS CVA MNE - DERIVATIVES', 'EQ XVA HEDGING')) AND (a.CASH_ISSUE_TYPE <>'COMM' OR a.CASH_ISSUE_TYPE IS NULL) AND a.CCC_DIVISION <> 'FID DVA' AND a.CCC_DIVISION <> 'FIC DVA' AND CCC_STRATEGY <> 'MS DVA STR NOTES IED' GROUP BY a.COB_DATE, a.ISSUE_ID_DECOMP) x GROUP BY COB_DATE UNION ALL SELECT Cut, COB_DATE, SUM(DELTA) AS DELTA, SUM(Abs(Delta)) AS GROSS_DELTA FROM (SELECT a.COB_DATE, a.ISSUE_ID_DECOMP, CASE WHEN a.ISSUER_COUNTRY_CODE_DECOMP IN ('USA','CAN') THEN 'UK Group.IED.NAM.ExclCVA' WHEN a.ISSUER_COUNTRY_CODE_DECOMP IN ('GBR') THEN 'UK Group.IED.UK.ExclCVA' WHEN a.ISSUER_COUNTRY_CODE_DECOMP IN ('ALB','AND','AUT','BLR','BEL','BIH','BGR','CYP','HRV','CZE','DNK','EST','FRO','FIN','FRA','GEO','DEU','GRC','GGY','VAT','HUN','ISL','IRL','IMN','ITA','JEY','LVA','LIE','LTU','LUX','MKD','MLT','MDA','MCO','MNE','NLD','NOR','POL','PRT','ROU','RUS','SMR','SRB','SVK','SVN','ESP','SJM','SWE','CHE','UKR') THEN 'UK Group.IED.EUROPE.ExclCVA' WHEN a.ISSUER_COUNTRY_CODE_DECOMP IN ('JPN') THEN 'UK Group.IED.JAPAN.ExclCVA' WHEN a.ISSUER_COUNTRY_CODE_DECOMP IN ('CHN') THEN 'UK Group.IED.CHINA.ExclCVA' WHEN a.ISSUER_COUNTRY_CODE_DECOMP NOT IN ('USA','CAN','GBR','JPN','CHN','ALB','AND','AUT','BLR','BEL','BIH','BGR','CYP','HRV','CZE','DNK','EST','FRO','FIN','FRA','GEO','DEU','GRC','GGY','VAT','HUN','ISL','IRL','IMN','ITA','JEY','LVA','LIE','LTU','LUX','MKD','MLT','MDA','MCO','MNE','NLD','NOR','POL','PRT','ROU','RUS','SMR','SRB','SVK','SVN','ESP','SJM','SWE','CHE','UKR') THEN 'UK Group.IED.OTHER.ExclCVA' END AS CUT, SUM(COALESCE(a.USD_EQ_DELTA_DECOMP,0))/1000 AS Delta FROM CDWUSER.U_DECOMP_MSR a WHERE a.COB_DATE = '2018-02-28' AND a.CCC_DIVISION = 'INSTITUTIONAL EQUITY DIVISION' AND a.LE_GROUP = 'UK' AND(a.CCC_BUSINESS_AREA NOT IN ('CPM TRADING (MPE)', 'CPM','CREDIT', 'MS CVA MNE - FID', 'MS CVA MNE - COMMOD') AND a.CCC_STRATEGY NOT IN ('MS CVA MPE - DERIVATIVES', 'MS CVA MNE - DERIVATIVES', 'EQ XVA HEDGING')) AND (a.CASH_ISSUE_TYPE <>'COMM' OR a.CASH_ISSUE_TYPE IS NULL) AND a.CCC_DIVISION <> 'FID DVA' AND a.CCC_DIVISION <> 'FIC DVA' AND CCC_STRATEGY <> 'MS DVA STR NOTES IED' GROUP BY a.COB_DATE, a.ISSUER_COUNTRY_CODE_DECOMP, a.ISSUE_ID_DECOMP) x GROUP BY Cut, COB_DATE