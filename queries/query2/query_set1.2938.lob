SELECT 'EMEA' as CUT, a.COB_DATE, CASE WHEN a.CURRENCY_OF_MEASURE in ('CNH','CNY') THEN 'CNH/CNY' WHEN a.CURRENCY_OF_MEASURE ='EUR' THEN 'EUR' WHEN a.CURRENCY_OF_MEASURE ='CHF' THEN 'CHF' WHEN a.CURRENCY_OF_MEASURE = 'JPY' THEN 'JPY' ELSE 'OTHER' END AS CURRENCY_OF_MEASURE, sum(coalesce(a.USD_FX,0))/1000 as USD_FX FROM CDWUSER.U_DM_EQ a WHERE a.COB_DATE >= '2016-12-01' AND a.CCC_DIVISION = 'INSTITUTIONAL EQUITY DIVISION' AND a.CCC_BANKING_TRADING = 'TRADING' AND a.CCC_PL_REPORTING_REGION = 'EMEA' AND a.USD_FX <> 0 GROUP BY a.COB_DATE, CASE WHEN a.CURRENCY_OF_MEASURE in ('CNH','CNY') THEN 'CNH/CNY' WHEN a.CURRENCY_OF_MEASURE ='EUR' THEN 'EUR' WHEN a.CURRENCY_OF_MEASURE ='CHF' THEN 'CHF' WHEN a.CURRENCY_OF_MEASURE = 'JPY' THEN 'JPY' ELSE 'OTHER' END UNION ALL SELECT 'UK GROUP' as CUT, a.COB_DATE, CASE WHEN a.CURRENCY_OF_MEASURE in ('CNH','CNY') THEN 'CNH/CNY' WHEN a.CURRENCY_OF_MEASURE ='EUR' THEN 'EUR' WHEN a.CURRENCY_OF_MEASURE ='CHF' THEN 'CHF' WHEN a.CURRENCY_OF_MEASURE = 'JPY' THEN 'JPY' ELSE 'OTHER' END AS CURRENCY_OF_MEASURE, sum(coalesce(a.USD_FX,0))/1000 as USD_FX FROM CDWUSER.U_DM_EQ a WHERE a.COB_DATE >= '2016-12-01' AND a.CCC_DIVISION = 'INSTITUTIONAL EQUITY DIVISION' AND a.CCC_BANKING_TRADING = 'TRADING' AND a.IS_UK_GROUP = 'Y' AND a.USD_FX <> 0 GROUP BY a.COB_DATE, CASE WHEN a.CURRENCY_OF_MEASURE in ('CNH','CNY') THEN 'CNH/CNY' WHEN a.CURRENCY_OF_MEASURE ='EUR' THEN 'EUR' WHEN a.CURRENCY_OF_MEASURE ='CHF' THEN 'CHF' WHEN a.CURRENCY_OF_MEASURE = 'JPY' THEN 'JPY' ELSE 'OTHER' END UNION ALL SELECT 'GLOBAL' as CUT, a.COB_DATE, CASE WHEN a.CURRENCY_OF_MEASURE in ('CNH','CNY') THEN 'CNH/CNY' WHEN a.CURRENCY_OF_MEASURE ='EUR' THEN 'EUR' WHEN a.CURRENCY_OF_MEASURE ='CHF' THEN 'CHF' WHEN a.CURRENCY_OF_MEASURE = 'JPY' THEN 'JPY' ELSE 'OTHER' END AS CURRENCY_OF_MEASURE, sum(coalesce(a.USD_FX,0))/1000 as USD_FX FROM CDWUSER.U_DM_EQ a WHERE a.COB_DATE >= '2016-12-01' AND a.CCC_DIVISION = 'INSTITUTIONAL EQUITY DIVISION' AND a.CCC_BANKING_TRADING = 'TRADING' AND a.USD_FX <> 0 GROUP BY a.COB_DATE, CASE WHEN a.CURRENCY_OF_MEASURE in ('CNH','CNY') THEN 'CNH/CNY' WHEN a.CURRENCY_OF_MEASURE ='EUR' THEN 'EUR' WHEN a.CURRENCY_OF_MEASURE ='CHF' THEN 'CHF' WHEN a.CURRENCY_OF_MEASURE = 'JPY' THEN 'JPY' ELSE 'OTHER' END