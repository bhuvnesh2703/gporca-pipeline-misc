SELECT     a.COB_DATE,     a.CURRENCY_OF_MEASURE,     SUM (CAST(a.USD_IR_KAPPA AS DOUBLE PRECISION)) / 10 AS USD_IR_KAPPA FROM cdwuser.U_IR_MSR_INTRPLT a WHERE (a.COB_DATE = '2018-02-28' or a.COB_DATE = '2018-02-27') and A.CCC_TAPS_COMPANY in ('1050') AND      (a.VAR_EXCL_FL <> 'Y') AND     a.USD_IR_KAPPA IS NOT NULL AND     a.CCC_DIVISION IN ('FIXED INCOME DIVISION') AND     /*a.CCC_BUSINESS_AREA NOT IN ('COMMODITIES') AND*/       CCC_BANKING_TRADING='TRADING' GROUP BY     a.COB_DATE,     a.CURRENCY_OF_MEASURE