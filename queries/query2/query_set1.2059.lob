Select COB_DATE, PRODUCT_TYPE_CODE, CASE WHEN CCC_BUSINESS_AREA = 'COMMODITIES' THEN CCC_STRATEGY ELSE CCC_BUSINESS_AREA END AS CCC_BUSINESS_AREA, CCC_PL_REPORTING_REGION, TIME_BUCKET_CALENDAR, SUM(DOLLAR_GREEK) as DOLLAR_GREEK FROM ( Select prod_pos_name_description,COB_DATE,ccc_business_area, ccc_strategy, CCC_PL_REPORTING_REGION,product_type_code, time_bucket_calendar, product_sub_type_code, EXPIRATION_DATE,time_bucket_quarter, sum(USD_CM_Delta) as dollar_greek, sum(RAW_CM_Delta) as raw_greek, case when EXPIRATION_DATE < ('2019-09-01') then time_bucket_quarter end as quarters, case when product_type_code in ('EAST OFF','EAST PEAK','MIDWEST OFF', 'MIDWEST PEAK','TEXAS OFF','TEXAS PEAK','WEST OFF','WEST PEAK') then 'Electricity' when product_type_code in ('NATGAS') then product_type_code end as GE_DETAIL0 FROM cdwuser.U_CM_MSR Where COB_DATE IN ('02-28-2018','02-21-2018') AND ((CCC_DIVISION='COMMODITIES' AND CCC_BUSINESS_AREA= 'AP EU ELECTRICNATURAL GAS') /*OLD LOGIC*/ OR (CCC_DIVISION='FIXED INCOME DIVISION' AND CCC_BUSINESS_AREA='COMMODITIES' AND CCC_STRATEGY = 'AP EU POWER & GAS')) /*NEW LOGIC*/ and CCC_TRD_BOOK <> 'DEVELOPMENT' AND (CCC_PL_REPORTING_REGION IN ('EUROPE','EMEA')) Group By prod_pos_name_description ,COB_DATE,ccc_business_area,CCC_PL_REPORTING_REGION, ccc_strategy, product_type_code,time_bucket_calendar, EXPIRATION_DATE,time_bucket_quarter, product_sub_type_code) abc GROUP BY COB_DATE, PRODUCT_TYPE_CODE, CASE WHEN CCC_BUSINESS_AREA = 'COMMODITIES' THEN CCC_STRATEGY ELSE CCC_BUSINESS_AREA END, CCC_PL_REPORTING_REGION, TIME_BUCKET_CALENDAR FOR READ ONLY