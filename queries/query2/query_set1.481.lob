SELECT b.cob_date, b.CCC_STRATEGY, b.CCC_PL_REPORTING_REGION, b.CCC_RISK_MANAGER_LOGIN, b.CCC_BUSINESS_AREA, b.SCENARIO_NAME, CASE WHEN UPPER(b.eq_tick) = ('VXX') THEN 'VXX' WHEN UPPER(b.eq_tick) = ('VIXM') THEN 'VIXM' WHEN UPPER(b.eq_tick) = ('SVXY') THEN 'SVXY' WHEN UPPER(b.eq_tick) = ('UVXY') THEN 'UVXY' WHEN UPPER(b.eq_tick) = ('TVIX') THEN 'TVIX' WHEN UPPER(b.eq_tick) LIKE ('%VIXY%') THEN 'VIXY' WHEN UPPER(b.eq_tick) LIKE ('%XIV%') THEN 'XIV' WHEN UPPER(b.EQ_SECURITY_DESCRIPTION) LIKE ('%VXX%') THEN 'VXX' WHEN UPPER(b.EQ_SECURITY_DESCRIPTION) LIKE ('%XIV%') THEN 'XIV' WHEN UPPER(b.EQ_SECURITY_DESCRIPTION) LIKE ('%SVXY%') THEN 'SVXY' WHEN UPPER(b.EQ_SECURITY_DESCRIPTION) LIKE ('%UVXY%') THEN 'UVXY' WHEN UPPER(b.EQ_SECURITY_DESCRIPTION) LIKE ('%TVIX%') THEN 'TVIX' WHEN UPPER(b.EQ_SECURITY_DESCRIPTION) LIKE ('%VIX%') THEN 'VIX'  WHEN b.eq_tick IN ('spx', 'spy', 'sptr1', 'sptr', 'spx2', 'spxpm') THEN 'SPX' WHEN UPPER(b.EQ_SECURITY_DESCRIPTION) LIKE '%SPTR%' THEN 'SPX' ELSE 'OTHER' END AS TYPE_FLAG, SUM(b.POS_TV_MINUS_BASE) AS PNL FROM CDWUSER.U_IED_SURFACE b where B.COB_DATE IN('2018-02-28', '2018-02-27') AND b.SCENARIO_NAME IN ('V+.1', 'V+.2','V+.02') AND b.CCC_BUSINESS_AREA IN ('CASH EQUITIES', 'DERIVATIVES') GROUP BY 1, 2, 3, 4, 5, 6, 7