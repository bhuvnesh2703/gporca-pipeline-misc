SELECT c.cob_date, c.ATTRIBUTE3 AS "Business Unit", c.ATTRIBUTE4 AS "Sub-Business Unit", c.ATTRIBUTE9 AS "Risk Metric", c.ATTRIBUTE10 AS "Unit of Measurement", SUM (c.VALUE1) / 1000000 AS "PNL" FROM CDWUSER.U_GENERIC_DATA c WHERE c.cob_date IN ('2018-01-19','2018-02-16') AND c.analytic_group = 'FDSF' AND c.analytics = 'SCENARIOS' AND c.ATTRIBUTE9 IN ('IR Spot/Volatility') GROUP BY c.cob_date, c.ATTRIBUTE3, c.ATTRIBUTE4, c.ATTRIBUTE9, c.ATTRIBUTE10