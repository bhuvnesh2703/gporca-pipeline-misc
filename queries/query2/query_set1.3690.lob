SELECT     a.COB_DATE,     a.ISSUER_COUNTRY_CODE,     SUM (a.USD_EQ_KAPPA) AS SUM_USD_EQ_KAPPA FROM CDWUSER.U_EQ_MSR_INTRPLT a WHERE     1 = 1 AND A.CCC_PL_REPORTING_REGION in ('JAPAN') AND      (a.VAR_EXCL_FL <> 'Y') AND     (a.ccc_business_area <> 'INTERNATIONAL WEALTH MGMT') AND     a.CCC_DIVISION = 'INSTITUTIONAL EQUITY DIVISION' AND (COB_DATE = '2018-02-28') and     a.CCC_BANKING_TRADING = 'TRADING'  GROUP BY     a.COB_DATE,     a.ISSUER_COUNTRY_CODE