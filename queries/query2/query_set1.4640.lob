SELECT A.COB_DATE, CASE WHEN A.SPG_CATEGORY IN ('RMBS SUB PRIME SECOND PAY') OR A.SPG_PRODUCT_TYPE_GROUP IN ('RESI- NON AGENCY') THEN 'NON_AGENCY_RESI_FINAL' ELSE SPG_CATEGORY END AS SPG_CATEGORY, SUM(CAST(A.USD_EXPOSURE AS NUMERIC (15,5))) / 1000 AS NET_EXPOSURE FROM CDWUSER.U_DM_FIRMWIDE A WHERE COB_DATE >= '07/01/2016' AND COB_DATE <= '02/28/2018' AND CCC_TAPS_COMPANY in ('0302','0347','0853','4863','4043','6120','6837','6899','4044','0856','5869','7458') AND CCC_BUSINESS_AREA IN ('CREDIT-SECURITIZED PRODS', 'SECURITIZED PRODUCTS GRP') AND CCC_DIVISION = 'FIXED INCOME DIVISION' AND CCC_BUSINESS_AREA NOT IN ('COMMODITIES') AND DIVISION_GROUP = 'ISG CORE' AND ( A.SPG_CATEGORY IN ('NON_AGENCY_RESI_FINAL', 'CMBS_SEC_FINAL', 'ABS_FINAL', 'CLO_FINAL', 'RMBS SUB PRIME SECOND PAY') OR A.SPG_PRODUCT_TYPE_GROUP IN ('RESI- NON AGENCY') ) AND ( A.SPG_DESC NOT IN ('AGENCY CMBS IO','OTHER RMBS') OR A.SPG_PRODUCT_TYPE_GROUP IN ('RESI- NON AGENCY') ) AND NOT ( A.CCC_BUSINESS_AREA = 'SECURITIZED PRODUCTS GRP' AND SPG_DESC IN ('CMBS IO REREMIC','CMBS IO','CMBS SECURITY','RMBS PRIME RESIDUAL','CMBS INDEX','CORPORATE INDEX') AND CCC_PRODUCT_LINE IN ('CRE LENDING', 'CREL BANK HFI', 'CRE LENDING SEC/HFS') ) GROUP BY A.COB_DATE, CASE WHEN A.SPG_CATEGORY IN ('RMBS SUB PRIME SECOND PAY') OR A.SPG_PRODUCT_TYPE_GROUP IN ('RESI- NON AGENCY') THEN 'NON_AGENCY_RESI_FINAL' ELSE SPG_CATEGORY END ; SELECT A.SPG_PRODUCT_TYPE_GROUP, A.SPG_DESC FROM DWUSER.U_DM_FIRMWIDE A WHERE A.COB_DATE IN ('2017-07-17') GROUP BY A.SPG_PRODUCT_TYPE_GROUP, A.SPG_DESC