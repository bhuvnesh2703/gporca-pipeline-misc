SELECT 'UK GROUP TOTAL TOTAL excl. CVA' as Cut, COB_DATE, sum(DELTA) as DELTA, sum(Abs(Delta)) as GROSS_DELTA FROM (SELECT a.COB_DATE, a.ISSUE_ID_DECOMP, sum(coalesce(a.USD_EQ_DELTA_DECOMP,0))/1000 as Delta FROM CDWUSER.U_DECOMP_MSR a WHERE a.COB_DATE = '2018-02-28' AND a.CCC_DIVISION = 'INSTITUTIONAL EQUITY DIVISION' AND a.LE_GROUP = 'UK' AND (a.CASH_ISSUE_TYPE <>'COMM' OR a.CASH_ISSUE_TYPE IS NULL) AND(a.CCC_BUSINESS_AREA NOT IN ('CPM TRADING (MPE)', 'CPM','CREDIT', 'MS CVA MNE - FID', 'MS CVA MNE - COMMOD') AND a.CCC_STRATEGY NOT IN ('MS CVA MPE - DERIVATIVES', 'MS CVA MNE - DERIVATIVES', 'EQ XVA HEDGING')) AND a.CCC_DIVISION <> 'FID DVA' AND a.CCC_DIVISION <> 'FIC DVA' AND CCC_STRATEGY <> 'MS DVA STR NOTES IED' group by a.COB_DATE, a.ISSUE_ID_DECOMP) x Group By COB_DATE UNION ALL SELECT Cut, COB_DATE, sum(DELTA) as DELTA, sum(Abs(Delta)) as GROSS_DELTA FROM (SELECT a.COB_DATE, a.ISSUE_ID_DECOMP, CASE WHEN a.ISSUER_COUNTRY_CODE_DECOMP IN ('USA','CAN') THEN 'UK GROUP NAM TOTAL excl. CVA' WHEN a.ISSUER_COUNTRY_CODE_DECOMP IN ('GBR') THEN 'UK GROUP UK TOTAL excl. CVA' WHEN a.ISSUER_COUNTRY_CODE_DECOMP IN ('ALB','AND','AUT','BLR','BEL','BIH','BGR','CYP','HRV','CZE','DNK','EST','FRO','FIN','FRA','GEO','DEU','GRC','GGY','VAT','HUN','ISL','IRL','IMN','ITA','JEY','LVA','LIE','LTU','LUX','MKD','MLT','MDA','MCO','MNE','NLD','NOR','POL','PRT','ROU','RUS','SMR','SRB','SVK','SVN','ESP','SJM','SWE','CHE','UKR') THEN 'UK GROUP EUROPE TOTAL excl. CVA' WHEN a.ISSUER_COUNTRY_CODE_DECOMP IN ('JPN') THEN 'UK GROUP JAPAN TOTAL excl. CVA' WHEN a.ISSUER_COUNTRY_CODE_DECOMP IN ('CHN') THEN 'UK GROUP CHINA TOTAL excl. CVA' WHEN a.ISSUER_COUNTRY_CODE_DECOMP NOT IN ('USA','CAN','GBR','JPN','CHN','ALB','AND','AUT','BLR','BEL','BIH','BGR','CYP','HRV','CZE','DNK','EST','FRO','FIN','FRA','GEO','DEU','GRC','GGY','VAT','HUN','ISL','IRL','IMN','ITA','JEY','LVA','LIE','LTU','LUX','MKD','MLT','MDA','MCO','MNE','NLD','NOR','POL','PRT','ROU','RUS','SMR','SRB','SVK','SVN','ESP','SJM','SWE','CHE','UKR') THEN 'UK GROUP OTHER TOTAL excl. CVA' END AS CUT, sum(coalesce(a.USD_EQ_DELTA_DECOMP,0))/1000 as Delta FROM CDWUSER.U_DECOMP_MSR a WHERE a.COB_DATE = '2018-02-28' AND a.CCC_DIVISION = 'INSTITUTIONAL EQUITY DIVISION' AND a.LE_GROUP = 'UK' AND (a.CASH_ISSUE_TYPE <>'COMM' OR a.CASH_ISSUE_TYPE IS NULL) AND(a.CCC_BUSINESS_AREA NOT IN ('CPM TRADING (MPE)', 'CPM','CREDIT', 'MS CVA MNE - FID', 'MS CVA MNE - COMMOD') AND a.CCC_STRATEGY NOT IN ('MS CVA MPE - DERIVATIVES', 'MS CVA MNE - DERIVATIVES', 'EQ XVA HEDGING')) AND a.CCC_DIVISION <> 'FID DVA' AND a.CCC_DIVISION <> 'FIC DVA' AND CCC_STRATEGY <> 'MS DVA STR NOTES IED' GROUP BY a.COB_DATE, a.ISSUER_COUNTRY_CODE_DECOMP, a.ISSUE_ID_DECOMP) x GROUP BY COB_DATE, Cut UNION ALL SELECT 'UK GROUP CVA TOTAL' as Cut, COB_DATE, sum(DELTA) as DELTA, sum(Abs(Delta)) as GROSS_DELTA FROM (SELECT a.COB_DATE, a.ISSUE_ID_DECOMP, sum(coalesce(a.USD_EQ_DELTA_DECOMP,0))/1000 as Delta FROM CDWUSER.U_DECOMP_MSR a WHERE a.COB_DATE = '2018-02-28' AND a.LE_GROUP = 'UK' AND (a.CASH_ISSUE_TYPE <>'COMM' OR a.CASH_ISSUE_TYPE IS NULL) AND (a.CCC_BUSINESS_AREA IN ('CPM TRADING (MPE)', 'CPM','CREDIT', 'MS CVA MNE - FID', 'MS CVA MNE - COMMOD') OR a.CCC_STRATEGY IN ('MS CVA MPE - DERIVATIVES', 'MS CVA MNE - DERIVATIVES', 'EQ XVA HEDGING')) group by a.COB_DATE, a.ISSUE_ID_DECOMP) x Group By COB_DATE UNION ALL SELECT Cut, COB_DATE, sum(DELTA) as DELTA, sum(Abs(Delta)) as GROSS_DELTA FROM (SELECT a.COB_DATE, a.ISSUE_ID_DECOMP, CASE WHEN a.ISSUER_COUNTRY_CODE_DECOMP IN ('USA','CAN') THEN 'UK GROUP CVA NAM' WHEN a.ISSUER_COUNTRY_CODE_DECOMP IN ('GBR') THEN 'UK GROUP CVA UK' WHEN a.ISSUER_COUNTRY_CODE_DECOMP IN ('ALB','AND','AUT','BLR','BEL','BIH','BGR','CYP','HRV','CZE','DNK','EST','FRO','FIN','FRA','GEO','DEU','GRC','GGY','VAT','HUN','ISL','IRL','IMN','ITA','JEY','LVA','LIE','LTU','LUX','MKD','MLT','MDA','MCO','MNE','NLD','NOR','POL','PRT','ROU','RUS','SMR','SRB','SVK','SVN','ESP','SJM','SWE','CHE','UKR') THEN 'UK GROUP CVA EUROPE' WHEN a.ISSUER_COUNTRY_CODE_DECOMP IN ('JPN') THEN 'UK GROUP CVA JAPAN' WHEN a.ISSUER_COUNTRY_CODE_DECOMP IN ('CHN') THEN 'UK GROUP CVA CHINA' WHEN a.ISSUER_COUNTRY_CODE_DECOMP NOT IN ('USA','CAN','GBR','JPN','CHN','ALB','AND','AUT','BLR','BEL','BIH','BGR','CYP','HRV','CZE','DNK','EST','FRO','FIN','FRA','GEO','DEU','GRC','GGY','VAT','HUN','ISL','IRL','IMN','ITA','JEY','LVA','LIE','LTU','LUX','MKD','MLT','MDA','MCO','MNE','NLD','NOR','POL','PRT','ROU','RUS','SMR','SRB','SVK','SVN','ESP','SJM','SWE','CHE','UKR') THEN 'UK GROUP CVA OTHER' END AS CUT, sum(coalesce(a.USD_EQ_DELTA_DECOMP,0))/1000 as Delta FROM CDWUSER.U_DECOMP_MSR a WHERE a.COB_DATE = '2018-02-28' AND a.LE_GROUP = 'UK' AND (a.CASH_ISSUE_TYPE <>'COMM' OR a.CASH_ISSUE_TYPE IS NULL) AND (a.CCC_BUSINESS_AREA IN ('CPM TRADING (MPE)', 'CPM','CREDIT', 'MS CVA MNE - FID', 'MS CVA MNE - COMMOD') OR a.CCC_STRATEGY IN ('MS CVA MPE - DERIVATIVES', 'MS CVA MNE - DERIVATIVES', 'EQ XVA HEDGING')) GROUP BY a.COB_DATE, a.ISSUER_COUNTRY_CODE_DECOMP, a.ISSUE_ID_DECOMP) x GROUP BY COB_DATE, Cut