SELECT COB_DATE , CCC_DIVISION , case when CCC_BUSINESS_AREA='CPM TRADING (MPE)' then 'CPM' else CCC_BUSINESS_AREA end as CCC_BUSINESS_AREA , CCC_PRODUCT_LINE , SUM(e.USD_FX) AS FX_DELTA FROM cdwuser.u_exp_msr e WHERE COB_DATE in ('2018-02-28','2018-01-31','2017-12-29','2017-11-30','2017-10-31','2017-09-29','2017-08-31','2017-07-31','2017-06-30','2017-05-31','2017-04-28','2017-03-31','2017-02-28') AND e.PARENT_LEGAL_ENTITY = '0302(G)' AND e.CCC_PL_REPORTING_REGION = 'AMERICAS' AND ((e.BOOK <> 'GLFX1' AND COB_DATE > '2017-02-01') OR COB_DATE < '2017-02-01') GROUP BY COB_DATE , CCC_DIVISION , case when CCC_BUSINESS_AREA='CPM TRADING (MPE)' then 'CPM' else CCC_BUSINESS_AREA end , CCC_PRODUCT_LINE