SELECT     COB_DATE,     SUM (USD_CM_Delta) AS USD_CM_Delta FROM CDWUSER.U_CM_MSR a WHERE (a.COB_DATE = '2018-02-28' or a.COB_DATE = '2018-02-21') and     product_type_code IN ('BASEMETAL', 'PRECIOUSMETAL') AND     CCC_PRODUCT_LINE = 'PRECIOUS METALS' AND A.CCC_PL_REPORTING_REGION in ('ASIA PACIFIC') AND      /*(a.PRODUCT_TYPE_CODE IN ('BONDFUT', 'BONDFUTOPT') OR     (a.VAR_EXCL_FL <> 'Y' AND a.MEASURE_VAR_EXCL_FL <> 'Y')) AND*/     CCC_DIVISION = 'FIXED INCOME DIVISION' AND     CCC_BUSINESS_AREA = 'COMMODITIES' /*AND     CCC_TRD_BOOK <> 'DEVELOPMENT'*/ GROUP BY     COB_DATE