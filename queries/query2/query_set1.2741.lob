SELECT a.COB_DATE, SUM (a.SLIDE_EQ_MIN_30_USD) AS MIN_30_SLIDE, SUM (a.SLIDE_EQ_MIN_20_USD) AS MIN_20_SLIDE, SUM (a.SLIDE_EQ_MIN_10_USD) AS MIN_10_SLIDE, SUM (a.USD_FX) AS USD_FX, SUM (a.USD_IR_UNIFIED_PV01) AS USD_IR_UNIFIED_PV01, SUM (a.USD_PV01SPRD) AS USD_PV01SPRD, SUM (a.USD_DELTA) AS USD_DELTA, SUM (a.USD_EQ_KAPPA) AS USD_EQ_KAPPA, SUM (a.SLIDE_EQ_MIN_10_USD) + 0.1 * SUM (a.USD_DELTA) AS D10, SUM (a.USD_THETA_GAMMA) AS USD_THETA_GAMMA, SUM (a.CORR_EQEQ)/1000000 AS CORR_EQ_EQ FROM CDWUSER.U_DM_EQ a WHERE a.COB_DATE >= '2017-09-27' AND a.CCC_DIVISION = 'INSTITUTIONAL EQUITY DIVISION' AND a.CCC_PL_REPORTING_REGION = 'EMEA' AND a.CCC_BANKING_TRADING = 'TRADING' GROUP BY a.COB_DATE