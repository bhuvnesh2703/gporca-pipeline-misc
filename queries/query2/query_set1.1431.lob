SELECT COB_DATE, CCC_BUSINESS_AREA, CCC_STRATEGY, BU_RISK_SYSTEM, SOURCE_SCN_NAME, BOOK, ( CASE WHEN SOURCE_SCN_NAME LIKE '%016' THEN 'MODULAR PARALLEL (-200)' WHEN SOURCE_SCN_NAME LIKE '%017' THEN 'MODULAR PARALLEL (-100)' WHEN SOURCE_SCN_NAME LIKE '%019' THEN 'MODULAR PARALLEL (-25)' WHEN SOURCE_SCN_NAME LIKE '%021' THEN 'MODULAR PARALLEL (+50)' WHEN SOURCE_SCN_NAME LIKE '%024' THEN 'MODULAR PARALLEL (+300)' WHEN SOURCE_SCN_NAME LIKE '%023' THEN 'MODULAR PARALLEL (+200)' WHEN SOURCE_SCN_NAME LIKE '%022' THEN 'MODULAR PARALLEL (+100)' WHEN SOURCE_SCN_NAME LIKE '%025' THEN 'MODULAR PARALLEL (+500)' ELSE 'OTHER' END ) AS SCENARIO, SUM(RAW_PNL) AS PNL FROM DWUSER.U_RAW_SCENARIO_PNL WHERE COB_DATE IN ( '2018-02-28' ) AND BU_RISK_SYSTEM LIKE 'STS%' AND ( SOURCE_SCN_NAME LIKE '%024' OR SOURCE_SCN_NAME LIKE '%023' OR SOURCE_SCN_NAME LIKE '%022' OR SOURCE_SCN_NAME LIKE '%021' OR SOURCE_SCN_NAME LIKE '%025' OR SOURCE_SCN_NAME LIKE '%016' OR SOURCE_SCN_NAME LIKE '%017' OR SOURCE_SCN_NAME LIKE '%018' OR SOURCE_SCN_NAME LIKE '%019' ) AND CCC_BUSINESS_AREA IN ('LIQUID FLOW RATES','STRUCTURED RATES') AND RAW_PNL <> 0 GROUP BY BOOK, COB_DATE, CCC_BUSINESS_AREA, CCC_STRATEGY, BU_RISK_SYSTEM, SOURCE_SCN_NAME, BOOK, ( CASE WHEN SOURCE_SCN_NAME LIKE '%016' THEN 'MODULAR PARALLEL (-200)' WHEN SOURCE_SCN_NAME LIKE '%017' THEN 'MODULAR PARALLEL (-100)' WHEN SOURCE_SCN_NAME LIKE '%019' THEN 'MODULAR PARALLEL (-25)' WHEN SOURCE_SCN_NAME LIKE '%021' THEN 'MODULAR PARALLEL (+50)' WHEN SOURCE_SCN_NAME LIKE '%024' THEN 'MODULAR PARALLEL (+300)' WHEN SOURCE_SCN_NAME LIKE '%023' THEN 'MODULAR PARALLEL (+200)' WHEN SOURCE_SCN_NAME LIKE '%022' THEN 'MODULAR PARALLEL (+100)' WHEN SOURCE_SCN_NAME LIKE '%025' THEN 'MODULAR PARALLEL (+500)' ELSE 'OTHER' END )