SELECT a.cob_date,         a.ccc_product_line,         (case when a.ccc_strategy_details in('EMERGING MARKETS ARB','INVENTORY OPT EM') then 'INVENTORY OPT EM' else a.ccc_strategy_details end) as ccc_strategy_details,         a.ccc_risk_manager_login,         Sum(a.usd_eq_gamma)                              AS eq_gamma_usd,         Sum(a.usd_theta_gamma)                          AS theta_gamma,         0                                                             AS pv01,         Sum(a.usd_eq_kappa)                                 AS KAPPA,  sum(coalesce(a.usd_delta,0)) as NRAMV,        Sum(a.USD_EQ_KAPPA_PLS100BP_TIMEADJ)               AS TA_PARTIAL_KAPPA,         sum((coalesce(a.SLIDE_EQ_MIN_30_USD,0)+coalesce(0.3*a.USD_DELTA,0))) as D30,         sum((coalesce(a.SLIDE_EQ_MIN_20_USD,0)+coalesce(0.2*a.USD_DELTA,0)))  AS D20,         sum((coalesce(a.SLIDE_EQ_MIN_10_USD,0)+coalesce(0.1*a.USD_DELTA,0))) as D10,        sum((coalesce(a.SLIDE_EQ_MIN_05_USD,0)+coalesce(0.05*a.USD_DELTA,0))) as D5,        sum((coalesce(a.SLIDE_EQ_PLS_05_USD,0)-coalesce(0.05*a.USD_DELTA,0))) as P5,        sum((coalesce(a.SLIDE_EQ_PLS_10_USD,0)-coalesce(0.1*a.USD_DELTA,0))) as P10,        sum((coalesce(a.SLIDE_EQ_PLS_20_USD,0)-coalesce(0.2*a.USD_DELTA,0))) as P20 FROM   cdwuser.u_eq_msr a  WHERE (COB_DATE = '2018-02-28' or COB_DATE = '2018-02-27') and   a.ISSUER_COUNTRY_REGION ='LATAM' AND (a.CCC_STRATEGY_DETAILS in ('EMERGING MARKETS DSP','EMERGING MARKETS1','INVENTORY OPT EM','MSET EM DSP') OR a.CCC_RISK_MANAGER_LOGIN IN ('christeb','limaleo','portek','eliasc','pessoat','pintoand'))         AND a.ccc_Division IN ( 'INSTITUTIONAL EQUITY DIVISION' )         AND ( a.CCC_BANKING_TRADING<> 'BANKING'                OR a.CCC_BANKING_TRADING IS NULL )  AND a.ISSUER_COUNTRY_REGION ='LATAM'           GROUP  BY a.cob_date,         a.ccc_product_line,         (case when a.ccc_strategy_details in('EMERGING MARKETS ARB','INVENTORY OPT EM') then 'INVENTORY OPT EM' else a.ccc_strategy_details end),         a.ccc_risk_manager_login UNION SELECT a.cob_date,         a.ccc_product_line,         (case when a.ccc_strategy_details in('EMERGING MARKETS ARB','INVENTORY OPT EM') then 'INVENTORY OPT EM' else a.ccc_strategy_details end) as ccc_strategy_details,         a.ccc_risk_manager_login,         0   AS eq_gamma_usd,         0    AS theta_gamma,          sum(coalesce(a.USD_IR_RHO/10,0)) AS PV01,       0     AS KAPPA,        0  as                          NRAMV,        0    AS TA_PARTIAL_KAPPA,         0   as D30,         0  AS D20,         0 as D10,        0 as D5,        0 as P5,        0 as P10,        0 as P20 FROM   cdwuser.u_ir_msr a  WHERE (COB_DATE = '2018-02-28' or COB_DATE = '2018-02-27') and   a.ISSUER_COUNTRY_REGION ='LATAM' AND (a.CCC_STRATEGY_DETAILS in ('EMERGING MARKETS DSP','EMERGING MARKETS1','INVENTORY OPT EM','MSET EM DSP') OR a.CCC_RISK_MANAGER_LOGIN IN ('christeb','limaleo','portek','eliasc','pessoat','pintoand'))         AND a.CCC_Division IN ( 'INSTITUTIONAL EQUITY DIVISION' )         AND ( a.CCC_BANKING_TRADING<> 'BANKING'                OR a.CCC_BANKING_TRADING IS NULL )            GROUP  BY a.cob_date,         a.ccc_product_line,         (case when a.ccc_strategy_details in('EMERGING MARKETS ARB','INVENTORY OPT EM') then 'INVENTORY OPT EM' else a.ccc_strategy_details end),         a.ccc_risk_manager_login