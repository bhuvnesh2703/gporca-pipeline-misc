SELECT A.COB_DATE, A.CCC_BUSINESS_AREA, A.CCC_PL_REPORTING_REGION, CASE WHEN a.CCC_BUSINESS_AREA IN ('FXEM MACRO TRADING') and a.CCC_PL_REPORTING_REGION in ('AMERICAS') THEN ('US_FXEM_MACRO') ELSE 'OTHER' END AS FLAG, case when a.CURRENCY_OF_MEASURE = 'USD' then 'USD' WHEN A.CURRENCY_OF_MEASURE = 'JPY' THEN 'JPY' WHEN A.CURRENCY_OF_MEASURE = 'GBP' THEN 'GBP' WHEN A.CURRENCY_OF_MEASURE = 'EUR' THEN 'EUR' ELSE 'OTHER' END AS CURRENCY, sum((USD_IR_UNIFIED_PV01) :: numeric(15,5)) as PV01 FROM CDWUSER.U_DM_WM A where a.COB_DATE IN( '2018-02-28', '2018-02-27', '2018-01-31', '2017-12-29', '2017-11-30', '2017-10-31' ) AND A.CCC_TAPS_COMPANY IN ('1633') AND A.PRODUCT_TYPE_CODE <> 'TOB' AND A.CCC_BANKING_TRADING = 'TRADING' AND VAR_EXCL_FL <> 'Y' AND A.VAR_EXCL_FL <> 'Y' and a.book not in ('MSDPB3MREST','MSDPT3MREST','RPOFO','RPOGW','RPOHC') and a.USD_IR_UNIFIED_PV01 is not null /*OR (a.CCC_BUSINESS_AREA IN ('FXEM MACRO TRADING') and a.CCC_PL_REPORTING_REGION not in 'AMERICAS')*/ group by A.COB_DATE, A.CCC_BUSINESS_AREA, A.CCC_PL_REPORTING_REGION, CASE WHEN a.CCC_BUSINESS_AREA IN ('FXEM MACRO TRADING') and a.CCC_PL_REPORTING_REGION in ('AMERICAS') THEN ('NON_US_FXEM_MACRO') ELSE 'OTHER' END, case when a.CURRENCY_OF_MEASURE = 'USD' then 'USD' WHEN A.CURRENCY_OF_MEASURE = 'JPY' THEN 'JPY' WHEN A.CURRENCY_OF_MEASURE = 'GBP' THEN 'GBP' WHEN A.CURRENCY_OF_MEASURE = 'EUR' THEN 'EUR' ELSE 'OTHER' END