SELECT SUM (USD_FX) AS Delta FROM cdwuser.U_FX_MSR f WHERE f.cob_date IN ('2018-02-28') AND f.CURRENCY_OF_MEASURE NOT IN ('USD','UDB') AND (currency_of_measure NOT LIKE '%HKD%' AND currency_of_measure NOT LIKE '%SAR%' AND currency_of_measure NOT LIKE '%AED%') AND LE_GROUP = 'UK' AND ((vertical_system NOT LIKE '%FXOPT%') OR PRODUCT_TYPE_CODE IN ('FXBSKT') OR book IN ('BASKET', 'BASKET HEDGES', 'CEEMEA MULTICCY', 'CORRELATION SWAP', 'CORRELATION SWAP 2', 'CORRELATION SWAP 3', 'CORRELATION SWAP 4', 'DUAL CURRENCY_OF_MEASURE', 'MULTICCY SPEC')) AND CCC_DIVISION NOT IN ('FID DVA','FIC DVA') AND CCC_STRATEGY NOT IN ('MS DVA STR NOTES IED')