SELECT case when a.CURRENCY_OF_MEASURE IN ('EUR','GBP','USD') then a.CURRENCY_OF_MEASURE ELSE 'Other' END As currency2, a.COB_DATE, a.PRODUCT_TYPE_CODE as sectype, a.CCC_PRODUCT_LINE, a.TERM_NEW, case when a.PRODUCT_TYPE_CODE IN ('AGN', 'BOND', 'BONDFUT', 'BONDFUTOPT', 'CRDINDEX', 'DEFSWAP', 'GOVTBONDOPT', 'GVTBOND', 'GVTBOND ETF','GVTBONDIL', 'GVTBONDIL ETF', 'GVTFRN') THEN 'Treasuries' ELSE 'Swaps' END AS sectype2, sum(coalesce(a.USD_IR_UNIFIED_PV01,0)) as PV01, a.CCC_STRATEGY FROM CDWUSER.U_DM_IR a WHERE a.COB_DATE in ('2018-02-28','2018-02-21','2017-12-29','2017-09-29','2017-06-30','2017-03-31','2016-12-30') AND a.ccc_pl_reporting_region = 'EMEA' AND a.ccc_business_area in ('STRUCTURED RATES','LIQUID FLOW RATES') GROUP BY a.CURRENCY_OF_MEASURE, a.COB_DATE, a.term_new, a.ccc_product_line, a.PRODUCT_TYPE_CODE, a.CCC_STRATEGY