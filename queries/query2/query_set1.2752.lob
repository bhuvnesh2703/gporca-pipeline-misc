SELECT a.COB_DATE, a.CCC_PL_REPORTING_REGION, a.CCC_BUSINESS_AREA, a.CCC_PRODUCT_LINE, a.CCC_STRATEGY, a.CCC_TAPS_COMPANY, sum(a.USD_PV01SPRD) as USD_PV01SPRD, sum(a.USD_FX) as USD_FX, sum(a.USD_IR_UNIFIED_PV01) as USD_IR_UNIFIED_PV01 FROM CDWUSER.U_DM_EQ a WHERE a.COB_DATE in ('2018-02-28','2018-02-27') AND a.IS_UK_GROUP = 'Y' AND a.CCC_BANKING_TRADING = 'TRADING' AND a.CCC_DIVISION = 'INSTITUTIONAL EQUITY DIVISION' GROUP BY a.COB_DATE, a.CCC_PL_REPORTING_REGION, a.CCC_BUSINESS_AREA, a.CCC_PRODUCT_LINE, a.CCC_STRATEGY, a.CCC_TAPS_COMPANY