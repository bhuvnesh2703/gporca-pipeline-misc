SELECT 'GLOBAL' as CUT, a.COB_DATE, a.LEVEL1, a.LEVEL2, -a.MARGINAL05 as MARGINAL05, -a.MARGINAL2TOP05 as MARGINAL2TOP05, -a.INCREMENTAL05 as INCREMENTAL05, -a.INCR2TOP05 as INCR2TOP05 FROM CDWUSER.U_VAR_ID_FLAT_VIEW a WHERE a.COB_DATE > '2017-09-28' AND a.HIERARCHY_GROUP_NAME='EQUITY_CV' AND a.HIERARCHY_NAME = 'greek_region' AND a.LEVEL3 IS NULL