select rd.DECOMP_TICK ,rd.DECOMP_SECURITY_DESCRIPTION ,jw.CCC_PL_REPORTING_REGION ,jw.CCC_STRATEGY ,jw.UNDERLIER_TICK ,jw.EXECUTIVE_MODEL ,jw.SECURITY_DESCRIPTION ,jw.PRODUCT_TYPE_CODE ,sum(round(jw.USD_DELTA,2)) as Delta ,sum(round(jw.USD_VEGA,2)) as Vega ,sum(round(jw.USD_DIV_RHO,2)) as Growth ,sum(round(jw.SLIDE_MIN_30_USD,2)) as down30 ,sum(round(jw.SLIDE_MIN_20_USD,2)) as down20 ,sum(round(jw.SLIDE_MIN_10_USD,2)) as down10 ,sum(round(jw.SLIDE_MIN_05_USD,2)) as down5 ,sum(round(jw.SLIDE_MIN_02_USD,2)) as down2 ,sum(round(jw.SLIDE_PLS_02_USD,2)) as up2 ,sum(round(jw.SLIDE_PLS_05_USD,2)) as up5 ,sum(round(jw.SLIDE_PLS_10_USD,2)) as up10 ,sum(round(jw.SLIDE_PLS_20_USD,2)) as up20 ,CASE WHEN jw.UNDERLIER_TICK = rd.DECOMP_TICK THEN 'YES' ELSE 'NO' END AS tick_match from cdwuser.U_IED_VIEW jw,cdwuser.U_IED_DECOMP rd where 1=1 and (jw.COB_DATE=rd.COB_DATE   and jw.POSITION_KEY =rd.POSITION_KEY) and jw.CCC_DIVISION='INSTITUTIONAL EQUITY DIVISION' and jw.CCC_BANKING_TRADING='TRADING' and jw.COB_DATE IN ('2018-02-28') and (rd.DECOMP_TICK in ('riot','mgti','fvti','crcw','btcs','btsc','bbkcf','bkllf','srax','arkk','bitcf','gbtc','ostk','XNET')     or     rd.DECOMP_SEDOL in ('BNQ4H72','B4KRJ56','BD2B5X9','2799384','BYXKNT4','BF2JWF8','660147','B05R1C4','B7NM2X8','BYR8SB1','BYZ9816','BD72F15','BD21614','BYW6553','BF41SF4','BD5VJ23','2269188','BD5M5Z1','BYNR3V7','6199032','BF2WP14','BYWPNK1','BZBG335','BDFCF40','BBCR986','BD0RZK6','BD3HNP4','BYWHFX4','BF03014','BYZDV42','BYNCND4'))  and jw.EXECUTIVE_MODEL not in ('EQUITYSWAP')  group by   rd.DECOMP_TICK ,rd.DECOMP_SECURITY_DESCRIPTION ,jw.CCC_PL_REPORTING_REGION ,jw.CCC_STRATEGY ,jw.UNDERLIER_TICK ,jw.EXECUTIVE_MODEL ,jw.SECURITY_DESCRIPTION ,jw.PRODUCT_TYPE_CODE