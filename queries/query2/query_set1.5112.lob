Select a.COB_DATE, a.CCC_PRODUCT_LINE, SUM ((A.USD_NOTIONAL) :: numeric(15,5)) AS NOTIONAL, SUM (CASE WHEN ((A.USD_EXPOSURE) :: numeric(15,5)) >= 0 THEN ((A.USD_EXPOSURE) :: numeric(15,5)) ELSE 0 END) AS GROSS_LONG_EXPOSURE, SUM (CASE WHEN ((A.USD_EXPOSURE) :: numeric(15,5)) < 0 THEN ((A.USD_EXPOSURE) :: numeric(15,5)) ELSE 0 END) AS GROSS_SHORT_EXPOSURE, SUM ((a.USD_PV01SPRD) :: numeric(15,5)) AS USD_PV01SPRD, SUM (CASE WHEN ((a.USD_PV01SPRD) :: numeric(15,5)) >= 0 THEN ((a.USD_PV01SPRD) :: numeric(15,5)) ELSE 0 END) AS GROSS_SHORT_PV01SPRD, SUM (CASE WHEN ((a.USD_PV01SPRD) :: numeric(15,5)) < 0 THEN ((a.USD_PV01SPRD) :: numeric(15,5)) ELSE 0 END) AS GROSS_LONG_PV01SPRD, SUM ((a.USD_PV10_BENCH) :: numeric(15,5)) AS USD_PV01_BENCH, SUM (CASE WHEN ((a.USD_PV10_BENCH) :: numeric(15,5)) >= 0 THEN ((a.USD_PV10_BENCH) :: numeric(15,5)) ELSE 0 END) AS GROSS_SHORT_PV10_BENCH, SUM (CASE WHEN ((a.USD_PV10_BENCH) :: numeric(15,5)) < 0 THEN ((a.USD_PV10_BENCH) :: numeric(15,5)) ELSE 0 END) AS GROSS_LONG_PV10_BENCH, SUM ((USD_PV01) :: numeric(15,5)) AS Gross_PV01, SUM (CASE WHEN ((A.USD_PV01) :: numeric(15,5)) >= 0 THEN ((A.USD_PV01) :: numeric(15,5)) ELSE 0 END) AS GROSS_SHORT_PV01, SUM (CASE WHEN ((A.USD_PV01) :: numeric(15,5)) < 0 THEN ((A.USD_PV01) :: numeric(15,5)) ELSE 0 END) AS GROSS_LONG_PV01 From cdwuser.U_dm_WM a where a.COB_DATE IN ( '2018-02-28', '2018-01-31', '2017-12-29', '2017-11-30', '2018-01-30', '2017-10-31', '2017-09-29' ) AND A.CCC_TAPS_COMPANY = '1633' AND A.CCC_PRODUCT_LINE in ('PAR LOANS TRADING') GROUP BY a.COB_DATE, a.CCC_PRODUCT_LINE