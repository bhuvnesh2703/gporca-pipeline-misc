SELECT A.COB_DATE, SUM (A.USD_NOTIONAL) ::numeric(15,5) AS Notional, SUM (A.USD_MARKET_VALUE) ::numeric(15,5) AS Market_Value, SUM (A.USD_IR_UNIFIED_PV01) ::numeric(15,5) AS USD_PV01, SUM (A.SLIDE_IR_PLS_100BP_USD) ::numeric(15,5) AS SLIDE_IR_PLS_100BP_USD, SUM (A.SLIDE_IR_PLS_200BP_USD) ::numeric(15,5) AS SLIDE_IR_PLS_200BP_USD FROM cdwuser.U_DM_WM A WHERE A.COB_DATE IN ( '2018-02-28', '2018-01-31' ) AND A.BOOK LIKE '%TBA_HEDGE%' AND A.CCC_TAPS_COMPANY = '6635' GROUP BY A.COB_DATE