SELECT cob_date, a.TICKER || '.' || a.EXCHANGE, a.UNDERLIER_TICK || '.' || a.UNDERLIER_EXCH AS Und_tick, SUM (a.USD_DELTA) AS Delta, a.UNDERLIER_PRODUCT_DESCRIPTION, a.PRODUCT_DESCRIPTION FROM cdwuser.U_EQ_MSR a where a.cob_date in ('2018-02-27','2018-02-20','2018-02-13','2018-02-06','2018-01-30') AND a.CCC_BANKING_TRADING = 'TRADING' AND a.ccc_taps_company = '0302' AND a.CCC_BUSINESS_AREA IN ('FXEM MACRO TRADING', 'EM CREDIT TRADING') GROUP BY cob_date, a.TICKER || '.' || a.EXCHANGE, a.UNDERLIER_TICK || '.' || a.UNDERLIER_EXCH, a.UNDERLIER_PRODUCT_DESCRIPTION, a.PRODUCT_DESCRIPTION