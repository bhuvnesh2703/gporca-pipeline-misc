SELECT     a.cob_date,     a.CURRENCY_COMBINED,     a.MAJOR_EM,     a.REGION_CURRENCY,     SUM (a.USD_FX) AS Delta FROM cdwuser.U_DM_FX a WHERE (a.COB_DATE = '2018-02-28' or a.COB_DATE = '2018-02-21') and     (a.VAR_EXCL_FL <> 'Y') AND     BU_RISK_RUN_CUSTOM1 <> 'STORAGE' AND     CURRENCY_COMBINED NOT IN ('USD', 'UBD') AND     CCC_DIVISION NOT IN ('FIC DVA') AND     USD_FX IS NOT NULL AND     USD_FX <> 0 AND     vertical_system NOT IN ('PIPELINE_NY') AND     VAR_EXCL_FL <> 'Y' AND     A.LENDING_FLAG = 'Non-LENDING' AND     A.CVA_FL = 'N' AND     CCC_BUSINESS_AREA NOT LIKE 'CPM TRADING%' AND     (CCC_BUSINESS_AREA IN ('FXEM MACRO TRADING', 'EM CREDIT TRADING')) GROUP BY     a.cob_date,     a.CURRENCY_COMBINED,     a.REGION_CURRENCY,     a.MAJOR_EM