SELECT a.COB_DATE, a.LIMIT_ID, a.LIMIT_NAME, a.LIMIT_VALUE/1000 as LIMIT_VALUE, a.AGGREGATE_BY_VALUE, a.RISK_VALUE_OVERRIDE/1000 as RISK_VALUE_OVERRIDE, a.TEMP_LIMIT_VALUE, COALESCE (a.TEMP_LIMIT_VALUE,a.LIMIT_VALUE)/1000 AS LIMIT_VAL FROM CDWUSER.U_FLOW_LIMITS a WHERE a.COB_DATE =(SELECT MAX(a.COB_DATE) FROM CDWUSER.U_FLOW_LIMITS a) AND a.RELATIONSHIP_ID = '380514' AND a.AGGREGATE_BY_VALUE = '.' UNION ALL SELECT a.COB_DATE, a.LIMIT_ID, a.LIMIT_NAME, a.LIMIT_VALUE/1000 as LIMIT_VALUE, a.AGGREGATE_BY_VALUE, a.RISK_VALUE_OVERRIDE/1000 as RISK_VALUE_OVERRIDE, a.TEMP_LIMIT_VALUE, COALESCE (a.TEMP_LIMIT_VALUE,a.LIMIT_VALUE)/1000 AS LIMIT_VAL FROM CDWUSER.U_FLOW_LIMITS a WHERE a.COB_DATE =(SELECT MAX(a.COB_DATE) FROM CDWUSER.U_FLOW_LIMITS a) AND a.LIMIT_ID = '8647'