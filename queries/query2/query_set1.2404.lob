SELECT * FROM     (         SELECT             A.PRODUCT_DESCRIPTION,             A.ISSUE_DATE,             - SUM (A.USD_NOTIONAL) AS NOTIONAL,             SUM (A.USD_IR_UNIFIED_PV01) AS PV01,             SUM (A.USD_IR_KAPPA) / 10 AS ir_kappa         FROM cdwuser.U_EXP_MSR a         WHERE a.COB_DATE = '2018-02-28' and CCC_PL_REPORTING_REGION in ('EMEA') AND              a.BOOK = 'CALDM' AND             a.PRODUCT_TYPE_CODE = 'BOND'         GROUP BY             A.PRODUCT_DESCRIPTION,             A.ISSUE_DATE     )     AS B WHERE     B.NOTIONAL IS NOT NULL AND     B.PV01 IS NOT NULL AND     B.ir_kappa IS NOT NULL  order by B.ISSUE_DATE asc