SELECT A.CCC_TRD_BOOK, A.VAR_STATUS, A.ASSET_TYPE_POS_DEF, SUM(COALESCE(A.USD_CM_DELTA ,0) + COALESCE(A.USD_FX_DELTA ,0) + COALESCE(A.USD_IR_DELTA, 0) + COALESCE(a.USD_DELTA, 0)) AS DELTA, SUM(COALESCE(A.USD_CM_GAMMA,0) + COALESCE(A.USD_FX_GAMMA,0) + COALESCE(a.USD_EQ_GAMMA, 0)) AS GAMMA, SUM(COALESCE(A.USD_CM_KAPPA,0) + COALESCE(A.USD_EQ_KAPPA,0) +COALESCE(A.USD_FX_KAPPA,0) + COALESCE(A.USD_CR_KAPPA,0)) AS VEGA, SUM(A.USD_IR_RHO) as RHO, SUM(A.USD_CM_LEASE_RATE) as LEASE_RATE, ABS(SUM(COALESCE(A.USD_CM_DELTA ,0) + COALESCE(A.USD_FX_DELTA ,0) + COALESCE(A.USD_IR_DELTA, 0) + COALESCE(a.USD_DELTA, 0))) AS ABS_DELTA, ABS(SUM(COALESCE(A.USD_CM_KAPPA,0) + COALESCE(A.USD_EQ_KAPPA,0) +COALESCE(A.USD_FX_KAPPA,0) + COALESCE(A.USD_CR_KAPPA,0))) AS ABS_VEGA FROM cdwuser.U_CM_VIEW A WHERE A.COB_DATE = '2018-02-28' AND A.ASSET_TYPE_GROUP <> 'ICAP' AND A.BENCHMARK = 'MISSING' AND A.VAR_STATUS <> 'X' AND (A.USD_CM_DELTA <> 0 OR A.USD_FX_DELTA <> 0 OR A.USD_IR_DELTA <> 0 OR A.USD_DELTA<> 0 OR A.USD_CM_GAMMA <> 0 OR A.USD_FX_GAMMA <> 0 OR A.USD_EQ_GAMMA <>0 OR A.USD_CM_KAPPA <> 0 OR A.USD_EQ_KAPPA <> 0 OR A.USD_FX_KAPPA <>0 OR A.USD_CR_KAPPA <> 0 OR A.USD_IR_RHO <> 0 OR A.USD_CM_LEASE_RATE <> 0) GROUP BY A.CCC_TRD_BOOK, A.VAR_STATUS, A.ASSET_TYPE_POS_DEF