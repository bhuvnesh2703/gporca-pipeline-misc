SELECT a.COB_DATE, a.CCC_BUSINESS_AREA, a.ccc_division, a.CCC_PRODUCT_LINE, a.currency_of_measure, a.PRODUCT_TYPE_CODE, a.TERM_BUCKET, a.TERM_OF_MEASURE, a.TERM_OF_MEASURE_WHOLE, a.vertical_system, a.TERM_OF_POSITION, a.CCC_PL_REPORTING_REGION, a.PRODUCT_HIERARCHY_LEVEL7, a.POSITION_ULT_ISSUER_PARTY_DARWIN_NAME AS REFERENCE_ENTITY_NAME, a.FID1_INDUSTRY_NAME_LEVEL1, CASE WHEN a.TERM_OF_MEASURE <= 30 THEN '<1mth' WHEN (a.TERM_OF_MEASURE > 30 AND a.TERM_OF_MEASURE <= 60) THEN '1-2mth' WHEN (a.TERM_OF_MEASURE > 60 AND a.TERM_OF_MEASURE <= 90) THEN '2-3mth' WHEN (a.TERM_OF_MEASURE > 90 AND a.TERM_OF_MEASURE <= 120) THEN '3-4mth' WHEN (a.TERM_OF_MEASURE > 120 AND a.TERM_OF_MEASURE <= 150) THEN '4-5mth' WHEN (a.TERM_OF_MEASURE > 150 AND a.TERM_OF_MEASURE <= 180) THEN '5-6mth' WHEN (a.TERM_OF_MEASURE > 180 AND a.TERM_OF_MEASURE <= 270) THEN '6-9mth' WHEN (a.TERM_OF_MEASURE > 270 AND a.TERM_OF_MEASURE <= 365) THEN '9mth-1yr' WHEN (a.TERM_OF_MEASURE > 365 AND a.TERM_OF_MEASURE <= 547) THEN '1yr-1.5yr' WHEN (a.TERM_OF_MEASURE > 547 AND a.TERM_OF_MEASURE <= 730) THEN '1.5yr-2yr' WHEN a.TERM_OF_MEASURE > 3650 THEN '>10yr' ELSE a.TERM_BUCKET END AS TERM_BUCKET, a.EXPIRATION_DATE, CASE WHEN a.PRODUCT_TYPE_CODE IN ('BOND', 'GVTBOND', 'BANKDEBT') THEN 'Bonds' WHEN a.PRODUCT_TYPE_CODE IN ('CDO2', 'CLNBSKT', 'CLNCDO2', 'CRDBSKT') THEN 'Baskets' ELSE 'Index, CDS & ZCS' END AS PRODUCT_GROUP, CASE WHEN a.FID1_INDUSTRY_NAME_LEVEL1 IN ('SOVEREIGN', 'GOVERNMENT SPONSORED') THEN 'Sovereign & Govt Sponsored' WHEN a. FID1_INDUSTRY_NAME_LEVEL1 = 'CORPORATES: FINANCIALS' THEN 'Corporates: Financials' ELSE 'Corporates: Non-Financials' END AS INDUSTRY_GROUP, SUM (a.USD_PV01SPRD_BENCH) AS BPV01, SUM (A.USD_PV10_BENCH) AS BPV10_PCT, SUM (A.USD_EXPOSURE) AS NETEXP FROM cdwuser.U_CR_MSR a WHERE a.cob_date in ('2018-02-28', '2018-02-27') and a.EXPIRATION_DATE >= '2018-02-28' and a.ISSUER_COUNTRY_CODE = 'ISR' AND a.ccc_division IN ('FIXED INCOME DIVISION', 'INSTITUTIONAL EQUITY DIVISION') AND a.CCC_BUSINESS_AREA NOT IN ('CPM', 'DSP - CREDIT', 'LENDING', 'OTC CLIENT CLEARING') AND a.CCC_PRODUCT_LINE NOT IN ('INVESTMENT GRADE PRIMARY', 'NON IG PRIMARY - LOANS', 'NON IG PRIMARY - HY BOND') AND a.ccc_banking_trading = 'TRADING' GROUP BY a.COB_DATE, a.CCC_BUSINESS_AREA, a.ccc_division, a.CCC_PRODUCT_LINE, a.PRODUCT_TYPE_CODE, a.TERM_BUCKET, a.TERM_OF_MEASURE, a.TERM_OF_MEASURE_WHOLE, a.TERM_OF_POSITION, a.CCC_PL_REPORTING_REGION, a.PRODUCT_HIERARCHY_LEVEL7, a.POSITION_ULT_ISSUER_PARTY_DARWIN_NAME, a.FID1_INDUSTRY_NAME_LEVEL1, CASE WHEN a.PRODUCT_TYPE_CODE IN ('BOND', 'GVTBOND', 'BANKDEBT') THEN 'Bonds' WHEN a.PRODUCT_TYPE_CODE IN ('CDO2', 'CLNBSKT', 'CLNCDO2', 'CRDBSKT') THEN 'Baskets' ELSE 'Index, CDS & ZCS' END, CASE WHEN a.FID1_INDUSTRY_NAME_LEVEL1 IN ('Sovereign', 'Government Sponsored') THEN 'Sovereign & Govt Sponsored' WHEN a. FID1_INDUSTRY_NAME_LEVEL1 = 'Corporates: Financials' THEN 'Corporates: Financials' ELSE 'Corporates: Non-Financials' END, a.EXPIRATION_DATE, a.vertical_system, a.currency_of_measure