select a.COB_DATE, a.BUCKETED_TERM, a.TERM_BUCKET, a.CURRENCY_MAPPING, CASE WHEN A.CURRENCY_MAPPING IN ('EUR', 'GBP', 'JPY', 'USD') THEN A.CURRENCY_MAPPING ELSE 'OTHER' END AS CURRENCY_BUCKET, a.VAR_EXCL_FL, a.CCC_BUSINESS_AREA, Case WHEN var_excl_fl ='Y' THEN 'Excluded' ELSE 'Included' END AS Var_Inclusion, a.CURRENCY_OF_MEASURE, a.BANK_FLAG, a.BOOK, a.BOOK_STRATEGY, sum(a.USD_IR_UNIFIED_PV01) as usd_pv01 from cdwuser.U_DM_Treasury a where a.cob_date in ('2018-02-28') and a.ccc_division = 'TREASURY CAPITAL MARKETS' and a.BOOK not in ('TSYGLRCSH') and (a.MTM_BOOK_POPULATION = 'MTM BOOKS' or BOOK = 'TSTOL') group by a.COB_DATE, a.BUCKETED_TERM, a.TERM_BUCKET, a.VAR_EXCL_FL, a.CCC_BUSINESS_AREA, Case WHEN var_excl_fl ='Y' THEN 'Excluded' ELSE 'Included' END, a.CURRENCY_OF_MEASURE, a.BANK_FLAG, a.BOOK, a.BOOK_STRATEGY, a.CURRENCY_MAPPING, CASE WHEN A.CURRENCY_MAPPING IN ('EUR', 'GBP', 'JPY', 'USD') THEN A.CURRENCY_MAPPING ELSE 'OTHER' END