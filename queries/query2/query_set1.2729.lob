SELECT a.COB_DATE, a.CCC_PL_REPORTING_REGION, sum(a.USD_NOTIONAL) as USD_NOTIONAL FROM CDWUSER.U_DM_EQ a WHERE a.COB_DATE in ('2018-02-28','2018-02-27') AND a.CCC_BANKING_TRADING = 'TRADING' AND a.CCC_DIVISION = 'INSTITUTIONAL EQUITY DIVISION' AND a.PRODUCT_TYPE_CODE = 'CRDBSKT' GROUP BY a.COB_DATE, a.CCC_PL_REPORTING_REGION