SELECT a.cob_date, a.CURRENCY_OF_MEASURE, SUM (a.USD_EXPOSURE) AS net_exp, a.TAPSCUSIP, a.CCC_PL_REPORTING_REGION, CASE WHEN a.TAPSCUSIP IS NULL THEN PRODUCT_DESCRIPTION ELSE PRODUCT_DESCRIPTION || ' : ' || TAPSCUSIP END AS desc FROM cdwuser.U_CR_MSR a WHERE cob_date in ('2018-02-28','2018-02-27') AND CCC_BUSINESS_AREA in ('STRUCTURED RATES') AND CCC_PL_REPORTING_REGION = 'EMEA' AND CCC_division IN ('FIXED INCOME DIVISION') AND CCC_BUSINESS_AREA NOT IN ('LENDING') AND CCC_BUSINESS_AREA NOT LIKE '%CVA%' AND CCC_BUSINESS_AREA NOT LIKE '%CPM%' AND ((PRODUCT_TYPE_CODE IN ('CAP', 'FLOOR', 'SWAP', 'ZCS') AND CURVE_TYPE = 'GovtYieldCurve') OR PRODUCT_TYPE_CODE IN ('AGN', 'BOND', 'BONDFUT', 'BONDFUTOPT', 'BONFUTOPT', 'GOVTBONDOPT', 'GOVTBONDOPTIL', 'GVTBOND', 'GVTBONDIL', 'GVTFRN', 'TRRSWAP', 'TRS - GVTBOND', 'TRS - GVTBONDIL')) AND (ccc_strategy NOT IN ('CREL BANK HFI/HFS', 'CREL SECURITIZATION') OR PRODUCT_TYPE_CODE NOT IN ('LOAN')) AND ccc_strategy NOT IN ('DISTRESSED TRADING1', 'NON MSMS1') AND FID1_SENIORITY NOT IN ('AT1', 'SubT1', 'SubUT2') AND COUNTRY_CD_OF_RISK IN ('ESP', 'ITA', 'GRC', 'PRT', 'CYP', 'EST', 'LVA', 'LTU', 'MLT', 'SVK', 'SVN') AND PRODUCT_TYPE_CODE NOT IN ('WAREHOUSE') AND VERTICAL_SYSTEM NOT IN ('PIPELINE_NY' ) AND NOT (CCC_BUSINESS_AREA = 'CREDIT-CORPORATES' AND (CCC_PRODUCT_LINE IN ('NON INVSMT GRADE PRIMARY', 'NON IG PRIMARY - LOANS', 'PRIMARY - LOANS', 'NON IG PRIMARY - HY BOND', 'INVESTMENT GRADE PRIMARY')OR CCC_STRATEGY IN ('NON IG PRIMARY - HY BOND', 'INVESTMENT GRADE PRIMARY'))) GROUP BY a.cob_date, a.CURRENCY_OF_MEASURE, a.TAPSCUSIP, PRODUCT_DESCRIPTION, CCC_PL_REPORTING_REGION UNION ALL SELECT a.cob_date, a.CURRENCY_OF_MEASURE, SUM (a.USD_EXPOSURE) AS net_exp, a.TAPSCUSIP, a.CCC_PL_REPORTING_REGION, CASE WHEN a.TAPSCUSIP IS NULL THEN PRODUCT_DESCRIPTION ELSE PRODUCT_DESCRIPTION || ' : ' || TAPSCUSIP END AS desc FROM cdwuser.U_CR_MSR a WHERE cob_date in ('2018-02-28','2018-02-27') AND CCC_BUSINESS_AREA in ('STRUCTURED RATES') AND CCC_PL_REPORTING_REGION = 'EMEA' AND CCC_division IN ('FIXED INCOME DIVISION') AND CCC_BUSINESS_AREA NOT IN ('LENDING') AND CCC_BUSINESS_AREA NOT LIKE '%CVA%' AND CCC_BUSINESS_AREA NOT LIKE '%CPM%' AND ((PRODUCT_TYPE_CODE IN ('CAP', 'FLOOR', 'SWAP', 'ZCS') AND CURVE_TYPE NOT IN ('GovtYieldCurve')) OR (VERTICAL_SYSTEM LIKE '%SPG%' AND IS_CASH_FL = 'Y') OR PRODUCT_TYPE_CODE IN ('CDO2', 'CDSOPTIDX', 'CLNBSKT', 'CLNCDO2', 'CP', 'CRDBSKT', 'CRDINDEX', 'DEFSWAP', 'FRN', 'LOANCDS', 'LOANDGDS', 'LOANINDEX', 'NTDCDO')) AND (ccc_strategy NOT IN ('CREL BANK HFI/HFS', 'CREL SECURITIZATION') OR PRODUCT_TYPE_CODE NOT IN ('LOAN')) AND ccc_strategy NOT IN ('DISTRESSED TRADING1', 'NON MSMS1') AND FID1_SENIORITY NOT IN ('AT1', 'SubT1', 'SubUT2') AND COUNTRY_CD_OF_RISK IN ('ESP', 'ITA', 'GRC', 'PRT', 'CYP', 'EST', 'LVA', 'LTU', 'MLT', 'SVK', 'SVN') AND PRODUCT_TYPE_CODE NOT IN ('WAREHOUSE') AND NOT (CCC_BUSINESS_AREA = 'CREDIT-CORPORATES' AND (CCC_PRODUCT_LINE IN ('NON INVSMT GRADE PRIMARY', 'NON IG PRIMARY - LOANS', 'PRIMARY - LOANS', 'NON IG PRIMARY - HY BOND', 'INVESTMENT GRADE PRIMARY')OR CCC_STRATEGY IN ('NON IG PRIMARY - HY BOND', 'INVESTMENT GRADE PRIMARY'))) GROUP BY a.cob_date, a.CURRENCY_OF_MEASURE, a.TAPSCUSIP, PRODUCT_DESCRIPTION, CCC_PL_REPORTING_REGION