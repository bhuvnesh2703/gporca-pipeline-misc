SELECT a.ATTRIBUTE9 AS "Risk Metric", CASE WHEN a.ATTRIBUTE8 IN ('USD') THEN 'USD' WHEN a.ATTRIBUTE8 IN ('EUR') THEN 'EUR' WHEN a.ATTRIBUTE8 IN ('GBP') THEN 'GBP' WHEN a.ATTRIBUTE8 IN ('JPY') THEN 'JPY' WHEN a.ATTRIBUTE8 IN ('CHF') THEN 'CHF' ELSE 'OTHERS' END AS Currencies, CASE WHEN a.ATTRIBUTE18 in ('3M','6M','12M','2Y','3Y') THEN '<3yrs' WHEN a.ATTRIBUTE18 in ('5Y','7Y','10Y') THEN '3-10yrs' WHEN a.ATTRIBUTE18 in ('20Y','30Y','50Y','100Y') THEN '>10yrs' ELSE 'Undefined*' END AS TERM, a.ATTRIBUTE10 AS "Unit", SUM (VALUE1) / 1000 AS "PNL" FROM CDWUSER.U_GENERIC_DATA a WHERE a.cob_date IN ('2018-02-16') AND analytic_group = 'FDSF' AND analytics = 'SENSITIVITIES' AND ATTRIBUTE1 IN ('IR') GROUP BY a.ATTRIBUTE9, CASE WHEN a.ATTRIBUTE8 IN ('USD') THEN 'USD' WHEN a.ATTRIBUTE8 IN ('EUR') THEN 'EUR' WHEN a.ATTRIBUTE8 IN ('GBP') THEN 'GBP' WHEN a.ATTRIBUTE8 IN ('JPY') THEN 'JPY' WHEN a.ATTRIBUTE8 IN ('CHF') THEN 'CHF' ELSE 'OTHERS' END, CASE WHEN a.ATTRIBUTE18 in ('3M','6M','12M','2Y','3Y') THEN '<3yrs' WHEN a.ATTRIBUTE18 in ('5Y','7Y','10Y') THEN '3-10yrs' WHEN a.ATTRIBUTE18 in ('20Y','30Y','50Y','100Y') THEN '>10yrs' ELSE 'Undefined*' END , a.ATTRIBUTE10