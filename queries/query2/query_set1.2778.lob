SELECT a.COB_DATE, (sum(a.USD_PV10_BENCH) + sum(a.USD_CREDIT_PV10PCT)) as CRPV10, sum(a.USD_EXPOSURE) as USD_EXPOSURE FROM CDWUSER.U_DM_EQ a WHERE a.COB_DATE in ('2018-02-28','2018-01-31') AND a.CCC_BANKING_TRADING = 'TRADING' AND a.CCC_DIVISION = 'INSTITUTIONAL EQUITY DIVISION' AND a.CCC_PRODUCT_LINE = 'CONVERTIBLE PRODUCTS' AND a.PRODUCT_TYPE_CODE in ('ASCOT','BOND','CONVRT') GROUP BY a.COB_DATE