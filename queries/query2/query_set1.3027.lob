SELECT p.cob_date as pnl_date, 'TOTAL' as Region, case when p.financial_element in ('AGENCY', 'MSCAP FEES', 'MSCI FEES', 'RISK') then 'Commissions' else 'Trading' end as Commissions, round(sum(p.daily_pl/1000000),3) as daily_pnl, round(sum(p.ytd_pl/1000000),2) as ytd_pnl FROM cdwuser.u_pct_pnl_current p WHERE p.ccc_division = 'INSTITUTIONAL EQUITY DIVISION' AND p.cob_date >= '2017-03-01' AND p.cob_date <= '2018-02-28' AND NOT p.cob_date='2017-01-03' AND (p.account_purpose_code <> 'J' OR p.account_purpose_code IS NULL) GROUP BY p.cob_date, case when p.financial_element in ('AGENCY', 'MSCAP FEES', 'MSCI FEES', 'RISK') then 'Commissions' else 'Trading' end UNION ALL SELECT p.cob_date as pnl_date, 'TOTAL excl. CVA, DVA, TCM' as Region, case when p.financial_element in ('AGENCY', 'MSCAP FEES', 'MSCI FEES', 'RISK') then 'Commissions' else 'Trading' end as Commissions, round(sum(p.daily_pl/1000000),3) as daily_pnl, round(sum(p.ytd_pl/1000000),2) as ytd_pnl FROM cdwuser.u_pct_pnl_current p WHERE p.ccc_division = 'INSTITUTIONAL EQUITY DIVISION' AND p.cob_date >= '2017-03-01' AND p.cob_date <= '2018-02-28' AND NOT p.cob_date='2017-01-03' AND p.ccc_strategy NOT IN ('MS CVA MPE - DERIVATIVES','MS CVA MNE - DERIVATIVES','MS DVA STR NOTES IED','TCM ALLOCATION IED','TCM ALLOCATION OFFSET','PB - TCM ALLOCATION','OTHER - TCM ALLOCATION','DERIVS - TCM ALLOCATION','CASH - TCM ALLOCATION','IWM - TCM ALLOCATION') AND (p.account_purpose_code <> 'J' OR p.account_purpose_code IS NULL) GROUP BY p.cob_date, case when p.financial_element in ('AGENCY', 'MSCAP FEES', 'MSCI FEES', 'RISK') then 'Commissions' else 'Trading' end