SELECT v.COB_DATE, v.ACCOUNT, 1000 * sum(coalesce(v.USD_DELTA,0)) as NRAMV, 1000 * sum(coalesce(v.USD_EQ_GAMMA,0)) as GAMMA, 1000 * sum(coalesce(v.USD_EQ_KAPPA,0)) as KAPPA, 1000 * sum(coalesce(v.USD_EQ_KAPPA_PLS100BP_TIMEADJ,0)) as TA_KAPPA, 0.1 * sum(coalesce(v.USD_IR_RHO,0)) as PV01, 1000 * sum(coalesce(v.SLIDE_EQ_MIN_30_USD,0)+0.3 * coalesce(v.USD_DELTA,0)) as D30, 1000 * sum(coalesce(v.SLIDE_EQ_MIN_20_USD,0)+0.2 * coalesce(v.USD_DELTA,0)) as D20, 1000 * sum(coalesce(v.SLIDE_EQ_MIN_10_USD,0)+0.1 * coalesce(v.USD_DELTA,0)) as D10, 1000 * sum(coalesce(v.SLIDE_EQ_MIN_05_USD,0)+0.05 * coalesce(v.USD_DELTA,0)) as D5, 1000 * sum(coalesce(v.SLIDE_EQ_PLS_05_USD,0)-0.05 * coalesce(v.USD_DELTA,0)) as P5, 1000 * sum(coalesce(v.SLIDE_EQ_PLS_10_USD,0)-0.1 * coalesce(v.USD_DELTA,0)) as P10, 1000 * sum(coalesce(v.SLIDE_EQ_PLS_20_USD,0)-0.2 * coalesce(v.USD_DELTA,0)) as P20 FROM CDWUSER.U_EXP_MSR v WHERE v.CCC_DIVISION ='INSTITUTIONAL EQUITY DIVISION' AND v.SILO_SRC ='IED' AND v.CCC_BANKING_TRADING ='TRADING' AND v.COB_DATE = '2018-02-28' AND v.account in ('074000YJ', '0S74000YJ') GROUP BY v.COB_DATE, v.ACCOUNT