SELECT CONCAT('Q',CONCAT(EXTRACT(QUARTER FROM COB_DATE),SUBSTRING(EXTRACT(YEAR FROM COB_DATE) FROM 3 for 2))) AS QUARTER, TIME_BUCKET_ANNUAL||'.'||TIME_BUCKET_QUARTER AS EXP_DATE_QUARTER, COB_DATE, CASE WHEN (PRODUCT_TYPE_CODE = 'PRECIOUSMETAL' OR CCC_PRODUCT_LINE = 'PRECIOUS METALS') THEN 'PRECIOUSMETAL' WHEN (PRODUCT_TYPE_CODE = 'BASEMETAL' OR (CCC_PRODUCT_LINE ='COMMOD LEGACY TRADING' AND CCC_STRATEGY IN ('BASE METALS'))) THEN 'BASEMETAL' WHEN (PRODUCT_TYPE_CODE IN ('COAL','COAL-US','DRY FREIGHT','EMISSIONS-EU','IRON ORE') OR (CCC_PRODUCT_LINE ='COMMOD LEGACY TRADING' AND CCC_STRATEGY IN ('BULKS'))) THEN 'BULKS' END AS PRODUCT_TYPE_GROUP, CASE WHEN PROD_POS_NAME_DESCRIPTION IN ('GLD','SLV') THEN 'ETF' ELSE 'PHYSICAL' END AS ETF_FLAG, SUM(RAW_CM_DELTA) AS RAW_DELTA, SUM(USD_CM_DELTA) AS DOLLAR_DELTA, SUM(RAW_CM_KAPPA) AS RAW_VEGA, SUM(USD_CM_KAPPA) AS DOLLAR_VEGA FROM CDWUSER.U_CM_MSR WHERE (CCC_DIVISION='COMMODITIES' OR (CCC_DIVISION = 'FIXED INCOME DIVISION' AND CCC_BUSINESS_AREA = 'COMMODITIES')) AND (CCC_BUSINESS_AREA = ('METALS') OR (CCC_PRODUCT_LINE = 'PRECIOUS METALS' OR (CCC_PRODUCT_LINE ='COMMOD LEGACY TRADING' AND CCC_STRATEGY IN ('BULKS','BASE METALS')))) AND CCC_BUSINESS_AREA NOT IN ('MS CVA MNE - COMMOD') AND /*CCC_PL_REPORTING_REGION = 'EMEA' AND*/ /*CCC_TRD_BOOK <> 'DEVELOPMENT' AND*/ COB_DATE IN ('2018-02-28','2018-02-27','2018-01-31','2017-12-29','2017-09-29','2017-06-30') AND PRODUCT_TYPE_CODE NOT IN ( 'INTEREST RATE', 'INFLATION', 'TBD', 'MISC', 'CVA', 'FVA', 'CURRENCY','FUND','CLEAN FREIGHT', 'DIRTY FREIGHT') AND BOOK NOT IN ('Y10425', 'Y10426') GROUP BY QUARTER, EXP_DATE_QUARTER, COB_DATE, PRODUCT_TYPE_GROUP, ETF_FLAG