select cob_date, case when currency_of_measure = 'EUR' then 'EUR' when currency_of_measure = 'GBP' then 'GBP' when currency_of_measure = 'JPY' then 'JPY' else 'OTHER' end as ccy_group, case when ( product_hierarchy_level7 = 'FX_SPOT/FORWARDS' and vertical_system not like '%STS%' ) then sum(a.USD_IR_UNIFIED_PV01) when ( curve_name in ('fxusdeur','fxusdgbp','fxusdjpy','fxusdaud', 'fxusdchf', 'fxusdnok', 'fxusdsek', 'fxusddkk', 'fxusdcad', 'fxusdkrw', 'fxusdkr1', 'fxusdkr2', 'fxusdmyr', 'fxusdbr1', 'fxusdhkd', 'fxusdhuf', 'fxusdidr', 'fxusdmxn', 'fxusdnzd', 'fxusdsgd', 'fxusdthb', 'fxusdtry', 'fxusdtwd', 'fxusdzar', 'fxusdcnh', 'fxusdcny') ) then sum(a.USD_IRPV01SPRD) end as pv01spread from cdwuser.U_IR_MSR a WHERE COB_DATE >= '2018-02-15' AND CCC_BUSINESS_AREA in ('FXEM MACRO TRADING','EM CREDIT TRADING') AND CCC_PL_REPORTING_REGION = 'EMEA' and currency_of_measure in ('AUD','BRL','CAD','CHF','CNY','EUR','GBP','HKD','HUF','IDR','JPY','KRW','MYR','NZD','SGD','TRY','ZAR') group by cob_date, currency_of_measure, curve_name, product_hierarchy_level7, vertical_system