select cob_date, a.CURRENCY_OF_MEASURE, sum(coalesce(a.usd_fx,0)) as fx, abs(sum(coalesce(a.usd_fx,0))) as abs_fx from cdwuser.u_dm_eq a  WHERE (a.COB_DATE = '2018-02-28' or a.COB_DATE = '2018-02-27') and a.IS_UK_GROUP = 'Y' AND a.ccc_division in ('INSTITUTIONAL EQUITY DIVISION') AND A.CCC_BANKING_TRADING IN ('TRADING') and coalesce(a.usd_fx,0)<>0 and a.currency_of_measure is not null group by cob_date, a.CURRENCY_OF_MEASURE