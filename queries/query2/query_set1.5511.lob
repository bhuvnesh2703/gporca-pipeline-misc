SELECT CASE WHEN a.SPG_DESC in ('RMBS PRIME RESIDUAL', 'RMBS PRIME SECURITY','RMBS PRIME DEFAULT SWAP', 'RMBS PRIME IO','RMBS PRIME LOAN', 'RMBS PRIME RESIDUAL', 'RMBS PRIME SECURITY', 'RMBS PRIME INDEX', 'RMBS PRIME REREMIC') THEN 'PRIME_RMBS' WHEN a.spg_desc IN ('RMBS SUB PRIME SECURITY', 'RMBS SUB PRIME RESIDUAL', 'RMBS ALTA SECURITY', 'RMBS ALTA RESIDUAL','RMBS NON CONFORMING DEFAULT SWAP','RMBS SUB PRIME RESIDUAL','RMBS SUB PRIME SECURITY') THEN 'NONCONFORMING_RMBS' WHEN a.SPG_DESC in ('RMBS CDO') then 'RMBS CDO' WHEN a.SPG_DESC in ('CMBS SECURITY', 'CMBS DEFAULT SWAP', 'CMBS INDEX') then 'CMBS' WHEN a.SPG_DESC in ('ABS OTHER SECURITY') and a.BOOK in ('FIXED STERLING ABS') then 'FIXED STERLING SECURITY' WHEN a.SPG_DESC in ('ABS STUDENT SECURITY', 'ABS EQUIPMENT SECURITY', 'ABS CREDIT CARD SECURITY', 'ABS CREDIT CARD SECURITY', 'ABS AUTO LOAN & SECURITY', 'ABS OTHER SECURITY', 'ABS DEFAULT SWAPS') then 'SME AND CONSUMER' WHEN a.SPG_DESC in ('CORPORATE CDO', 'CORPORATE CLO', 'CORPORATE CDO DEFAULT SWAP') THEN 'CLO' WHEN a.SPG_DESC in ('CORPORATE BONDS','CORPORATE AND CDS') then 'CORPORATE BOND' WHEN a.SPG_DESC in ('CORPORATE DEFAULT SWAP','CORPORATE INDEX') then 'CORPORATE HEDGES' end as EU_CATEGORY, a.COB_DATE, a.COUNTRY_TYPE, a.GROUPED_RATING, SUM (a.USD_EXPOSURE) as EXPOSURE from cdwuser.U_DM_SPG a where a.COB_DATE in ('2018-02-28', '2018-02-21', '2017-12-29', '2017-09-29', '2017-06-30', '2017-03-31') and a.CCC_BUSINESS_AREA = 'SECURITIZED PRODUCTS GRP' AND a.CCC_PL_REPORTING_REGION IN ('EMEA') group by CASE WHEN a.SPG_DESC in ('RMBS PRIME RESIDUAL', 'RMBS PRIME SECURITY','RMBS PRIME DEFAULT SWAP', 'RMBS PRIME IO','RMBS PRIME LOAN', 'RMBS PRIME RESIDUAL', 'RMBS PRIME SECURITY', 'RMBS PRIME INDEX', 'RMBS PRIME REREMIC') THEN 'PRIME_RMBS' WHEN a.spg_desc IN ('RMBS SUB PRIME SECURITY', 'RMBS SUB PRIME RESIDUAL', 'RMBS ALTA SECURITY', 'RMBS ALTA RESIDUAL','RMBS NON CONFORMING DEFAULT SWAP','RMBS SUB PRIME RESIDUAL','RMBS SUB PRIME SECURITY') THEN 'NONCONFORMING_RMBS' WHEN a.SPG_DESC in ('RMBS CDO') then 'RMBS CDO' WHEN a.SPG_DESC in ('CMBS SECURITY', 'CMBS DEFAULT SWAP', 'CMBS INDEX') then 'CMBS' WHEN a.SPG_DESC in ('ABS OTHER SECURITY') and a.BOOK in ('FIXED STERLING ABS') then 'FIXED STERLING SECURITY' WHEN a.SPG_DESC in ('ABS STUDENT SECURITY', 'ABS EQUIPMENT SECURITY', 'ABS CREDIT CARD SECURITY', 'ABS CREDIT CARD SECURITY', 'ABS AUTO LOAN & SECURITY', 'ABS OTHER SECURITY', 'ABS DEFAULT SWAPS') then 'SME AND CONSUMER' WHEN a.SPG_DESC in ('CORPORATE CDO', 'CORPORATE CLO', 'CORPORATE CDO DEFAULT SWAP') THEN 'CLO' WHEN a.SPG_DESC in ('CORPORATE BONDS','CORPORATE AND CDS') then 'CORPORATE BOND' WHEN a.SPG_DESC in ('CORPORATE DEFAULT SWAP','CORPORATE INDEX') then 'CORPORATE HEDGES' end, a.COB_DATE, a.COUNTRY_TYPE, a.GROUPED_RATING