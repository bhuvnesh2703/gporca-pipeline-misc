SELECT COB_DATE, DIVISION, SUM (DELTA) AS DELTA, PRODUCT_DESCRIPTION_DECOMP FROM ( SELECT d.COB_DATE, d.DIVISION, d.ISSUE_ID_DECOMP, SUM (COALESCE (d.USD_EQ_DELTA_DECOMP,0)) AS Delta, CASE WHEN d.PRODUCT_DESCRIPTION_DECOMP IS NULL THEN 'UNDEFINED' ELSE d.PRODUCT_DESCRIPTION_DECOMP END PRODUCT_DESCRIPTION_DECOMP FROM cdwuser.U_DECOMP_MSR d WHERE d.cob_date in ('2018-02-28') and ISSUER_COUNTRY_CODE_DECOMP = 'ZAF' AND d.CCC_BANKING_TRADING = 'TRADING' AND d.division IN ('FID', 'IED') AND (d.CASH_ISSUE_TYPE <> 'COMM' OR d.CASH_ISSUE_TYPE IS NULL) AND ABS (d.USD_EQ_DELTA_DECOMP) > 0 GROUP BY d.COB_DATE, d.DIVISION, d.ISSUE_ID_DECOMP, d.PRODUCT_DESCRIPTION_DECOMP ) x GROUP BY COB_DATE, DIVISION, PRODUCT_DESCRIPTION_DECOMP