SELECT COB_DATE, ccc_pl_reporting_region, UK_Group, SUM (DELTA) AS DELTA, SUM (Abs_Delta) AS GROSS_DELTA from ( SELECT d.COB_DATE, d.ISSUE_ID_DECOMP, ccc_pl_reporting_region, CASE WHEN LE_GROUP = 'UK' THEN 'UK Group' ELSE 'Non Uk Group' END AS UK_Group, SUM (COALESCE (d.USD_EQ_DELTA_DECOMP, 0)) AS Delta, ABS (SUM (COALESCE (d.USD_EQ_DELTA_DECOMP, 0))) AS Abs_Delta FROM CDWUSER.U_DECOMP_MSR d WHERE COB_DATE in ('2018-02-28','2018-02-21') AND d.CCC_BANKING_TRADING = 'TRADING' AND d.SILO_SRC = 'IED' AND d.DIVISION = 'IED' AND (d.CASH_ISSUE_TYPE <> 'COMM' OR d.CASH_ISSUE_TYPE IS NULL) GROUP BY d.COB_DATE, d.ISSUE_ID_DECOMP, ccc_pl_reporting_region, CASE WHEN LE_GROUP = 'UK' THEN 'UK Group' ELSE 'Non Uk Group' END ) x GROUP BY COB_DATE, ccc_pl_reporting_region, UK_Group