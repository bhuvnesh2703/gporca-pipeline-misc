SELECT a.COB_DATE, a.CCC_BUSINESS_AREA, A.CCC_PRODUCT_LINE, a.curve_name, CASE WHEN COALESCE(a.CURVE_TYPE,'') = 'CPCRMNE' THEN 'MS CDS' WHEN COALESCE(a.CURVE_TYPE,'') = 'MS_SECCPM' THEN 'MS Bond' WHEN COALESCE(a.CURVE_TYPE,'') IN ('CPCRFUND', 'CPCR_MPEFUND') THEN 'Dealer Bond' WHEN a.PRODUCT_SUB_TYPE_CODE IN ('MPE', 'MPE_CVA', 'MNE', 'MNE_CVA', 'MNE_CP', 'MPE_PROXY', 'MPE_FVA', 'MPE_FVA_RAW', 'MNE_FVA_NET', 'MNE_FVA') THEN 'MPE CVA' WHEN a.PRODUCT_TYPE_CODE IN ('CDSOPTIDX', 'CRDBSKT', 'CRDINDEX', 'LOANINDEX', 'MUNICDX') THEN 'INDEX' ELSE 'SN' END AS CVA_Type_Flag,     CASE          WHEN BOOK = 'EQCVA' AND BU_RISK_SYSTEM LIKE 'STS%' THEN POSITION_ULT_ISSUER_PARTY_DARWIN_NAME         WHEN POSITION_ULTIMATE_CREDIT_PARTY_DARWIN_NAME = 'N/A' THEN POSITION_ULT_ISSUER_PARTY_DARWIN_NAME           WHEN A.CURVE_NAME='cpcr_voya_bond' THEN 'VOYA FINANCIAL, INC.'         ELSE POSITION_ULT_ISSUER_PARTY_DARWIN_NAME      END AS CTPY_NAME, CASE WHEN a.BOOK in ('COMET','COMPR','COMRC','COMNA','COMLN') THEN 'STRAT BOOK' ELSE 'NON-STRAT BOOK' END AS STRAT_TYPE, SUM(a.USD_PV10_BENCH) AS BPV10, sum(SLIDE_PV_PLS_10PCT_USD) as PV10 FROM cdwuser.U_CR_MSR a WHERE (a.cob_date = '2018-02-28') AND CCC_TAPS_COMPANY in ('0302') AND   (a.CCC_BUSINESS_AREA IN ('CPM TRADING (MPE)','CPM', 'CREDIT', 'MS CVA MNE - FID', 'MS CVA MNE - COMMOD')  OR a.CCC_STRATEGY IN ('MS CVA MPE - DERIVATIVES', 'MS CVA MNE - DERIVATIVES','EQ XVA HEDGING')) AND (a.USD_PV10_BENCH IS NOT NULL or SLIDE_PV_PLS_10PCT_USD  is not NULL) GROUP BY a.COB_DATE, a.CCC_BUSINESS_AREA, A.CCC_PRODUCT_LINE, a.curve_name, CASE WHEN COALESCE(a.CURVE_TYPE,'') = 'CPCRMNE' THEN 'MS CDS' WHEN COALESCE(a.CURVE_TYPE,'') = 'MS_SECCPM' THEN 'MS Bond' WHEN COALESCE(a.CURVE_TYPE,'') IN ('CPCRFUND', 'CPCR_MPEFUND') THEN 'Dealer Bond' WHEN a.PRODUCT_SUB_TYPE_CODE IN ('MPE', 'MPE_CVA', 'MNE', 'MNE_CVA', 'MNE_CP', 'MPE_PROXY', 'MPE_FVA', 'MPE_FVA_RAW', 'MNE_FVA_NET', 'MNE_FVA') THEN 'MPE CVA' WHEN a.PRODUCT_TYPE_CODE IN ('CDSOPTIDX', 'CRDBSKT', 'CRDINDEX', 'LOANINDEX', 'MUNICDX') THEN 'INDEX' ELSE 'SN' END,     CASE          WHEN BOOK = 'EQCVA' AND BU_RISK_SYSTEM LIKE 'STS%' THEN POSITION_ULT_ISSUER_PARTY_DARWIN_NAME         WHEN POSITION_ULTIMATE_CREDIT_PARTY_DARWIN_NAME = 'N/A' THEN POSITION_ULT_ISSUER_PARTY_DARWIN_NAME           WHEN A.CURVE_NAME='cpcr_voya_bond' THEN 'VOYA FINANCIAL, INC.'         ELSE POSITION_ULT_ISSUER_PARTY_DARWIN_NAME     END, CASE WHEN a.BOOK in ('COMET','COMPR','COMRC','COMNA','COMLN') THEN 'STRAT BOOK' ELSE 'NON-STRAT BOOK' END