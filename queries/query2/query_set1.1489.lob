SELECT COB_DATE, CASE WHEN PRODUCT_TYPE_NAME IN ('FUTURE','LISTED OPTION') THEN 'COMEX' ELSE 'LBMA' END AS PRODUCT_TYPE, PRODUCT_SUB_TYPE_CODE, SUM(USD_CM_DELTA)/1000 AS Delta FROM CDWUSER.U_CM_MSR WHERE COB_DATE IN ('2018-02-28','2018-02-27','2018-01-31','2017-12-29','2017-09-29','2017-06-30') AND (CCC_DIVISION='COMMODITIES' OR (CCC_DIVISION = 'FIXED INCOME DIVISION' AND CCC_BUSINESS_AREA = 'COMMODITIES')) AND CCC_BUSINESS_AREA NOT IN ('OLYMPUS', 'INVESTOR BUSINESS') AND PRODUCT_TYPE_CODE NOT IN ('CURRENCY', 'INTEREST RATE') AND CCC_PRODUCT_LINE IN ('PRECIOUS METALS') AND BOOK NOT IN ('Y10425','Y10426') AND PRODUCT_SUB_TYPE_CODE IN ('GOLD') GROUP BY COB_DATE, CASE WHEN PRODUCT_TYPE_NAME IN ('FUTURE','LISTED OPTION') THEN 'COMEX' ELSE 'LBMA' END, PRODUCT_SUB_TYPE_CODE