select a.ISSUER_COUNTRY_CODE,COB_DATE,SUM(usd_pv01sprd) AS spv01 from CDWUSER.U_DM_CC a where a.COB_DATE >= '2018-01-31' and a.ISSUER_COUNTRY_CODE in ('ARE','IRQ','QAT','SAU','LBN','ISR','EGY','ISR','KWT','BHR','JOR','OMN') and ccc_business_area IN ('FXEM MACRO TRADING','EM CREDIT TRADING') AND CCC_PL_REPORTING_REGION in ('EMEA') group by COB_DATE, ISSUER_COUNTRY_CODE