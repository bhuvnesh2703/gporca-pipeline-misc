SELECT     a.COB_DATE,     SUM(coalesce(CAST(a.USD_EQ_KAPPA AS DOUBLE PRECISION), 0)) AS USD_EQ_KAPPA,     SUM(coalesce(CAST(a.SLIDE_EQ_MIN_30_USD AS DOUBLE PRECISION), 0)) AS SLIDE_EQ_MIN_30_USD,     SUM(coalesce(CAST(a.SLIDE_EQ_MIN_10_USD AS DOUBLE PRECISION), 0)) AS SLIDE_EQ_MIN_10_USD FROM     CDWUSER.U_EQ_MSR_INTRPLT a WHERE  (a.COB_DATE = '2018-02-28' or a.COB_DATE = '2018-02-27') and A.CCC_PL_REPORTING_REGION in ('JAPAN') AND      (a.VAR_EXCL_FL <> 'Y') AND     (a.ccc_business_area <> 'INTERNATIONAL WEALTH MGMT') AND     a.CCC_DIVISION = 'INSTITUTIONAL EQUITY DIVISION' AND       CCC_BANKING_TRADING IN ('TRADING') GROUP BY     A.COB_DATE