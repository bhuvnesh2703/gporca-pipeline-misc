SELECT a.COB_DATE, CASE WHEN FID1_INDUSTRY_NAME_LEVEL2 LIKE 'FINANCIAL%' THEN 'FINANCIALS' WHEN (FID1_INDUSTRY_NAME_LEVEL2 IN('AGENCIES','SOVEREIGNS') OR FID1_INDUSTRY_NAME_LEVEL2 = 'UNDEFINED') THEN 'OTHERS' ELSE FID1_INDUSTRY_NAME_LEVEL2 END as Sector, sum(a.USD_EQ_DELTA_DECOMP) as USD_EQ_DELTA, abs(sum(a.USD_EQ_DELTA_DECOMP)) as ABS_USD_EQ_DELTA FROM CDWUSER.U_DM_EQ a WHERE a.COB_DATE in ('2018-02-28','2018-02-27') AND a.CCC_BANKING_TRADING = 'TRADING' AND a.CCC_DIVISION = 'INSTITUTIONAL EQUITY DIVISION' AND a.USD_EQ_DELTA_DECOMP <> 0 GROUP BY a.COB_DATE, CASE WHEN FID1_INDUSTRY_NAME_LEVEL2 LIKE 'FINANCIAL%' THEN 'FINANCIALS' WHEN (FID1_INDUSTRY_NAME_LEVEL2 IN('AGENCIES','SOVEREIGNS') OR FID1_INDUSTRY_NAME_LEVEL2 = 'UNDEFINED') THEN 'OTHERS' ELSE FID1_INDUSTRY_NAME_LEVEL2 END