SELECT a.COB_DATE, a.VERTICAL_SYSTEM, A.TERM_BUCKET, SUM (A.USD_FX_KAPPA) AS FX_KAPPA, a.PRODUCT_TYPE_CODE, CASE WHEN (a.VERTICAL_SYSTEM LIKE '%EQUITY%' AND a.PRODUCT_TYPE_CODE NOT IN ('GVTBOND' , 'RATEFUT')) THEN 'OTHER EQ' ELSE 'OTHER FID' END AS CATEGORY, CASE WHEN A.PRODUCT_TYPE_CODE IN ('GVTBOND' ) THEN ' Government Bonds' WHEN A.PRODUCT_TYPE_CODE LIKE '%RATEFUT%' THEN 'Rate Futures' ELSE 'OTHERS' END AS PRODUCT_TYPE_CODE FROM CDWUSER.U_FX_MSR A WHERE COB_DATE = '2015-10-27' AND A.CCC_DIVISION = 'INSTITUTIONAL EQUITY DIVISION' AND CURRENCY_OF_MEASURE LIKE '%BR%' AND CCC_BUSINESS_AREA NOT LIKE '%CPM%' GROUP BY a.COB_DATE, a.PRODUCT_TYPE_CODE, A.TERM_BUCKET, CASE WHEN A.PRODUCT_TYPE_CODE IN ('GVTBOND' ) THEN ' Government Bonds' WHEN A.PRODUCT_TYPE_CODE LIKE '%RATEFUT%' THEN 'Rate Futures' ELSE 'OTHERS' END, a.vertical_system, CASE WHEN (a.VERTICAL_SYSTEM LIKE '%EQUITY%' AND a.PRODUCT_TYPE_CODE NOT IN ('GVTBOND' , 'RATEFUT')) THEN 'OTHER EQ' ELSE 'OTHER FID' END