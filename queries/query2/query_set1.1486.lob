SELECT COB_DATE, CASE WHEN PRODUCT_SUB_TYPE_CODE = 'GOLD' OR PRODUCT_SUB_TYPE_CODE = 'SILVER' THEN PRODUCT_SUB_TYPE_CODE ELSE 'OTHER' END AS PRODUCT_TYPE_CODE, SUM(USD_CM_DELTA) AS DOLLAR_DELTA FROM CDWUSER.U_CM_MSR WHERE (CCC_DIVISION='COMMODITIES' OR (CCC_DIVISION = 'FIXED INCOME DIVISION' AND CCC_BUSINESS_AREA = 'COMMODITIES')) AND CCC_PRODUCT_LINE = 'PRECIOUS METALS' AND CCC_BUSINESS_AREA NOT IN ('MS CVA MNE - COMMOD') AND /*CCC_PL_REPORTING_REGION = 'EMEA' AND*/ /*CCC_TRD_BOOK <> 'DEVELOPMENT' AND*/ COB_DATE BETWEEN ('2018-01-29') AND ('2018-02-28') AND PRODUCT_TYPE_CODE NOT IN ( 'INTEREST RATE', 'INFLATION', 'TBD', 'MISC', 'CVA', 'FVA', 'CURRENCY','FUND','CLEAN FREIGHT', 'DIRTY FREIGHT') AND BOOK NOT IN ('Y10425', 'Y10426') GROUP BY COB_DATE, CASE WHEN PRODUCT_SUB_TYPE_CODE = 'GOLD' OR PRODUCT_SUB_TYPE_CODE = 'SILVER' THEN PRODUCT_SUB_TYPE_CODE ELSE 'OTHER' END