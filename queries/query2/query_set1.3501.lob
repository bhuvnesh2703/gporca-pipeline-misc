SELECT DISTINCT b.cob_date, case when b.fill_forward_date is null THEN NULL ELSE 'SCN' END AS SURFACE_FF_TYPE, B.SCENARIO_NAME, COUNT (DISTINCT b.ROWID) AS surface_rows FROM cdwuser.U_ied_surface b full join cdwuser.U_IED_VIEW a on a.POSITION_ID=b.POSITION_ID and a.PROCESS_ID=b.PROCESS_ID and a.COB_DATE=b.COB_DATE where  (a.COB_DATE = '2018-02-28')  and a.CCC_DIVISION in ('INSTITUTIONAL EQUITY DIVISION') GROUP BY b.cob_date, case when b.fill_forward_date is null  THEN NULL ELSE 'SCN' END, B.SCENARIO_NAME union all SELECT DISTINCT b.cob_date, case when b.fill_forward_date is null THEN NULL ELSE 'SCN' END AS SURFACE_FF_TYPE, B.SCENARIO_NAME, COUNT (DISTINCT b.ROWID) AS surface_rows FROM cdwuser.U_ied_surface b full join cdwuser.U_IED_VIEW a on a.POSITION_ID=b.POSITION_ID and a.PROCESS_ID=b.PROCESS_ID and a.COB_DATE=b.COB_DATE where  (a.COB_DATE = '2018-01-31') and a.CCC_DIVISION in ('INSTITUTIONAL EQUITY DIVISION') GROUP BY b.cob_date, case when b.fill_forward_date is null  THEN NULL ELSE 'SCN' END, B.SCENARIO_NAME