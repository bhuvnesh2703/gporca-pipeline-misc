SELECT a.cob_date , PRODUCT_TYPE_CODE , case when ccc_product_line='CPM TRADING' then 'XVA HEDGING' else ccc_product_line end as ccc_product_line , a.CURRENCY_OF_MEASURE AS CURRENCY_CODE , SUM (a.USD_EXPOSURE) AS usd_exposure FROM cdwuser.u_exp_msr a WHERE COB_DATE in ('2018-02-28','2018-01-31','2017-12-29','2017-11-30','2017-10-31','2017-09-29','2017-08-31','2017-07-31','2017-06-30','2017-05-31','2017-04-28','2017-03-31','2017-02-28') AND a.CCC_DIVISION IN ('FIXED INCOME DIVISION') AND a.CCC_PL_REPORTING_REGION IN ('AMERICAS') AND PARENT_LEGAL_ENTITY = '0302(G)' group by a.cob_date , a.PRODUCT_TYPE_CODE , a.CCC_PRODUCT_LINE , a.CURRENCY_OF_MEASURE