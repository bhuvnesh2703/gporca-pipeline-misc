SELECT     A.COB_DATE,     A.CURRENCY_OF_MEASURE,      SUM (A.USD_DELTA) AS EQUITY_DELTA FROM cdwuser.U_EQ_MSR A WHERE     COB_DATE IN ('2018-02-28', '2018-02-27') AND     A.CURRENCY_OF_MEASURE IN ('MXN', 'CAD', 'EUR', 'USD', 'JPY', 'AUD') AND     A.VAR_EXCL_FL = 'N' AND     A.CCC_TAPS_COMPANY IN ('8840', '8890') GROUP BY     A.COB_DATE,     A.CURRENCY_OF_MEASURE