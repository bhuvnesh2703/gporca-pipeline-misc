select A.COB_DATE, A.CCC_TAPS_COMPANY, A.CCC_BUSINESS_AREA, A.CCC_PRODUCT_LINE, A.FID1_INDUSTRY_NAME_LEVEL1, A.PRODUCT_DESCRIPTION, A.VERTICAL_SYSTEM, a.PRODUCT_TYPE_CODE, a.CCC_PL_REPORTING_REGION, A.POSITION_ULT_ISSUER_PARTY_DARWIN_NAME, a.LE_GROUP, SUM(A.USD_EXPOSURE) AS NETEXP, SUM(A.USD_NOTIONAL) AS NOTIONAL from CDWUSER.u_EXP_MSR a WHERE A.cob_date in ('2018-02-28','2018-02-21') AND a.CCC_DIVISION = 'FIXED INCOME DIVISION' AND A.CCC_BUSINESS_AREA NOT IN ('LENDING', 'CPM TRADING (MPE)', 'MS CVA MNE - FID') AND A.FID1_INDUSTRY_NAME_LEVEL1 in ('SOVEREIGN', 'CORPORATES: FINANCIALS') and A.ISSUER_COUNTRY_CODE = 'GRC' group by A.COB_DATE, A.CCC_TAPS_COMPANY, A.CCC_BUSINESS_AREA, A.CCC_PRODUCT_LINE, A.FID1_INDUSTRY_NAME_LEVEL1, A.PRODUCT_DESCRIPTION, A.VERTICAL_SYSTEM, a.PRODUCT_TYPE_CODE, a.CCC_PL_REPORTING_REGION, A.POSITION_ULT_ISSUER_PARTY_DARWIN_NAME, a.LE_GROUP