SELECT     a.COB_DATE,     a.CURRENCY_OF_MEASURE,     SUM (CAST(a.USD_IR_KAPPA AS DOUBLE PRECISION)) / 10 AS USD_IR_KAPPA FROM cdwuser.U_IR_MSR_INTRPLT a WHERE (a.COB_DATE = '2018-02-28' or a.COB_DATE = '2018-01-31') and A.CCC_PL_REPORTING_REGION in ('JAPAN','ASIA PACIFIC') AND A.CCC_TAPS_COMPANY in ('0302','0347','0853','4043','4298','4863','6120', '6899','6837','6893','4044','5869','0856','6325','0301','0893','0993') AND      (a.VAR_EXCL_FL <> 'Y') AND     a.USD_IR_KAPPA IS NOT NULL AND     a.CCC_DIVISION IN ('FIXED INCOME DIVISION') AND     /*a.CCC_BUSINESS_AREA NOT IN ('COMMODITIES') AND*/       CCC_BANKING_TRADING='TRADING' GROUP BY     a.COB_DATE,     a.CURRENCY_OF_MEASURE