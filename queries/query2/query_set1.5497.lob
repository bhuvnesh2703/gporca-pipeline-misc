SELECT cob_date, a.ccc_division, CURRENCY_OF_RISK_CCY1 AS RISK_CURRENCY_COMBINED, EXPIRATION_DATE, SUM (SLIDE_FXOPTVAR_MIN_20PCT_USD) AS neg20, SUM (SLIDE_FXKAPPA_MIN_20PCT_USD) AS neg20kappa, SUM (SLIDE_FXOPTVAR_MIN_10PCT_USD) AS neg10, SUM (SLIDE_FXKAPPA_MIN_10PCT_USD) AS neg10kappa, SUM (SLIDE_FXOPTVAR_MIN_05PCT_USD) AS neg5, SUM (SLIDE_FXKAPPA_MIN_05PCT_USD) AS neg5kappa, SUM (SLIDE_FXOPTVAR_MIN_03PCT_USD) AS neg3, SUM (SLIDE_FXKAPPA_MIN_03PCT_USD) AS neg3kappa, SUM (SLIDE_FXOPTVAR_MIN_01PCT_USD) AS neg1, SUM (SLIDE_FXKAPPA_MIN_01PCT_USD) AS neg1kappa, SUM (SLIDE_FXOPTVAR_MIN_0P5PCT_USD) AS neg05, SUM (SLIDE_FXKAPPA_MIN_0P5PCT_USD) AS neg05kappa, SUM (SLIDE_FXOPTVAR_PLS_0P5PCT_USD) AS pos05, SUM (SLIDE_FXKAPPA_PLS_0P5PCT_USD) AS pos05kappa, SUM (SLIDE_FXOPTVAR_PLS_01PCT_USD) AS pos1, SUM (SLIDE_FXKAPPA_PLS_01PCT_USD) AS pos1kappa, SUM (SLIDE_FXOPTVAR_PLS_03PCT_USD) AS pos3, SUM (SLIDE_FXKAPPA_PLS_03PCT_USD) AS pos3kappa, SUM (SLIDE_FXOPTVAR_PLS_05PCT_USD) AS pos5, SUM (SLIDE_FXKAPPA_PLS_05PCT_USD) AS pos5kappa, SUM (SLIDE_FXOPTVAR_PLS_10PCT_USD) AS pos10, SUM (SLIDE_FXKAPPA_PLS_10PCT_USD) AS pos10kappa, SUM (SLIDE_FXOPTVAR_PLS_20PCT_USD) AS pos20, SUM (SLIDE_FXKAPPA_PLS_20PCT_USD) AS pos20kappa, 0 AS base, SUM (SLIDE_FXKAPPA_USD) AS basekappa FROM cdwuser.U_FX_MSR a where cob_date in ('2018-02-28') AND currency_pair LIKE '%SAR%' AND ccc_division IN ('FIXED INCOME DIVISION') AND ccc_business_area NOT IN ('LENDING', 'OTC CLIENT CLEARING', 'CPM', 'DSP - CREDIT') AND CCC_BANKING_TRADING_LOCALREG = 'TRADING' AND EXPIRATION_DATE > '2018-02-28' GROUP BY cob_date, a.ccc_division, CURRENCY_OF_RISK_CCY1, EXPIRATION_DATE