WITH BASE_DATA AS(     select          a.COB_DATE,         a.ISSUER_COUNTRY_CODE,         a.POSITION_ULT_ISSUER_PARTY_DARWIN_NAME as ULT_NAME,         case             when A.COB_DATE IN ('2018-02-28') THEN             COALESCE(sum(a.USD_EXPOSURE),0)         ELSE 0 END AS usd_NET_exposure,         case             when A.COB_DATE IN ('2018-02-28') THEN             ABS(COALESCE(sum(a.USD_EXPOSURE),0))         ELSE 0 END AS abs_usd_NET_exposure,         case             when A.COB_DATE IN ('2018-02-28') THEN             COALESCE(sum(a.USD_EXPOSURE),0)         ELSE (-1)*COALESCE(sum(a.USD_EXPOSURE),0) END AS change_usd_NET_exposure     FROM         cdwuser.U_CR_MSR a     WHERE  (a.COB_DATE = '2018-02-28' or a.COB_DATE = '2018-02-21') and A.CCC_PL_REPORTING_REGION in ('ASIA PACIFIC') AND        CCC_BANKING_TRADING='TRADING' AND         a.CCC_DIVISION = 'INSTITUTIONAL EQUITY DIVISION' and         a.CREDIT_RISK_ISSUER_NAME not in ('REPUBLIC OF INDONESIA', 'STEINHOFF FINANCE HOLDING GMBH',  'BAYER AG','MORGAN STANLEY','UNKNOWN') and         a.CREDIT_RISK_ISSUER_NAME not like 'BERKSHIRE HATHA%' and         a.CREDIT_RISK_ISSUER_NAME not like '%REPUBLIC OF CHINA' and         (PRODUCT_TYPE_CODE in ('SWAP','DEFSWAP','OPTION') or PRODUCT_TYPE_CODE like  '%BOND%' or PRODUCT_TYPE_CODE like 'CONVRT') and         ticker not in ('7048257', '7048274')     group by         a.COB_DATE,         a.ISSUER_COUNTRY_CODE,         a.POSITION_ULT_ISSUER_PARTY_DARWIN_NAME ) SELECT     ISSUER_COUNTRY_CODE,     ULT_NAME,     SUM(usd_NET_exposure) AS usd_NET_exposure,     SUM(change_usd_NET_exposure) AS change_usd_NET_exposure,        SUM(abs_usd_NET_exposure) AS abs_usd_NET_exposure    FROM     BASE_DATA GROUP BY     ISSUER_COUNTRY_CODE,     ULT_NAME ORDER BY     SUM(abs_usd_NET_exposure) DESC FETCH FIRST 10 ROWS ONLY