SELECT CASE WHEN issuer_country_code IN ('ARE','HKG','OMN','SAU','QAT','KWT','BHR','CHN') THEN 'Y' ELSE is_pegged END AS IS_PEGGED, a.ISSUER_COUNTRY_CODE, CASE WHEN (a.ISSUER_COUNTRY_CODE in ('VEN') OR (CCC_STRATEGY IN ('DISTRESSED TRADING1') AND COUNTRY_GROUP NOT IN ('Other'))) THEN 'Distressed' WHEN ISSUER_COUNTRY_CODE IN ('AGO','CMR','ETH','GEO','MNE','RWA','TZA') THEN 'EMEA' WHEN ISSUER_COUNTRY_CODE IN ('ARM') THEN 'CIS' WHEN ISSUER_COUNTRY_CODE IN ('BGD') THEN 'ASIA' ELSE COUNTRY_GROUP END AS COUNTRY_GROUP, /* CASE WHEN ( a.CCC_BUSINESS_AREA IN ('CPM', 'CPM TRADING (MPE)', 'CREDIT') OR a.CCC_STRATEGY IN ('MS CVA MPE - DERIVATIVES', 'MS CVA MNE - DERIVATIVES', 'EQ XVA HEDGING')) THEN 'CVA' ELSE a.CCC_DIVISION END AS CCC_DIVISION, */ a.CCC_DIVISION, SUM(a.USD_EXPOSURE) AS Net_Exposure FROM cdwuser.U_DM_FX a WHERE a.cob_date IN ('2018-02-28') AND a.IS_UK_GROUP = 'Y' AND a.IS_MORGAN_STANLEY='N' AND ccc_business_area not in ('LENDING') AND a.USD_EXPOSURE IS NOT NULL AND CCC_DIVISION NOT IN ('FID DVA','FIC DVA') AND CCC_STRATEGY NOT IN ('MS DVA STR NOTES IED') GROUP BY CASE WHEN issuer_country_code IN ('ARE','HKG','OMN','SAU','QAT','KWT','BHR','CHN') THEN 'Y' ELSE is_pegged END, a.ISSUER_COUNTRY_CODE, CASE WHEN (a.ISSUER_COUNTRY_CODE in ('VEN') OR (CCC_STRATEGY IN ('DISTRESSED TRADING1') AND COUNTRY_GROUP NOT IN ('Other'))) THEN 'Distressed' WHEN ISSUER_COUNTRY_CODE IN ('AGO','CMR','ETH','GEO','MNE','RWA','TZA') THEN 'EMEA' WHEN ISSUER_COUNTRY_CODE IN ('ARM') THEN 'CIS' WHEN ISSUER_COUNTRY_CODE IN ('BGD') THEN 'ASIA' ELSE COUNTRY_GROUP END, /* CASE WHEN ( a.CCC_BUSINESS_AREA IN ('CPM', 'CPM TRADING (MPE)', 'CREDIT') OR a.CCC_STRATEGY IN ('MS CVA MPE - DERIVATIVES', 'MS CVA MNE - DERIVATIVES', 'EQ XVA HEDGING')) THEN 'CVA' ELSE a.CCC_DIVISION END */ a.CCC_DIVISION