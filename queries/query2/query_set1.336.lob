SELECT COB_DATE, PRODUCT_TYPE_CODE, PRODUCT_SUB_TYPE_CODE, CCC_PL_REPORTING_REGION, CCC_TAPS_COMPANY, COUNTRY_CD_OF_RISK, COUNTRY_OF_RISK_REGION, CCC_DIVISION, CCC_BUSINESS_AREA, CCC_PRODUCT_LINE, CCC_STRATEGY, LE_GROUP, SUM(USD_DELTA) as delta, CASE WHEN COUNTRY_CD_OF_RISK IN ('AUT', 'BEL', 'CYP', 'EST', 'FIN', 'FRA', 'DEU', 'LVA', 'LTU', 'LUX', 'NLD', 'SVK', 'SVN', 'GRC', 'IRL', 'ESP', 'PRT', 'ITA', 'GBR') THEN 'Euro_Zone' ELSE 'Other' END AS country_region FROM CDWUSER.U_EQ_MSR WHERE COB_DATE in ('2018-02-28','2018-02-21') GROUP BY COB_DATE, PRODUCT_TYPE_CODE, PRODUCT_SUB_TYPE_CODE, CCC_PL_REPORTING_REGION, CCC_TAPS_COMPANY, COUNTRY_CD_OF_RISK, COUNTRY_OF_RISK_REGION, CCC_DIVISION, CCC_BUSINESS_AREA, CCC_PRODUCT_LINE, CCC_STRATEGY, LE_GROUP FOR READ ONLY