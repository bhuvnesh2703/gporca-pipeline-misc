SELECT COB_DATE, CASE WHEN PRODUCT_TYPE_CODE IN ('DIST', 'GAS') THEN 'Distillates' WHEN PRODUCT_TYPE_CODE IN ('JET') THEN 'Jet Fuel' WHEN PRODUCT_TYPE_CODE IN ('CRUDE') THEN 'Crude' ELSE 'Others' END AS PRODUCT_TYPE, SUM(USD_CM_Delta) AS DOLLAR_DELTA, SUM(RAW_CM_Delta/1000) AS RAW_DELTA FROM CDWUSER.U_CM_MSR WHERE (CCC_DIVISION='COMMODITIES' OR (CCC_DIVISION = 'FIXED INCOME DIVISION' AND CCC_BUSINESS_AREA = 'COMMODITIES')) AND BU_RISK_RUN_CUSTOM1 = 'NOSTORAGE' AND /*CCC_TRD_BOOK <> 'DEVELOPMENT' AND*/ (CCC_BUSINESS_AREA IN ('OIL LIQUIDS', 'TMG', 'OLYMPUS') OR (CCC_PRODUCT_LINE = 'OIL & PRODUCTS' AND CCC_STRATEGY = 'EMEA OIL & PRODUCTS')) AND PRODUCT_TYPE_CODE NOT IN ('CURRENCY', 'INTEREST RATE', 'INFLATION', 'TBD', 'MISC','CVA', 'FVA','COAL','CLEAN FREIGHT','DIRTY FREIGHT') AND (CCC_BUSINESS_AREA NOT IN ('INVESTMENTS AND STR TRANS','INVESTOR BUSINESS') OR CCC_PRODUCT_LINE NOT IN ('COMMOD EXOTICS','COMMOD INDEX')) AND COB_DATE IN ('2018-02-28','2018-02-27','2018-01-31','2017-12-29','2017-09-29','2017-06-30') AND CCC_PL_REPORTING_REGION = 'EMEA' GROUP BY COB_DATE, CASE WHEN PRODUCT_TYPE_CODE IN ('DIST', 'GAS') THEN 'Distillates' WHEN PRODUCT_TYPE_CODE IN ('JET') THEN 'Jet Fuel' WHEN PRODUCT_TYPE_CODE IN ('CRUDE') THEN 'Crude' ELSE 'Others' END