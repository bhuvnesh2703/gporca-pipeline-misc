SELECT A.COB_DATE, a.BOOK, a.CURRENCY_OF_MEASURE, A.CUSIP, a.reset_index, a.direction, a.tenor, a.expiration_date, SUM (A.USD_NOTIONAL) AS USD_NOTIONAL FROM ( select A.COB_DATE, a.BOOK, a.CURRENCY_OF_MEASURE, A.CUSIP, a.reset_index, a.direction, a.tenor, a.expiration_date, A.USD_NOTIONAL as usd_notional from cdwuser.U_DM_TREASURY a WHERE COB_DATE IN ('2018-02-28') AND A.DATASET_TYPE = 'LRV' AND A.EXPIRATION_DATE > A.COB_DATE union all select A.COB_DATE, a.BOOK, a.CURRENCY_OF_MEASURE, A.TAPSCUSIP AS CUSIP, NULL AS reset_index, NULL AS direction, NULL AS TENOR, a.expiration_date, -A.USD_NOTIONAL*1000 AS USD_NOTIONAL from cdwuser.U_EXP_MSR a WHERE COB_DATE IN ('2018-02-28') AND A.CCC_DIVISION = 'TREASURY CAPITAL MARKETS' AND A.BOOK = 'PRFEQ' )A GROUP BY A.COB_DATE, a.BOOK, a.CURRENCY_OF_MEASURE, A.CUSIP, a.reset_index, a.direction, a.tenor, a.expiration_date