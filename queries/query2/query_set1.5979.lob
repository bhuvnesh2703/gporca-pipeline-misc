SELECT COB_DATE, SUM (USD_EXPOSURE) AS USD_EXPOSURE FROM cdwuser.U_CR_MSR WHERE ccc_division = 'TREASURY CAPITAL MARKETS' AND cob_date in ('2018-02-28', '2018-02-27') AND PRODUCT_HIERARCHY_LEVEL7 = 'MONEY MARKET MUTUAL FUNDS' AND CCC_PL_REPORTING_REGION = 'ASIA PACIFIC' and BOOK not in ('TSYGLRCSH') GROUP BY COB_DATE