SELECT a.COB_DATE,a.TIME_BUCKET_ANNUAL, a.CCC_PL_REPORTING_REGION, a.bu_risk_run_custom1, a.BU_RISK_SYSTEM, a.PRODUCT_TYPE_CODE, a.TIME_BUCKET_CALENDAR, a.prod_pos_name_description, sum(cast(USD_IR_RHO/10 as numeric(15,5))) as usd_ir_rho FROM cdwuser.U_IR_MSR A WHERE A.COB_DATE IN ('2018-02-28','2018-01-31') AND (CCC_DIVISION='COMMODITIES' AND CCC_BUSINESS_AREA not in ('CREDIT','MS CVA MNE - COMMOD', 'COMMODS FINANCING', 'INVESTMENTS AND STR TRANS') /* OLD LOGIC*/ OR (A.CCC_DIVISION = 'FIXED INCOME DIVISION' AND A.CCC_BUSINESS_AREA = 'COMMODITIES' and (a.CCC_STRATEGY NOT IN ('MS CVA MNE - COMMOD') and a.CCC_PRODUCT_LINE NOT IN ('COMMOD LENDING') and A.CCC_STRATEGY NOT IN ('CMD LEGACY LAONS & CLAIMS', 'CVA RISK MANAGEMENT', 'FVA RISK MANAGEMENT', 'FUNDING COLLATERAL COMM'))) /* NEW LOGIC*/ ) AND a.CCC_TRD_BOOK <> 'DEVELOPMENT' and PRODUCT_TYPE_CODE <> 'PRECIOUSMETAL' and bu_risk_run_custom1 <> 'Storage' GROUP BY a.futures_equivalent_date,a.CCC_PL_REPORTING_REGION, a.BU_RISK_SYSTEM, a.TIME_BUCKET_CALENDAR, PRODUCT_TYPE_CODE, a.prod_pos_name_description, bu_risk_run_custom1,a.TIME_BUCKET_ANNUAL, a.PRODUCT_TYPE_CODE, a.BU_RISK_SYSTEM, a.bu_risk_run_custom1, a.COB_DATE