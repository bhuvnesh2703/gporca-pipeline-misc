SELECT a.COB_DATE, CASE WHEN a.CCC_PL_REPORTING_REGION in ('JAPAN','ASIA PACIFIC') THEN 'Asia' ELSE a.CCC_PL_REPORTING_REGION END as REGION, sum(coalesce(a.USD_EQ_KAPPA_PLS100BP_TIMEADJ,0)) as TA_KAPPA FROM CDWUSER.U_DM_EQ a WHERE a.COB_DATE >= '2017-09-27' AND a.CCC_DIVISION = 'INSTITUTIONAL EQUITY DIVISION' AND a.CCC_BANKING_TRADING = 'TRADING' GROUP BY a.COB_DATE, CASE WHEN a.CCC_PL_REPORTING_REGION in ('JAPAN','ASIA PACIFIC') THEN 'Asia' ELSE a.CCC_PL_REPORTING_REGION END