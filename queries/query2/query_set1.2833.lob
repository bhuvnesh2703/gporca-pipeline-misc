SELECT a.COB_DATE, SUM (a.USD_DELTA) AS USD_DELTA, SUM (a.USD_EQ_KAPPA) AS USD_EQ_KAPPA, SUM(a.USD_EQ_KAPPA_PLS100BP_TIMEADJ) as USD_TA_KAPPA, SUM (a.SLIDE_EQ_MIN_20_USD) + SUM (a.USD_DELTA) * 0.2 AS D20 FROM CDWUSER.U_DM_EQ a WHERE a.COB_DATE >= '2017-09-27' AND a.COB_DATE <='2018-02-28' AND a.CCC_BANKING_TRADING = 'TRADING' AND a.SILO_SRC = 'IED' AND a.CCC_DIVISION = 'INSTITUTIONAL EQUITY DIVISION' GROUP BY a.COB_DATE