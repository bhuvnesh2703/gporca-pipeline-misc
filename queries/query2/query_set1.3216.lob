SELECT 'UK' as Cut, a.COB_DATE, CASE WHEN a.CCC_PL_REPORTING_REGION = 'EMEA' THEN 'EMEA' WHEN a.CCC_PL_REPORTING_REGION = 'AMERICAS' THEN 'AMERICAS' WHEN a.CCC_PL_REPORTING_REGION IN ('ASIA PACIFIC','JAPAN') THEN 'ASIA' ELSE 'OTHER' END AS CCC_PL_REPORTING_REGION, CASE WHEN a.CCC_TAPS_COMPANY = '0302' THEN 'MSIP' WHEN a.CCC_TAPS_COMPANY in ('4663','7281','5274','5254','8179','7280','6262','1311','8292','0721','4391','0517') THEN 'MSBIL' WHEN a.CCC_TAPS_COMPANY = '0319' THEN 'MSIM' ELSE 'OTHER' END as CCC_TAPS_COMPANY, CASE WHEN (a.CCC_BUSINESS_AREA IN ('CPM TRADING (MPE)', 'CPM','CREDIT', 'MS CVA MNE - FID', 'MS CVA MNE - COMMOD') OR a.CCC_STRATEGY IN ('MS CVA MPE - DERIVATIVES', 'MS CVA MNE - DERIVATIVES', 'EQ XVA HEDGING')) THEN 1 ELSE 0 END AS CVAFlag, CASE WHEN a.CCC_DIVISION = 'FID DVA' OR a.CCC_DIVISION = 'FIC DVA' OR CCC_STRATEGY = 'MS DVA STR NOTES IED' THEN 'DVA' ELSE a.CCC_BUSINESS_AREA END AS CCC_BUSINESS_AREA, CASE WHEN a.BOOK IN ('INDEX','SINGLE NAME') THEN 'Other Derivatives' WHEN a.BOOK IN ('LN ELNLT','ELNLT','MS DVA STRUCTURED NO') THEN 'DVA' WHEN a.BOOK IN ('EXOTICS - INDEX') THEN 'Exotics - INDEX' WHEN a.BOOK IN ('EXOTICS - STOCK') THEN 'Exotics - STOCK' WHEN a.BOOK IN ('EXOTICS - HYBRIDS','EXOTICS') THEN 'Exotics - Others' ELSE 'Others' END AS Group, sum(a.CORR_EQEQ)/1000000 as CORR_EQEQ FROM CDWUSER.U_DM_EQ a WHERE a.COB_DATE in ('2018-02-28','2018-02-21') AND a.IS_UK_GROUP = 'Y' AND (a.CORR_EQEQ <> 0 OR a.CORR_EQEQ IS NOT NULL) GROUP BY a.COB_DATE, a.CCC_PL_REPORTING_REGION, a.CCC_BUSINESS_AREA, a.CCC_DIVISION, a.CCC_STRATEGY, a.CCC_TAPS_COMPANY, CASE WHEN a.BOOK IN ('INDEX','SINGLE NAME') THEN 'Other Derivatives' WHEN a.BOOK IN ('LN ELNLT','ELNLT','MS DVA STRUCTURED NO') THEN 'DVA' WHEN a.BOOK IN ('EXOTICS - INDEX') THEN 'Exotics - INDEX' WHEN a.BOOK IN ('EXOTICS - STOCK') THEN 'Exotics - STOCK' WHEN a.BOOK IN ('EXOTICS - HYBRIDS','EXOTICS') THEN 'Exotics - Others' ELSE 'Others' END UNION ALL SELECT 'EMEA' as Cut, a.COB_DATE, CASE WHEN a.CCC_PL_REPORTING_REGION = 'EMEA' THEN 'EMEA' WHEN a.CCC_PL_REPORTING_REGION = 'AMERICAS' THEN 'AMERICAS' WHEN a.CCC_PL_REPORTING_REGION IN ('ASIA PACIFIC','JAPAN') THEN 'ASIA' ELSE 'OTHER' END AS CCC_PL_REPORTING_REGION, CASE WHEN a.CCC_TAPS_COMPANY = '0302' THEN 'MSIP' WHEN a.CCC_TAPS_COMPANY in ('4663','7281','5274','5254','8179','7280','6262','1311','8292','0721','4391','0517') THEN 'MSBIL' WHEN a.CCC_TAPS_COMPANY = '0319' THEN 'MSIM' ELSE 'OTHER' END as CCC_TAPS_COMPANY, CASE WHEN (a.CCC_BUSINESS_AREA IN ('CPM TRADING (MPE)', 'CPM','CREDIT', 'MS CVA MNE - FID', 'MS CVA MNE - COMMOD') OR a.CCC_STRATEGY IN ('MS CVA MPE - DERIVATIVES', 'MS CVA MNE - DERIVATIVES', 'EQ XVA HEDGING')) THEN 1 ELSE 0 END AS CVAFlag, CASE WHEN a.CCC_DIVISION = 'FID DVA' OR a.CCC_DIVISION = 'FIC DVA' OR CCC_STRATEGY = 'MS DVA STR NOTES IED' THEN 'DVA' ELSE a.CCC_BUSINESS_AREA END AS CCC_BUSINESS_AREA, CASE WHEN a.BOOK IN ('INDEX','SINGLE NAME') THEN 'Other Derivatives' WHEN a.BOOK IN ('LN ELNLT','ELNLT','MS DVA STRUCTURED NO') THEN 'DVA' WHEN a.BOOK IN ('EXOTICS - INDEX') THEN 'Exotics - INDEX' WHEN a.BOOK IN ('EXOTICS - STOCK') THEN 'Exotics - STOCK' WHEN a.BOOK IN ('EXOTICS - HYBRIDS','EXOTICS') THEN 'Exotics - Others' ELSE 'Others' END AS Group, sum(a.CORR_EQEQ)/1000000 as CORR_EQEQ FROM CDWUSER.U_DM_EQ a WHERE a.COB_DATE in ('2018-02-28','2018-02-21') AND a.CCC_PL_REPORTING_REGION = 'EMEA' AND (a.CORR_EQEQ <> 0 OR a.CORR_EQEQ IS NOT NULL) GROUP BY a.COB_DATE, a.CCC_PL_REPORTING_REGION, a.CCC_BUSINESS_AREA, a.CCC_DIVISION, a.CCC_STRATEGY, a.CCC_TAPS_COMPANY, CASE WHEN a.BOOK IN ('INDEX','SINGLE NAME') THEN 'Other Derivatives' WHEN a.BOOK IN ('LN ELNLT','ELNLT','MS DVA STRUCTURED NO') THEN 'DVA' WHEN a.BOOK IN ('EXOTICS - INDEX') THEN 'Exotics - INDEX' WHEN a.BOOK IN ('EXOTICS - STOCK') THEN 'Exotics - STOCK' WHEN a.BOOK IN ('EXOTICS - HYBRIDS','EXOTICS') THEN 'Exotics - Others' ELSE 'Others' END