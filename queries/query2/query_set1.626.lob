SELECT a.COB_DATE, a.VERTICAL_SYSTEM, a.CCC_TAPS_COMPANY, a.CCC_PRODUCT_LINE, a.CCC_RISK_MANAGER_LOGIN, a.PRODUCT_TYPE_CODE, sum(coalesce(a.USD_EXPOSURE,0)) as USD_EXPOSURE FROM cdwuser.U_EXP_MSR a WHERE a.COB_DATE >= '2018-02-20' AND a.CCC_DIVISION = 'FIXED INCOME DIVISION' AND a.CCC_PRODUCT_LINE NOT IN ('DISTRESSED TRADING') AND a.PRODUCT_TYPE_CODE IN ('BOND','BONDFUT','BONDFUTOPT','BONDIL','BONDOPT','PREF') AND a.FID1_SENIORITY IN ('AT1','SUBT1','SUBUT2') GROUP BY a.COB_DATE, a.VERTICAL_SYSTEM, a.CCC_TAPS_COMPANY, a.CCC_PRODUCT_LINE, a.CCC_RISK_MANAGER_LOGIN, a.PRODUCT_TYPE_CODE