SELECT 'EMEA' as CUT, a.COB_DATE, CASE WHEN a.CURRENCY_OF_MEASURE in ('CNH','CNY') THEN 'CNH/CNY' WHEN a.CURRENCY_OF_MEASURE ='JPY' THEN 'JPY' WHEN a.CURRENCY_OF_MEASURE ='HKD' THEN 'HKD' ELSE 'OTHER' END AS CURRENCY_OF_MEASURE, sum(coalesce(a.USD_IR_UNIFIED_PV01,0)) as USD_IR_UNIFIED_PV01 FROM CDWUSER.U_DM_EQ a WHERE a.COB_DATE >= '2016-12-01' AND a.CCC_DIVISION = 'INSTITUTIONAL EQUITY DIVISION' AND a.CCC_BANKING_TRADING = 'TRADING' AND a.CCC_PL_REPORTING_REGION = 'EMEA' AND a.USD_IR_UNIFIED_PV01 <> 0 GROUP BY a.COB_DATE, CASE WHEN a.CURRENCY_OF_MEASURE in ('CNH','CNY') THEN 'CNH/CNY' WHEN a.CURRENCY_OF_MEASURE ='JPY' THEN 'JPY' WHEN a.CURRENCY_OF_MEASURE ='HKD' THEN 'HKD' ELSE 'OTHER' END UNION ALL SELECT 'UK GROUP' as CUT, a.COB_DATE, CASE WHEN a.CURRENCY_OF_MEASURE in ('CNH','CNY') THEN 'CNH/CNY' WHEN a.CURRENCY_OF_MEASURE ='JPY' THEN 'JPY' WHEN a.CURRENCY_OF_MEASURE ='HKD' THEN 'HKD' ELSE 'OTHER' END AS CURRENCY_OF_MEASURE, sum(coalesce(a.USD_IR_UNIFIED_PV01,0)) as USD_IR_UNIFIED_PV01 FROM CDWUSER.U_DM_EQ a WHERE a.COB_DATE >= '2016-12-01' AND a.CCC_DIVISION = 'INSTITUTIONAL EQUITY DIVISION' AND a.CCC_BANKING_TRADING = 'TRADING' AND a.IS_UK_GROUP = 'Y' AND a.USD_IR_UNIFIED_PV01 <> 0 GROUP BY a.COB_DATE, CASE WHEN a.CURRENCY_OF_MEASURE in ('CNH','CNY') THEN 'CNH/CNY' WHEN a.CURRENCY_OF_MEASURE ='JPY' THEN 'JPY' WHEN a.CURRENCY_OF_MEASURE ='HKD' THEN 'HKD' ELSE 'OTHER' END UNION ALL SELECT 'GLOBAL' as CUT, a.COB_DATE, CASE WHEN a.CURRENCY_OF_MEASURE in ('CNH','CNY') THEN 'CNH/CNY' WHEN a.CURRENCY_OF_MEASURE ='JPY' THEN 'JPY' WHEN a.CURRENCY_OF_MEASURE ='HKD' THEN 'HKD' ELSE 'OTHER' END AS CURRENCY_OF_MEASURE, sum(coalesce(a.USD_IR_UNIFIED_PV01,0)) as USD_IR_UNIFIED_PV01 FROM CDWUSER.U_DM_EQ a WHERE a.COB_DATE >= '2016-12-01' AND a.CCC_DIVISION = 'INSTITUTIONAL EQUITY DIVISION' AND a.CCC_BANKING_TRADING = 'TRADING' AND a.USD_IR_UNIFIED_PV01 <> 0 GROUP BY a.COB_DATE, CASE WHEN a.CURRENCY_OF_MEASURE in ('CNH','CNY') THEN 'CNH/CNY' WHEN a.CURRENCY_OF_MEASURE ='JPY' THEN 'JPY' WHEN a.CURRENCY_OF_MEASURE ='HKD' THEN 'HKD' ELSE 'OTHER' END