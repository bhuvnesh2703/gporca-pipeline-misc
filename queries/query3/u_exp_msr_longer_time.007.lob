SELECT a.COB_DATE, a.CCC_PRODUCT_LINE, a.CCC_STRATEGY, a.BOOK, a.PRODUCT_TYPE_CODE, a.CCAR_INDEX_CLASS, sum(coalesce(a.USD_PV01SPRD_COMPOSITE,0)) as USD_PV01SPRD_COMPOSITE, sum(coalesce(a.USD_PV10_BENCH_COMP,0)) as USD_PV10_BENCH_COMP, sum(coalesce(a.USD_CREDIT_GAMMA,0))/10000 as USD_CR_GAMMA, sum(coalesce(a.USD_CR_KAPPA,0)) as USD_CR_KAPPA, sum(coalesce(a.SLIDE_PVSPRCOMP_MIN_50PCT_USD,0)) as SLIDE_PVSPRCOMP_MIN_50PCT_USD, sum(coalesce(a.SLIDE_PVSPRCOMP_MIN_19PCT_USD,0)) as SLIDE_PVSPRCOMP_MIN_19PCT_USD, sum(coalesce(a.SLIDE_PVSPRCOMP_MIN_10PCT_USD,0)) as SLIDE_PVSPRCOMP_MIN_10PCT_USD, sum(coalesce(a.SLIDE_PVSPRCOMP_PLS_10PCT_USD,0)) as SLIDE_PVSPRCOMP_PLS_10PCT_USD, sum(coalesce(a.SLIDE_PVSPRCOMP_PLS_21PCT_USD,0)) as SLIDE_PVSPRCOMP_PLS_21PCT_USD, sum(coalesce(a.SLIDE_PVSPRCOMP_PLS_50PCT_USD,0)) as SLIDE_PVSPRCOMP_PLS_50PCT_USD, sum(coalesce(a.SLIDE_PVSPRCOMP_PLS_100PCT_USD,0)) as SLIDE_PVSPRCOMP_PLS_100PCT_USD, sum(coalesce(a.SLIDE_PVSPRCOMP_PLS_200PCT_USD,0)) as SLIDE_PVSPRCOMP_PLS_200PCT_USD, sum(coalesce(a.SLIDE_PVSPRCOMP_PLS_300PCT_USD,0)) as SLIDE_PVSPRCOMP_PLS_300PCT_USD, sum(coalesce(a.SLIDE_PVSPRCOMP_PLS_400PCT_USD,0)) as SLIDE_PVSPRCOMP_PLS_400PCT_USD, sum(coalesce(a.SLIDE_PVSPRCOMP_PLS_500PCT_USD,0)) as SLIDE_PVSPRCOMP_PLS_500PCT_USD, sum(coalesce(a.SLIDE_PVSPRCOMP_PLS_600PCT_USD,0)) as SLIDE_PVSPRCOMP_PLS_600PCT_USD, sum(coalesce(a.SLIDE_PVSPRCOMP_PLS_700PCT_USD,0)) as SLIDE_PVSPRCOMP_PLS_700PCT_USD, sum(coalesce(a.SLIDE_PVSPRCOMP_PLS_800PCT_USD,0)) as SLIDE_PVSPRCOMP_PLS_800PCT_USD FROM CDWUSER.U_EXP_MSR a WHERE a.COB_DATE >= '2017-08-11' AND a.CCC_PL_REPORTING_REGION = 'EMEA' AND a.CCC_BUSINESS_AREA = 'DSP - CREDIT' AND a.CCC_PRODUCT_LINE = 'DSP INDEX PRODUCTS' AND ((a.CCC_STRATEGY IN ('EU INDEX OPTIONS') and a.book not in ('ABNMG')) or a.book in ('ABCHN')) GROUP BY a.COB_DATE, a.CCC_PRODUCT_LINE, a.CCC_STRATEGY, a.BOOK, a.PRODUCT_TYPE_CODE, a.CCAR_INDEX_CLASS