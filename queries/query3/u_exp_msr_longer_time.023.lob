SELECT a.COB_DATE, 'MSCS' AS LE, SUM (USD_DELTA) AS EQ_DELTA, SUM (USD_EQ_KAPPA) AS EQ_KAPPA FROM CDWUSER.U_EXP_MSR A WHERE A.cob_date >= '12/29/2017' and a.cob_date <= '02/28/2018' AND A.CCC_TAPS_COMPANY IN ('0111') AND A.VAR_EXCL_FL <> 'Y' AND A.CCC_DIVISION = 'INSTITUTIONAL EQUITY DIVISION' AND A.CCC_BANKING_TRADING_FEED = 'TRADING' GROUP BY A.COB_DATE