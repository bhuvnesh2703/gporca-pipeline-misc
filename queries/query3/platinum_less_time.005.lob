select case when a.CATEGORY = 'RMBS' then 'RESI' when a.CATEGORY in ('ABS', 'CLO') then 'ABS & CLO' when a.CATEGORY in ('CMBS', 'CRE Loan - FV') then 'CMBS' when a.CATEGORY = 'Corporate Hedge' then 'Macro Hedges' else 'Other' end as SPG_MUFG_Category, sum(cast (A.net_exposure as numeric(15,5))) as Net_Exposure from ( select a.COB_DATE, case when spg_desc in ('WAREHOUSE CLO') then 'Warehouse - FV' when spg_desc like 'WAREHOUSE%' then 'Warehouse - HFI' when SPG_DESC ='OTHER RMBS' or SPG_DESC like '%AGENCY%' or spg_desc like '%POOL%' or spg_desc like '%TBA%' or spg_desc like '%RMBS IOS%' or spg_desc like '%RMBS MBX%' or spg_desc like '%RMBS MBS%' then 'Agency' when spg_desc like '%CMBS%' and spg_desc not in ('CMBS LOAN', 'CMBS LOAN IO') then 'CMBS' when spg_desc like '%RMBS%' and spg_desc not like '%AGENCY%' and CCC_PRODUCT_LINE not in ('WAREHOUSE', 'AGENCY MORTGAGE TRADING') then 'RMBS' when ccc_product_line in ('CRE LEND SECURITIZATION', 'CRE LEND - BANK HFI/HFS', 'CRE LENDING','CREL BANK HFI') and book like '%HFI' and spg_desc in ('CMBS LOAN', 'CMBS LOAN IO') then 'CRE Loan - HFI' when ccc_product_line in ('CRE LEND SECURITIZATION', 'CRE LEND - BANK HFI/HFS', 'CRE LENDING','CRE LENDING SEC/HFS') and book in ('CRE_LENDING_EU', 'CRE_LENDING') and spg_desc in ('CMBS LOAN', 'CMBS LOAN IO') then 'CRE Loan - FV' when ccc_product_line in ('CRE LEND SECURITIZATION', 'CRE LEND - BANK HFI/HFS', 'CRE LENDING','CRE LENDING SEC/HFS') and book in ('CRE_LENDING_EU_HFS', 'CRE_LENDING_HFS') and spg_desc in ('CMBS LOAN', 'CMBS LOAN IO') then 'CRE Loan - HFS' when spg_desc like 'ABS%' then 'ABS' when spg_desc like '%CORPORATE CDO%' or spg_desc like '%CORPORATE CLO%' then 'CLO' when spg_desc like '%CORPORATE INDEX%' or spg_desc like '%CORPORATE DEFAULT SWAP%' then 'Corporate Hedge' when spg_desc like '%GOVERNMENT%' or spg_desc like '%RATE%' or spg_desc in ('SWAP') then 'Rate Hedge' Else 'OTHER' end as CATEGORY, sum(cast (A.USD_NET_EXPOSURE as numeric(15,5))) as net_exposure from cdwuser.U_DM_SPG a where a.COB_DATE = '2018-02-28' AND a.CCC_BUSINESS_AREA = 'SECURITIZED PRODUCTS GRP' group by a.COB_DATE, case when spg_desc in ('WAREHOUSE CLO') then 'Warehouse - FV' when spg_desc like 'WAREHOUSE%' then 'Warehouse - HFI' when SPG_DESC ='OTHER RMBS' or SPG_DESC like '%AGENCY%' or spg_desc like '%POOL%' or spg_desc like '%TBA%' or spg_desc like '%RMBS IOS%' or spg_desc like '%RMBS MBX%' or spg_desc like '%RMBS MBS%' then 'Agency' when spg_desc like '%CMBS%' and spg_desc not in ('CMBS LOAN', 'CMBS LOAN IO') then 'CMBS' when spg_desc like '%RMBS%' and spg_desc not like '%AGENCY%' and CCC_PRODUCT_LINE not in ('WAREHOUSE', 'AGENCY MORTGAGE TRADING') then 'RMBS' when ccc_product_line in ('CRE LEND SECURITIZATION', 'CRE LEND - BANK HFI/HFS', 'CRE LENDING','CREL BANK HFI') and book like '%HFI' and spg_desc in ('CMBS LOAN', 'CMBS LOAN IO') then 'CRE Loan - HFI' when ccc_product_line in ('CRE LEND SECURITIZATION', 'CRE LEND - BANK HFI/HFS', 'CRE LENDING','CRE LENDING SEC/HFS') and book in ('CRE_LENDING_EU', 'CRE_LENDING') and spg_desc in ('CMBS LOAN', 'CMBS LOAN IO') then 'CRE Loan - FV' when ccc_product_line in ('CRE LEND SECURITIZATION', 'CRE LEND - BANK HFI/HFS', 'CRE LENDING','CRE LENDING SEC/HFS') and book in ('CRE_LENDING_EU_HFS', 'CRE_LENDING_HFS') and spg_desc in ('CMBS LOAN', 'CMBS LOAN IO') then 'CRE Loan - HFS' when spg_desc like 'ABS%' then 'ABS' when spg_desc like '%CORPORATE CDO%' or spg_desc like '%CORPORATE CLO%' then 'CLO' when spg_desc like '%CORPORATE INDEX%' or spg_desc like '%CORPORATE DEFAULT SWAP%' then 'Corporate Hedge' when spg_desc like '%GOVERNMENT%' or spg_desc like '%RATE%' or spg_desc in ('SWAP') then 'Rate Hedge' Else 'OTHER' end ) A group by case when a.CATEGORY = 'RMBS' then 'RESI' when a.CATEGORY in ('ABS', 'CLO') then 'ABS & CLO' when a.CATEGORY in ('CMBS', 'CRE Loan - FV') then 'CMBS' when a.CATEGORY = 'Corporate Hedge' then 'Macro Hedges' else 'Other' end