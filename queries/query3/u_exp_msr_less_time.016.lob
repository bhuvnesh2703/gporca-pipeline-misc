SELECT     a.COB_DATE,     CASE          WHEN FEED_SOURCE_NAME = 'CORISK' THEN SUBSTRING (PROD_POS_NAME_DESCRIPTION,1,3)     ELSE a.CURRENCY_OF_MEASURE END AS CURRENCY,     SUM (a.USD_IR_UNIFIED_PV01) AS USD_PV01,     SUM (a.USD_FX) AS FX_DELTA FROM cdwuser.U_EXP_MSR a WHERE (a.COB_DATE = '2018-02-28' or a.COB_DATE = '2018-02-21') and CCC_PL_REPORTING_REGION IN ('JAPAN')     AND (a.CCC_BUSINESS_AREA IN ('CPM TRADING (MPE)', 'CPM', 'CREDIT', 'MS CVA MNE - FID', 'MS CVA MNE - COMMOD') OR      a.CCC_STRATEGY IN ('MS CVA MPE - DERIVATIVES', 'MS CVA MNE - DERIVATIVES')) AND     a.ccc_product_line NOT IN ('CREDIT LOAN PORTFOLIO', 'CMD STRUCTURED FINANCE') AND     A.CURVE_TYPE NOT IN ('CPCR_CLEAR') AND     (a.USD_FX IS NOT NULL OR      a.USD_IR_UNIFIED_PV01 IS NOT NULL) GROUP BY     a.COB_DATE,     CASE          WHEN FEED_SOURCE_NAME = 'CORISK' THEN SUBSTRING (PROD_POS_NAME_DESCRIPTION,1,3)     ELSE a.CURRENCY_OF_MEASURE END