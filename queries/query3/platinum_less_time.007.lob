SELECT a.COB_DATE, SUM (CASE WHEN a.PRODUCT_TYPE_CODE IN ('ADR', 'STOCK', 'SWAP', 'WARRNT', 'ETF', 'OPTION') THEN a.USD_DELTA ELSE a.USD_NET_EXPOSURE END) AS NET_EXPOSURE FROM CDWUSER.U_DM_CC A where a.COB_DATE in ('2018-02-28') AND a.CCC_PRODUCT_LINE = 'DISTRESSED TRADING' AND PRODUCT_TYPE_CODE NOT IN ('GVTBOND', 'ETF', 'OPTION', 'CRDINDEX', 'CDSOPTIDX', 'FUTURE', 'BOND ETF') GROUP BY a.COB_DATE