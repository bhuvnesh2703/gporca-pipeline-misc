SELECT 'EM CREDIT' AS TYPE, SUM (cast (A.USD_PV10_BENCH as numeric(15,5))) AS USD_PV10_BENCH FROM CDWUSER.U_DM_FX A WHERE a.COB_DATE IN ('2018-02-28') and (CCC_BUSINESS_AREA IN ('EM CREDIT TRADING')) AND VAR_EXCL_FL = 'N' AND VERTICAL_SYSTEM NOT IN ('PIPELINE_NY') AND CCC_PRODUCT_LINE NOT IN ('DERIVATIVE PRODUCTS') and not ISSUER_COUNTRY_CODE IN ('VEN')